int Position;   
function GetShapes(f_var) {
    if(!SpiralPosition1)
	{
		InnerSpiral -= 1;
		if(InnerSpiral < TaylorRadiusMin)
			SpiralPosition1 = TRUE;
	}
	else
	{
	    InnerSpiral += 1;
		if(InnerSpiral > f_var)
			SpiralPosition1 = FALSE;
	}
	if(!SpiralPosition2)
	{
		OuterSpiral -= 1;
		if(OuterSpiral < TaylorRadiusMin)
			SpiralPosition2 = TRUE;
	}
	else
	{
		OuterSpiral += 1;
		if(OuterSpiral >  f_var)
			SpiralPosition2 = FALSE;
	} 
	if(TaylorShape == 1)                    
	{ 
        RY = (get_val(AIM_Y))  + ( (SinAngle * f_var) )/100;
        RX = (get_val(AIM_X))  + ( (CosAngle * f_var) )/100;  
	} 
	if(TaylorShape == 2)        
	{ 
        RY = (get_val(AIM_Y))  + ( (SinAngle * f_var) )/100;
        RX = (get_val(AIM_X))  + ( (CosAngle * TaylorRadiusMin) )/100; 
	}
	if(TaylorShape == 3)                     
	{ 
        RX = (get_val(AIM_X))  + ( (CosAngle * f_var) )/100;  
        RY = (get_val(AIM_Y))  + ( (SinAngle * TaylorRadiusMin) )/100;
 
	} 
	if(TaylorShape == 4)                  
	{
        RY = (get_val(AIM_Y))  + ( (SinAngle * InnerSpiral) )/100;
        RX = (get_val(AIM_X))  + ( (CosAngle * OuterSpiral) )/100;  
	}
    if(TaylorShape == 5)
	{
		if(Position == 0 || Position == 1)
    	{  
		    RX = (get_val(AIM_X))  + ( (CosAngle * f_var) )/100;
		}
	    if(Position == 2 || Position == 3)
		{  
		    RY = (get_val(AIM_Y))  + ( (SinAngle * f_var) )/100;
		}
			Position++;		
 
		if(Position == 4)
		{
			Position = 0;
		}
    } 
    set_val(AIM_X, RX);
    set_val(AIM_Y, RY);
}
