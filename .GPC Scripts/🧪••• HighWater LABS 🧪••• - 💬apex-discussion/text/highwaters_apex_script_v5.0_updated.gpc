int in_hand, to_set, weapon[1], barrel[1], sight[1],weapon_list_start[3],weapon_list_end[3];
function select() {
//  weapon select
	// set sight							HOLD ADS and PRESS RIGHT
	if(event_press(PS4_DOWN))
	{
		sight[to_set] += 1;
		if(sight[to_set] > 2 || (GUN_INFO[to_set][2] != 0 && GUN_INFO[to_set][2] <= 2 && sight[to_set] > 1) || GUN_INFO[to_set][2] == 4)
			sight[to_set] = 0;
		display_select = 2;
		color = sight[to_set] + 4;
		//combo_run(A_RUMBLE);
	}
	if(get_ival(PS4_DOWN))
		set_val(PS4_DOWN, 0);  
// set barrel							HOLD ADS and PRESS DOWN
	if(event_press(PS4_RIGHT))
	{
		barrel[to_set] += 1;
		if(barrel[to_set] > 3 || GUN_INFO[to_set][2] > 1)
			barrel[to_set] = 0;
		display_select = 1;
		color = barrel[to_set] + 3;
		//combo_run(A_RUMBLE);
	}
	if(get_ival(PS4_RIGHT))
		set_val(PS4_RIGHT, 0); 
// select favorite
	if(get_ptime(PS4_UP) > 300 && get_val(PS4_UP))
	{
		weapon[to_set] = quick_gun_primary[0];
		color = 1;
		display_select = 10;
		//combo_run(P_RUMBLE);
	}
	if(get_ival(PS4_UP))
		set_val(PS4_UP,0);
// set AR                    -                    HOLD ADS and PRESS D-pad UP REPEAT to cycle through ARs
	if(event_press(PS4_UP))
	{	
		weapon[to_set] += 1;		
		if(weapon[to_set] <=  ar_list_start || weapon[to_set] > ar_list_end)
			weapon[to_set] = ar_list_start;
		if(weapon[to_set] >= ar_list_end)
			weapon[to_set] = ar_list_start;
		color = 1;
		display_select = 10;
		//combo_run(P_RUMBLE);
	}
	if(get_ival(PS4_UP))
		set_val(PS4_UP, 0);
	// reset 										HOLD ADS and HOLD LEFT
	if(get_ptime(PS4_LEFT) > 300 && get_val(PS4_LEFT))
	{
		sight[to_set] = 0;
		barrel[to_set] = 0;
		weapon[to_set] = 0;
		color = 2;
		display_title = TRUE;
		//combo_run(RUMBLE_OFF);
	}
	if(get_ival(PS4_LEFT))
		set_val(PS4_LEFT, 0);
// set other                  -                   HOLD ADS and PRESS D-pad LEFT
	if(event_press(PS4_LEFT))
	{
		weapon[to_set] += 1;		
		if(weapon[to_set] <= other_list_start || weapon[to_set] > other_list_end)
			weapon[to_set] = other_list_start;
		if(weapon[to_set] >= other_list_end)
			weapon[to_set] = other_list_start;
		color = 1;
		display_select = 10;
		//combo_run(P_RUMBLE);
	}
	if(get_ival(PS4_LEFT))
		set_val(PS4_LEFT, 0);
// default carepack weapon sights	
	if(weapon[to_set] == 7 || weapon[to_set] == 16)
		sight[to_set] = 1;

}
