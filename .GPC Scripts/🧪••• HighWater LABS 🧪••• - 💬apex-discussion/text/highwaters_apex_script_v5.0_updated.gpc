function led_color() {
	if(color==2  || weapon[in_hand] == 0)
		set_rgb(255,0,0);
	if(color==1 && weapon[in_hand] != 0)
		set_rgb(0,153,153);
	if(color==0 && weapon[in_hand] != 0) 
		set_rgb(51,51,0);   
	if(color==3)
		set_rgb(0,0,0);
	if(color==4)
		set_rgb(32,32,32);
	if(color==5)
		set_rgb(0,25,51);
	if(color==6)
