			if(modIdx  < 0) modIdx = maxMods;
			
			display = ON;
		}
		
		if(event_press(PS4_DOWN))
		{
			modIdx++;
			
			if(modIdx  > maxMods) modIdx = 0;
			
			display = ON;
		}
		
		if(event_press(PS4_CIRCLE))
		{
			combo_run(RUMBLE_ALERT);
			
