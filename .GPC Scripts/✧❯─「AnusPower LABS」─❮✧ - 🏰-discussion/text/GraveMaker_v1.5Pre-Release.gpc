init
{
	
	if(get_pvar(SPVAR_64, 0, 9999, 0) != 1486)
	{
		set_pvar(SPVAR_64, 1486):
		reset_script_var();
		save_pvar();
	}
	
	load_pvar();
	
	if(INVERTED)
	{
		inverted = -1;
	}
	else
	{
		inverted = 1;
	}
	
	reset_leds():
	
	combo_run(bootScreen);
}

