function DoGzTimeAA() {
	AAT += get_rtime();

	if (!AxisT)
		Set_Val(AIM_Y, AAT * Strength / 10);

	if (AxisT == 1)  
		Set_Val(AIM_X, AAT * Strength / 10);

	if (AxisT == 2)  
		Set_Val(AIM_Y, inv(AAT * Strength / 10));

	if (AxisT == 3)
	{
		Set_Val(AIM_X, inv(AAT * Strength / 10));

		if (AAT > 50)
		{
			AAT		= 0;
			AxisT	= 0;
		}
	}
	else if (AAT > 50)
	{
		AAT		 = 0;
		AxisT	+= 1;
	}
}

function AimAssistEcho() {
	TimerEcho += get_rtime();

	if (TimerEcho < StepTime)
	{
		EchoVal = EchoValue;
	}
	else if (TimerEcho < (StepTime * 2))
	{
		EchoVal = EchoValue + Increase;
	}
	else if  (TimerEcho < (StepTime * 3))
	{
		EchoVal = EchoValue + (Increase * 2);
	}
	else if  (TimerEcho < (StepTime * 4))
	{
		EchoVal = EchoValue + (Increase * 3);
	}
	else if  (TimerEcho < (StepTime * 5))
	{
		EchoVal = EchoValue + (Increase * 4);
	}

	if  (TimerEcho > StepTime * 6)
	{
		TimerEcho = 0;
	}
		combo_run(cEchoAssist);
}

