rest_time = hold_time - 20;
if(rest_time < 0) rest_time = 0;}

combo vibrate{ 
wait(300);
reset_rumble();
}

    
combo RAPID_FIRE {
set_val(PS4_R2, 100);
wait(hold_time);
set_val(PS4_R2, 0);
wait(rest_time);
