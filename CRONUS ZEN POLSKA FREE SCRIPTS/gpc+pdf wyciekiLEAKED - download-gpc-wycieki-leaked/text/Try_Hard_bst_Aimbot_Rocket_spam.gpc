/*--  GTA ONLINE MY FIRST CRACK AT SCRIPTING EVER SO DONT BE TOO HARSH ----
- ROCKET SPAM SNIPER SPAM AUTO RUN TURBO ROLL ADS AND SHOOT SOONER AIMBOT BST -
===============================================================================

BY X-xBUG2EE-Bx-X

Edited by ! Frankie#7907


 
 /*------------------------------------------------------ SNIPER ON/OFF ---------L2 + CIRCLE-------R2 TO FIRE
 -------------------------------------------------------- RPG ON/OFF ------------L2 + SQUARE-------R2 TO FIRE
 -------------------------------------------------------- AUTORUN ON/OFF---------L2 + R1---------RIGHT JOYSTICK UP AND DOWN TO RUN
 -------------------------------------------------------- TURBO ROLL ON/OFF------L2 + UP---------X TO ROLL
 -------------------------------------------------------- SHOOT SOONER ON/OFF----SHARE---------------------------L2
 -------------------------------------------------------- BST---------------------------------------L2 + DOWN TO DROP
 /*--------------------------------------------------------------------------------------------------------------------------------------*/
   define LYA = PS4_LY;
   
   int AIMBOT = TRUE;
   int BSTQuickDrop = TRUE;
   int Turbo_ROLL = TRUE;
   int EASY_SPRINT = TRUE;
   int SHOOT_SOONER = TRUE;
   int ADS_FAST = TRUE;
   int RPG = FALSE;
   int SNIPER = FALSE;
   int AutoRunOn = FALSE;
/*------------------------------------------------*/
main {
    if(get_val(PS4_L2) >= 1)
        combo_run(ADS_FAST);
    else if(combo_running(ADS_FAST))
        combo_stop(ADS_FAST);
        
    if(get_val(PS4_SHARE))
        SHOOT_SOONER = !SHOOT_SOONER;
    if(get_val(PS4_L2)>=1 && SHOOT_SOONER)
        combo_run(SHOOT_SOONER)
       
    if(get_val(PS4_L2) && event_press(PS4_R1))
        AutoRunOn = !AutoRunOn;
    if(get_val(LYA)<0 && AutoRunOn)
        combo_run(EASY_SPRINT)
    else if(combo_running(EASY_SPRINT))
        combo_stop(EASY_SPRINT);
        
    if(get_val(PS4_L2) && event_press(PS4_UP))
        Turbo_ROLL = !Turbo_ROLL
    if(get_val(PS4_R2)>=1 && Turbo_ROLL)
        combo_run(Turbo_ROLL);
       
    if(get_val(PS4_L2) && event_press(PS4_DOWN))
    	combo_run(BSTQuickDrop)

    if(get_val(PS4_L2) && event_press(PS4_SQUARE))
        RPG = !RPG
    if(get_val(PS4_R2)>=1 && RPG)
        combo_run(RPG)
        
    if(get_val(PS4_L2) && event_press(PS4_CIRCLE))
        SNIPER = !SNIPER;
    if(event_press(PS4_R2)>=1 && SNIPER)
        combo_run(SNIPER);
   
    if(event_press(PS4_L2))
        combo_run(AIMBOT);
    if(event_release(PS4_L2))
        combo_stop(AIMBOT);
}

combo ADS_FAST {
    set_val(PS4_L2, 100);
    wait(3000);}
combo SHOOT_SOONER {
    set_val(PS4_R2, 100);
    wait(3000);}
combo EASY_SPRINT {
    set_val(PS4_R1, 100);
    wait(10);
    set_val(PS4_R1, 0)}
/*---------------------------------------*/
/*-----------------TURBO ROLL----------------------*/
combo Turbo_ROLL {
    set_val(PS4_CROSS, 100);
    wait(20);
    set_val(PS4_CROSS, 0);
    wait(10);
    set_val(PS4_CROSS, 0);
}
combo BSTQuickDrop {
set_val(PS4_TOUCH, 100);
wait(600);
wait(100);
set_val(PS4_CROSS, 100);
wait(50);
