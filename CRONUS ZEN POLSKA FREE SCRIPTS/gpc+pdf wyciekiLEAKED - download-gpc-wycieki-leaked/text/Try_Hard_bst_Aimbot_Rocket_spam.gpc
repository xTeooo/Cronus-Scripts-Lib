wait(40);
set_val(PS4_DOWN, 0);
wait(40);
set_val(PS4_CROSS, 40);
wait(40);
set_val(PS4_CROSS, 0);
}
combo RPG 

{   
    ///Wait for shot
    wait(210)
    
    //switch to grenade
    set_val(PS4_L1, 100)
    wait(180)
    set_val(PS4_L1, 100)
    set_val(PS4_RX, -85)
    set_val(PS4_RY, -85)
    wait(45)
    set_val(PS4_RX, 0)
    set_val(PS4_RY, 0)
    set_val(PS4_L1, 0)
    wait(50)
    
    //switch back to RPG
    set_val(PS4_L1, 100)
    wait(180)
    set_val(PS4_L1, 100)
    set_val(PS4_RX, -100)
    wait(45)
    set_val(PS4_L1, 0)
    wait(100)
    
}
