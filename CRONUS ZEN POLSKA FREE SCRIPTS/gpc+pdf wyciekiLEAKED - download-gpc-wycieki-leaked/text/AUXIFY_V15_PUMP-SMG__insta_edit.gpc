    set_val(RIGHT_BUMP,100);     
    wait(320);     
    set_val(FIRE_BTN,100);    
    wait(200);
    set_val(LEFT_BUMP,100)
    wait(320)
}
 
 
combo GhostPeek {
set_val(CROUCH_BTN, 100);
wait(15);
