function Pack16To32(int var1, int var2){
	if (var1 < 0) {
		var1 = (abs(var1) & 0x7FFF) | 0x8000;
			}
	if (var2 < 0) {
		var2 = (abs(var2) & 0x7FFF) | 0x8000;
			}
	return (var1 << 16) | (var2 & 0xFFFF);
	}
function Unpack32To16(int pvar, int var){
	if(var == 0) {
		pvar = pvar >> 16;
			}
	if (pvar & 0x8000) {
		return 0 - (pvar & 0x7FFF);
			}
	return pvar & 0xffff;
	}
