function LoadARInfo(){	
	//Default data from script
	//Primary Gun
	gunARInfo[0] = GunARIndex[GetGunARIndex()];
	gunARInfo[1] = GunARIndex[GetGunARIndex() + 1];
	//Seconday Gun
	gunARInfo[2] = GunARIndex[GetGunARIndex() + 6];
	gunARInfo[3] = GunARIndex[GetGunARIndex() + 7];
	
	operatorWithFrags = GunARIndex[GetGunARIndex() + 10];
	
	//Get var used for this operator
	saveVar = (32768 - activeOperator) * -1;
	tempPolar = get_pvar(saveVar, -2147483648, 2147483647, 0);
	
	if(tempPolar != 0){
		saveValues = Unpack32To16(tempPolar, 0);
		gunARInfo[0]    = abs((saveValues / 100) % 100);  		
	    gunARInfo[1]    = (saveValues % 100);
	    gunIndex[GUN_PRIMARY] = abs(saveValues / 10000);
	    set_val(TRACE_1, gunARInfo[0]);
		set_val(TRACE_2, gunARInfo[1]);
		set_val(TRACE_3, gunIndex[GUN_PRIMARY]);
	    saveValues = Unpack32To16(tempPolar, 1);
		gunARInfo[2]    = abs((saveValues / 100) % 100);  		
	    gunARInfo[3]    = (saveValues % 100);
	    gunIndex[GUN_SECONDARY] = abs(saveValues / 10000);
	    set_val(TRACE_2, saveValues);
	}
}
function SaveARInfo(){
		
		//old trick to concat int's
		//Primary Gun
		tempPolar = (gunIndex[0]) *  pow(10, 2) + gunARInfo[0];		
		primaryValues = tempPolar * pow(10, 2)  + abs(gunARInfo[1]);
		if(gunARInfo[1] < 0) primaryValues = (primaryValues) * -1;
		//Secondary Gun	
		tempPolar = (gunIndex[1]) *  pow(10, 2) + gunARInfo[2];
		secondaryValue = tempPolar * pow(10, 2) + abs(gunARInfo[3]);
		if(gunARInfo[3] < 0) secondaryValue = (secondaryValue) * -1;
		
		tempPolar =  Pack16To32(primaryValues, secondaryValue);
		
		saveVar = (32768 - activeOperator) * -1;
		set_pvar(saveVar, tempPolar);
		set_val(TRACE_1, primaryValues);
		set_val(TRACE_2, secondaryValue);
		set_val(TRACE_3, tempPolar);
	
		combo_run(SaveNofication);
}
