//AR functions 
int tempv, ar_y, ar_x;
function y_val() { 

    tempv = get_val(XB1_RY);
    
    if(abs(tempv) < 10)
        tempv = 0;
        
    if(abs(tempv) > ar_y + 5)
        return tempv;
        
    return tempv + ar_y;           
}
function x_val() {
	tempv = get_val(XB1_RX);
	if(abs(tempv) < 10)
		tempv = 0;
	if(abs(tempv) > abs(ar_x + 5))
		return tempv;
	return tempv + ar_x;   	    
}
combo AutoScan{
	Set(SwapButton, 100);
	wait(1280);
	Unset(SwapButton);
	wait(100);
}
combo DoNoficationDelay{
	cls_oled(0);	
   	print(CenterText(toggleMessage[modIndex + 1] - toggleMessage[modIndex] - 1, OLED_FONT_MEDIUM_WIDTH), LINE_CENTER - 10, OLED_FONT_MEDIUM, OLED_WHITE, toggleMessage[modIndex]);
   	print(CenterText(toggleStatus[modVal + 1] - toggleStatus[modVal] - 1, OLED_FONT_MEDIUM_WIDTH), LINE_CENTER + 10, OLED_FONT_MEDIUM, OLED_WHITE, toggleStatus[modVal]);
	wait(1000);
	set_pvar(SPVAR_63, ModsMask);
   	
   	DisplaySwitchGun();
	if(activeGun == GUN_PRIMARY){
		LED(GREEN);
	}else{
		LED(BLUE);
	}
	block_all_inputs();
}
combo SaveNofication{
	cls_oled(0);						
	print(CenterText(sizeof(saveMessage) - 1, OLED_FONT_MEDIUM_WIDTH), LINE_CENTER , OLED_FONT_MEDIUM, OLED_WHITE, saveMessage[0]);
	wait(500);   	
   	DisplaySwitchGun();
	if(activeGun == GUN_PRIMARY){
		LED(GREEN);
	}else{
		LED(BLUE);
	}
	block_all_inputs();
}
combo PinOnShot{
	Set(UpButton, 100);
	wait(40);
	Unset(UpButton);
	wait(40);
}
combo RapidFire {
    set_val(FireButton, 100);
    wait(560 / presses_per_sec);
    set_val(FireButton, 0);
    wait(560 / presses_per_sec - get_rtime());
    set_val(FireButton, 0);
}
combo ResetNotify{
	cls_oled(0);	
