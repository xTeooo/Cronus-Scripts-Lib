main{        
 
if (get_val(4))  {

combo_run(DERP);
combo_run(antirecoil);
}
if (get_val(7))  {
combo_stop(DERP);
combo_stop(AUTORELOAD);
combo_stop(antirecoil);
combo_stop(RELOAD);
}

if(event_release(4)) {
combo_run(AUTORELOAD); 
combo_run(RELOAD);
}
if(get_val(10 && 11)>10) {
combo_stop(antirecoil);
}

}

combo antirecoil{
wait(20);
set_val(10, 60);
set_val(9, -20);
}


combo  RELOAD {
wait(1200);
set_val(20, 100);
wait(100);
}



combo DERP {


set_val(20, 100);
wait(20);
set_val(6, 100);
set_val(17, 100);
wait(10);
set_val(4, 0);
set_val(4, 100);
}

combo AUTORELOAD {
wait(350);
set_val(17, 100);
wait(10);
set_val(10, -50);
wait(100);
set_val(17, 100);
}

