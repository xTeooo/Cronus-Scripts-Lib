	Set_RGB(0, 0, 0);
	wait(100);
	Set_RGB(0, 255, 0);
	wait(100);
	Set_RGB(0, 0, 0);
	wait(100);
	Set_RGB(0, 255, 0);
	wait(2000);
	Set_RGB(0, 0, 0);
}

