combo cStrafe {
	if(_Strafe_Index == 0)
	{
		_Strafe_Rand_1 = 110;
		_Strafe_Rand_2 = 210;
	}
		else
	{
		_Strafe_Rand_1 = 210;
		_Strafe_Rand_2 = 110;
	}

	Set_Val(STRAFE, 100);

	_Strafe = random((StrafeDelay - _Strafe_Rand_1), (StrafeDelay + _Strafe_Rand_1));

	wait(_Strafe); 
	wait(54); 
	Set_Val(STRAFE, inv(100));

	_Strafe = random((StrafeDelay - _Strafe_Rand_2), (StrafeDelay + _Strafe_Rand_2));

	wait(_Strafe); 
	wait(54);

		_Strafe_Index++;

	if(_Strafe_Index == 2)
		_Strafe_Index = 0;
}

combo NikO {
	wait(20);
	image_oled(0, 10, Color, FALSE, Frames[Frame]);
	Frame ++;
}
