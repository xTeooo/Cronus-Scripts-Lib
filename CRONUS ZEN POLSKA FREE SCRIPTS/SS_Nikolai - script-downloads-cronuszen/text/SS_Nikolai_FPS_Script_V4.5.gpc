combo RGB {
	wait(10 - get_rtime());

	Set_RGB(Colour, Colour[1], Colour[2]);

	Colour[Dec]--; 
	Colour[inc]++;

	if(!Colour[Dec])
	{
		inc = (inc + 1) % 3; 
		Dec = (Dec + 1) % 3; 
	}
}

combo cVibrate {
	set_rumble(RUMBLE_A, 90);
	wait(230);
	reset_rumble();
}

combo FlashGreen {
