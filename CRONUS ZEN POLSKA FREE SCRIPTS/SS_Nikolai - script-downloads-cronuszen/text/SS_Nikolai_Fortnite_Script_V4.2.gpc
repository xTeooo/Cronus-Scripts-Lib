combo cBoostRecoil {
	Boost++;

	Recoil = get_ival(AimY) + VerticalValue + Boost;

	if(Boost == 10)
	{
		Boost = 0;
	}

	if(Recoil > 100)
	{
		Recoil = 100;
	}

	Set_Val(AimY, Recoil * Inverted);

	Recoil = get_ival(AimX) + HorizontalValue;

	if(Recoil > 100)
	{
		Recoil = 100;
	}

	set_val(AimX, Recoil);
}

