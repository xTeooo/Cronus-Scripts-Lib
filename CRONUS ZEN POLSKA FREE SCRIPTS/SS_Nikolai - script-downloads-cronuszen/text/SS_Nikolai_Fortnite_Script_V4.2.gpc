					if((BoostAR != 0) && (TimeValue > 40))
					{
						BoostAR   = 0;
						TimeValue = 0;
					}

					RecoilTimer += get_rtime();

					if(RecoilTimer > 70)
					{
						RumValue    = 15;
						RecoilTimer =  0;
					}
					else
					{
						RumValue = 0;
					}
						OutPut = (VerticalValue + RumValue + BoostAR);
						Set_Val(AimY, OutPut* Inverted);
						Set_Val(AimX, HorizontalValue);
				}

					if (!get_ival(Fire_BTN))
					{
						RecoilTimer = 0;
						TimeValue   = 0;
						BoostAR     = 0;
					}
			break;
			}
		}
