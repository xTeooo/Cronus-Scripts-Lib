function GetShapes(f_var) {
	if (!SpiralPosition1) {
		InnerSpiral -= 1;
		if (InnerSpiral < YewWorksShapeRangeMin)SpiralPosition1 = On;
			}
	else {
		InnerSpiral += 1;
		if (InnerSpiral > f_var)SpiralPosition1 = Off;
			}
	if (!SpiralPosition2) {
		OuterSpiral -= 1;
		if (OuterSpiral < YewWorksShapeRangeMin)SpiralPosition2 = On;
			}
	else {
		OuterSpiral += 1;
		if (OuterSpiral > f_var)SpiralPosition2 = Off;
			}
	if (YewWorksShape == Circular) {
		RYT = (get_val(RY)) + ((SinAngle * f_var))/100;
		RXT = (get_val(RX)) + ((CosAngle * f_var))/100;
			}
	if (YewWorksShape == TallOval) {
		RYT = (get_val(RY)) + ((SinAngle * f_var))/100;
		RXT = (get_val(RX)) + ((CosAngle * YewWorksShapeRangeMin))/100;
			}
	if (YewWorksShape == WideOval) {
		RXT = (get_val(RX)) + ((CosAngle * f_var))/100;
		RYT = (get_val(RY)) + ((SinAngle * YewWorksShapeRangeMin))/100;
			}
	if (YewWorksShape == Spiral) {
		RYT = (get_val(RY)) + ((SinAngle * InnerSpiral))/100;
		RXT = (get_val(RX)) + ((CosAngle * OuterSpiral))/100;
			}
	if (YewWorksShape == Helix) {
		if (PositionT <= 1) {
			RXT = (get_val(RX)) + ((CosAngle * f_var))/100;
					}
		if (PositionT > 1) {
			RYT = (get_val(RY)) + ((SinAngle * f_var))/100;
					}
		PositionT++;
		if (PositionT == 4) PositionT = Off;
			}
	set_val(RX,RXT);
	set_val(RY,RYT);
	}
