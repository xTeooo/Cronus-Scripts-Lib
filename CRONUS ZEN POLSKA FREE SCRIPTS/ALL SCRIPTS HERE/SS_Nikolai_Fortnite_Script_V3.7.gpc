	    Timer++;

		if(Timer >= TaylorAngle)
		{
			anglePosition ++;
			Timer = 0;
		}

		if(anglePosition == 8) 
			anglePosition = 0;
	}

	if(TaylorShapes == 17)
	{
		if(anglePositionV1 == 0)
		{
