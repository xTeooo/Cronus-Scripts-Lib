	set_rgb(0, 255, 0);
	wait(100);
	set_rgb(0, 0, 0);
	wait(100);
	set_rgb(0, 255, 0);
	wait(100);
	set_rgb(0, 0, 0);
	wait(100);
	set_rgb(0, 255, 0);
	wait(2000);
	set_rgb(0, 0, 0);
}

combo FlashRed {
	set_rgb(255, 0, 0);
	wait(100);
	set_rgb(0, 0, 0);
	wait(100);
	set_rgb(255, 0, 0);
	wait(100);
	set_rgb(0, 0, 0);
	wait(100);
	set_rgb(255, 0, 0);
	wait(2000);
	set_rgb(0, 0, 0);
}

combo cStickyAA {
	Set_Val(AimY, StickyAimSize);
	wait(StickyAimTime);
	Set_Val(AimX, StickyAimSize);
	wait(StickyAimTime);
	Set_Val(AimY, inv(StickyAimSize));
	wait(StickyAimTime);
	Set_Val(AimX, inv(StickyAimSize));
	wait(StickyAimTime);
}

combo cPolar {
	Set_Ival(AimX,	Radius);
	wait(20);
	Set_Ival(AimY,	Radius);
	wait(20);
	Set_Ival(AimX,	inv(Radius));
	wait(20);
	Set_Ival(AimY,	inv(Radius));
	wait(20);
}

combo cBoostRecoil {
	Boost++;

	Recoil = get_val(AimY) + VerticalValue + Boost;

	if(Boost == 10)
	{
		Boost = 0;
	}

	if(Recoil > 100)
	{
		Recoil = 100;
	}

	Set_Val(AimY, Recoil * Inverted);
}

