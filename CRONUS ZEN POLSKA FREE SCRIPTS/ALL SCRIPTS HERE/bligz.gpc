function ScreenTime()
{

    if (Clear)
    {
        Ms = 0; Sec = 0; Min = 0;
        Clear = Off;
    }

    if (ScreenTime)
    {
        Ms += get_rtime();

        if (Ms >= 1000)
        {
            Ms = 0;
            Sec += 1
        }
        if (Sec == 60)
        {
            Sec = 0;
            Min += 1;
        }
        if (Min == TimeoutLimit)
        {
            Saver = 0;
            Ms = 0;
            Sec = 0;
            Min = 0;
            ScreenTime = 0;
            cls_oled(0);
        }
    }
}

