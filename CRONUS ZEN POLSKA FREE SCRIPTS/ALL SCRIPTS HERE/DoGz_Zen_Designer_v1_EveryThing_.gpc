                DoGzARVal -= 1;
            }

        if (DoGzARPos && DoGzARVal == ArProgFinish) 
            DoGzARPos = Off;

            if (!DoGzARPos && DoGzARVal == ArProgStart)
                DoGzARPos = On;

                    if(ProgARInvert) {
                        Set_Val(RY,DoGzARVal * -1);
                    }
                        else {
                            Set_Val(RY,DoGzARVal);
                        }
    }
}

