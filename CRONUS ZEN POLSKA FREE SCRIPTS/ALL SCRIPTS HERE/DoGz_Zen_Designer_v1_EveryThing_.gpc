    TimeDoGz += get_rtime();
    
    if (DoGzVersion2) {
        if (DoGzPos) {
            DoGzVal += DoGzIncrement;
        }
            else {
                DoGzVal -= DoGzIncrement;
            }

        if (DoGzPos && DoGzVal == DoGzAAMax) 
            DoGzPos = Off;

            if (!DoGzPos && DoGzVal == DoGzAAMin)
                DoGzPos = On;

        if (TimeDoGz < 50)
            Set_Val(RX,DoGzVal);
            
