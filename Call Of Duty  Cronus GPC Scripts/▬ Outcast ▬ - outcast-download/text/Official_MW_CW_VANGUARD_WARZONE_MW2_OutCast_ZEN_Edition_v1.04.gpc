	 				if(rightStickMovement > steadyMinThreshold  && rightStickMovement <= steadyMaxThreshold || adaptiveSteadyAim){ 
					    if(RY) {
					    	if(abs(RY) >= 0) {
					        	steadyAim = (RY % steadyAimValue)
					        	if(steadyAim >= steadyAimValue){  
						        	RY = RY + (((steadyAim * steadyAimValue) / 2) - steadyAim);
						        }
								if(steadyAim <= steadyAimValue){ 
									RY = (RY - steadyAim);
								} 
					      	}
					      	set_val(POLAR_RY,clamp(RY, -32767,32767));
					   }
					   if(RX) {
					    	if(abs(RX) >= 0) {
					        	steadyAim = (RX % steadyAimValue)
					        	if(steadyAim >= steadyAimValue){  
						        	RX = RX + (((steadyAim * steadyAimValue) / 2) - steadyAim);
						        }
								if(steadyAim <= steadyAimValue){ 
									RX = (RX - steadyAim);
								} 
					      	}
					      	set_val(POLAR_RX,clamp(RX, -32767,32767));
					    } 
					}	
			    } 
			    if(boostOnFire[profileIdx] == 1 && get_ival(Fire)){ 
			    	angle = (angle + speedAngle[profileIdx] + boostSpeed[profileIdx]) % 360;
			    	if(spiralChange[profileIdx] == 1){ 
						GetAngle(taylorShapes[profileIdx],angle,(radiusSize[profileIdx] * 327) + random(boostMinRadius[profileIdx] * 327,boostMaxRadius[profileIdx] * 327)); 
					} 
					else { 
						GetAngle(taylorShapes[profileIdx],angle,(radiusSize[profileIdx] * 327) + (boostMaxRadius[profileIdx] * 327)); 
					}
				}
				else { 
					GetAngle(taylorShapes[profileIdx],angle,radiusSize[profileIdx]  * 327);
					angle = (angle + speedAngle[profileIdx]) % 360;
				} 
				if(taylorTracking[profileIdx] >= 1){ 
					trackingAngle    = (trackingAngle + trackingSpeed[profileIdx]) % 360;
					if(taylorTracking[profileIdx] == 1){ 
						trackingAngleOne = ((trackingSize[profileIdx] * 327) * sin(trackingAngle)/23170);
				   	    trackingAngleTwo = ((trackingSize[profileIdx] * 327) * cos(trackingAngle)/23170);
				   	} 
				   	if(taylorTracking[profileIdx] == 2){
						dynamicTracking  = (trackingSize[profileIdx]  * (32767 - isqrt(pow(abs(get_val(POLAR_RX)), 2) + pow(abs(get_val(POLAR_RY)), 2)))) / 32767;
						trackingAngleOne = ((dynamicTracking * 327) * sin(trackingAngle)/23170);
				   	    trackingAngleTwo = ((dynamicTracking * 327) * cos(trackingAngle)/23170);
				   	}
				   	if(taylorTracking[profileIdx] == 3){
						dynamicTracking  = ((((trackingSize[profileIdx] * 2) * 327) + 32767 + isqrt(pow(abs(get_val(POLAR_RX)), 2) + pow(abs(get_val(POLAR_RY)), 2)) * (trackingSize[profileIdx]))) / 32767;
						trackingAngleOne = ((dynamicTracking * 327) * sin(trackingAngle)/23170);
				   	    trackingAngleTwo = ((dynamicTracking * 327) * cos(trackingAngle)/23170);
				   	} 
				}
				if(aimAssist[profileIdx] == 1){ 
