function ToggleSwitch(f_val) { 
	  
	if(ModORVal[1] == ModsNToggles[ModIdx++][0]){
		
		if(ModsNToggles[ModIdx][7] == 1) { 
			if(event_press(PS4_UP)){
	      		f_val += 1;
	      		if((f_val + MasterToggles[ToggleAdminVals[ProfileIdx]][ModORVal[1]])  > ModsNToggles[ModORVal[1]][2])
	       		f_val = ModsNToggles[ModORVal[1]][2] - MasterToggles[ToggleAdminVals[ProfileIdx]][ModORVal[1]];
                DisplayMenu = TRUE;    
            }
            if(event_press(PS4_DOWN)){	
            	f_val -= 1;
			    if((f_val + MasterToggles[ToggleAdminVals[ProfileIdx]][ModORVal[1]]) < inv(ModsNToggles[ModORVal[1]][1]))
			    f_val = inv(ModsNToggles[ModORVal[1]][1] + MasterToggles[ToggleAdminVals[ProfileIdx]][ModORVal[1]]);	
		        DisplayMenu = TRUE;
            }
            if(ModsNToggles[ModIdx][8] == 4){
            	String(AATogglev2[MasterToggles[ToggleAdminVals[ProfileIdx]][ModORVal[1]] + f_val],Small,6,0);
            }
            if(ModsNToggles[ModIdx][8] == 0){
            	String(ToggleOnly[MasterToggles[ToggleAdminVals[ProfileIdx]][ModORVal[1]] + f_val],Small,6,0); 
            } 
            if(ModsNToggles[ModIdx][8] == 3) { 
				if(f_val == 1){ 
					ToggleDoGzAA[ProfileIdx] = 0;
				} 
			} 
        }
        if(ModsNToggles[ModIdx][7] == 2) { 
			if(event_press(PS4_UP)){
	      		f_val += 1;
	      		if((f_val + MasterToggles[ToggleAdminv1v2[ProfileIdx]][ModORVal[1]])  > ModsNToggles[ModORVal[1]][2])
	       		f_val = ModsNToggles[ModORVal[1]][2] - MasterToggles[ToggleAdminv1v2[ProfileIdx]][ModORVal[1]];
                DisplayMenu = TRUE;    
            }
            if(event_press(PS4_DOWN)){	
            	f_val -= 1;
			    if((f_val + MasterToggles[ToggleAdminv1v2[ProfileIdx]][ModORVal[1]]) < inv(ModsNToggles[ModORVal[1]][1]))
			    f_val = inv(ModsNToggles[ModORVal[1]][1] + MasterToggles[ToggleAdminv1v2[ProfileIdx]][ModORVal[1]]);	
		        DisplayMenu = TRUE;
            }
            if(ModsNToggles[ModIdx][8] == 3){
            	String(AAToggle[MasterToggles[ToggleAdminv1v2[ProfileIdx]][ModORVal[1]] + f_val],Small,6,0);
            }
        }
        if(ModsNToggles[ModIdx][7] == 0 && ModsNToggles[ModIdx][8] != 16 && ModsNToggles[ModIdx][8] != 13){  
		
			f_val = MultiFunction(PS4_UP,PS4_DOWN,f_val,1,ModsNToggles[ModORVal[1]][1],ModsNToggles[ModORVal[1]][2]); 
		
			if(ModsNToggles[ModIdx][8] == 16) { 
				if(f_val == 1){ 
					ToggleAdminVals[ProfileIdx] = 0;
					TaylorAssist[ProfileIdx] = 0;
				} 
			} 
			if(ModsNToggles[ModIdx][8] == 13) { 
				if(f_val == 1){ 
					ToggleAdminv1v2[ProfileIdx] = 0;
					ToggleTaylorAA[ProfileIdx] = 0;
				} 
			} 
			
			if(ModsNToggles[ModIdx][8] == 1) { 
				if(f_val == 1){ 
					ToggleProgressive[ProfileIdx] = 0;
					ToggleBasicProgAR[ProfileIdx] = 0;
				} 
			} 
			if(ModsNToggles[ModIdx][8] == 15) { 
				if(f_val == 1){ 
					ToggleProgressive[ProfileIdx] = 0;
					ToggleTaylorAR[ProfileIdx] = 0;
				} 
			} 
			if(ModsNToggles[ModIdx][8] == 2) { 
				if(f_val == 1){ 
					ToggleBasicProgAR[ProfileIdx] = 0;
					ToggleTaylorAR[ProfileIdx] = 0;
				} 
			} 
			if(ModsNToggles[ModIdx][8] == 14) { 
				if(f_val == 1){ 
					ToggleBurstFire[ProfileIdx] = 0;
				} 
			} 
			if(ModsNToggles[ModIdx][8] == 5) { 
				if(f_val == 1){ 
					ToggleRapidFire[ProfileIdx] = 0;
				} 
			} 
			if(ModsNToggles[ModIdx][8] == 14){
				RfFireType[ProfileIdx] = MultiFunction(PS4_R1,PS4_L1,RfFireType[ProfileIdx],1,0,1); 
			}
			if(ModsNToggles[ModIdx][8] == 5){
				BfFireType[ProfileIdx] = MultiFunction(PS4_R1,PS4_L1,BfFireType[ProfileIdx],1,0,1); 
			}
			if(ModsNToggles[ModIdx][8] == 1 || ModsNToggles[ModIdx][8] == 15 || ModsNToggles[ModIdx][8] == 2){
				ARFireType[ProfileIdx] = MultiFunction(PS4_R1,PS4_L1,ARFireType[ProfileIdx],1,0,1); 
			}
		    if(ModsNToggles[ModIdx][8] == 1) { 
		    	String(Activation[ARFireType[ProfileIdx]],Small,1,0);
		    	String(ARToggle[ToggleTaylorAR[ProfileIdx]],Small,12,0);
		    }
		    else if(ModsNToggles[ModIdx][8] == 15) { 
		    	String(Activation[ARFireType[ProfileIdx]],Small,1,0);
		    	String(ProgAR[ToggleBasicProgAR[ProfileIdx]],Small,12,0);
		    }
		    else if(ModsNToggles[ModIdx][8] == 20) { 
		    	String(QScopeToggle[ToggleQuickScope[ProfileIdx]],Small,6,0);
		    }
		    else if(ModsNToggles[ModIdx][8] == 2) { 
		    	String(Activation[ARFireType[ProfileIdx]],Small,1,0);
		    	String(ProgAR[ToggleProgressive[ProfileIdx]],Small,12,0);
		    }
		    else if(ModsNToggles[ModIdx][8] == 3){
		    	String(AAToggle[ToggleTaylorAA[ProfileIdx]],Small,6,0);
		    } 
		    else if(ModsNToggles[ModIdx][8] == 14){
		    	String(RFToggle[RfFireType[ProfileIdx]],Small,1,0);
		    	
		    	if(f_val == 1)
					String(Misc[0],Small,12,0);   
				else 
			    	String(Misc[1],Small,12,0);  
		    }
		    else if(ModsNToggles[ModIdx][8] == 5){
		    	String(RFToggle[BfFireType[ProfileIdx]],Small,1,0);
		    	String(BurstFireMode[ToggleBurstFire[ProfileIdx]],Small,12,0);
		    	
		    }
		    else if(ModsNToggles[ModIdx][8] == 6){
		    	String(SlideType[ToggleCancelSlide],Small,6,0);
		    } 
		    else if(ModsNToggles[ModIdx][8] == 7){
		    	String(VMSpeed[ToggleVmSpeed],Medium,6,0);
		    } 
		    else if(ModsNToggles[ModIdx][8] == 8){
		    	String(ControllerLayouts[Buttons],Small,6,0);
		    } 
		    else if(ModsNToggles[ModIdx][8] == 9){
		    	String(ControllerSticks[Sticks],Small,6,0);
		    } 
		    else if(ModsNToggles[ModIdx][8] == 10){
		    	String(PressButtons[ActivateIndividualBtnIdx],Small,6,0);
		    } 
		   
		    else if(ModsNToggles[ModIdx][8] == 11){
		    	String(UnmapButtons[UnmapBtnIdx[0]],Small,6,0);
		    } 
		    else if(ModsNToggles[ModIdx][8] == 12){
		    	String(UnmapButtons[UnmapBtnIdx[1]],Small,6,0);
		    }  
		    else
		    {
			    if(f_val == 1)
					String(Misc[0],Small,6,0);   
				else 
			    	String(Misc[1],Small,6,0);  
			} 
	    }
	
	    if(ModsNToggles[ModIdx][8] == 13){
			if(event_press(PS4_UP)){ 
				f_val = Refresh(f_val + 1,ModsNToggles[ModORVal[1]][1],ModsNToggles[ModORVal[1]][2]) 
				ResetAdminValues();
				DisplayMenu = TRUE;
			} 
			if(event_press(PS4_DOWN)){ 
				f_val = Refresh(f_val - 1,ModsNToggles[ModORVal[1]][1],ModsNToggles[ModORVal[1]][2]) 
				ResetAdminValues();
				DisplayMenu = TRUE;
			} 
		    String(AdminAA[ToggleAdminVals[ProfileIdx]],Small,6,0);
		}
		
		if(ModsNToggles[ModIdx][8] == 16){
			if(event_press(PS4_UP)){ 
				f_val = Refresh(f_val + 1,ModsNToggles[ModORVal[1]][1],ModsNToggles[ModORVal[1]][2]) 
				ResetAdminValuesv1();
				DisplayMenu = TRUE;
			} 
			if(event_press(PS4_DOWN)){ 
				f_val = Refresh(f_val - 1,ModsNToggles[ModORVal[1]][1],ModsNToggles[ModORVal[1]][2]) 
				ResetAdminValuesv1();
				DisplayMenu = TRUE;
			} 
		    String(AdminAA[ToggleAdminv1v2[ProfileIdx]],Small,6,0);
		}
	}  	
    return f_val;
}

