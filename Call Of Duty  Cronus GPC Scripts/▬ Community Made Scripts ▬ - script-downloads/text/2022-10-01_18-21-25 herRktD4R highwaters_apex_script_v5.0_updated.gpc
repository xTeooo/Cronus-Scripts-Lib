int X,Y,a;
int radius,step;
int circleTop = TRUE; 
function Circle()  {
    if(circleTop) {
        Y = isqrt(pow(radius,2) - pow(X,2));
        set_val(AIM_X,X + get_val(AIM_X));
        set_val(AIM_Y,Y + get_val(AIM_Y)); 
        X += step;
        if(X > radius) {
            circleTop = FALSE;
            X = radius;
         }
    }else{
        Y = inv(isqrt(pow(radius,2) - pow(X,2)));
        set_val(AIM_X,X + get_val(AIM_X));
        set_val(AIM_Y,Y + get_val(AIM_Y)); 
        X -= step;
        if(X < inv(radius)) {
            if(!get_val(XB1_RT)) {
                circleTop = TRUE;
                X = inv(radius)
             }else{
                X = radius;    
             }
        }   
    }
}
int _RET;
function GetSteadyAim(Axis, _Resolution) {
    if (Axis >= 0)
    {
        _RET = (Axis % _Resolution);
        if (_RET >= (_Resolution / 2))
            Axis = Axis + (_Resolution - _RET);
        else
            Axis -= _RET;
    }
    else
    {
        _RET = (Axis % _Resolution);
        if (abs(_RET) >= (_Resolution / 2))
            Axis = Axis - (_Resolution + _RET);
        else
            Axis += abs(_RET);
    }
        return Axis;
}
