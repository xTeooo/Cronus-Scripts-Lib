int SmartSens_side,SmartSens,final_val;
function SmartSens(f_val, f_axis) { 

	if(f_val < 0){
		SmartSens_side = -1; // Left / Up 
	}
	if(f_val >= 0){
		SmartSens_side = 1; // Right / Down 
	}
	
	if(SmartSens_side == -1) // if f_val has a negative value (Left side or Up) 
	{
	    if(get_ival(f_axis) > 0) // if stick input has a positive value (Right side or Down)
	    	SmartSens = (get_val(f_axis) * (100 - f_val)) / 100; 
	    
	    else if(get_ival(f_axis) <= 0) // if stick input has a negative value (Left side or Up)
	    	SmartSens = (get_val(f_axis) * (100 + f_val)) / 100;
	}
	
	if(SmartSens_side == 1) // if f_val has a positive value (Right side or Down)
	{
	    if(get_ival(f_axis) < 0) // if stick input has a negative value (Left side or Up)
	    	SmartSens = (get_val(f_axis) * (100 + f_val)) / 100; 
	    	
	    else if(get_ival(f_axis) >= 0) // if stick input has a positive value (Right side or Down)
	    	SmartSens = (get_val(f_axis) * (100 - f_val)) / 100;
	}
	
	final_val = f_val + SmartSens;
	
// Set SmartSens on the axis
	if(SmartSens_side == -1 && get_val(f_axis) >= final_val || SmartSens_side == 1 && get_val(f_axis) <= final_val)
	    set_val(f_axis, clamp(final_val + bfr, -100, 100));
	    
	set_val(TRACE_1, f_val + SmartSens);
}
const int16 Sight_Vals[]={0, 15, 30};
const int16 Barrel_Vals[][]={
{   0,    0,    0},   //    0
{  -5,  -20,  -10},   //    1
{ -10,  -25,  -15},   //    2
{ -15,  -30,  -20}};  //    3
int adjustments,hor_side, new_ar, HS;
function adjust_recoil(axis, val, s, b, h, e, diff){
	if(axis)
		hor_side = Barrel_Vals[b][1] + GUN_INFO[weapon[in_hand]][24];
	else
		hor_side = Barrel_Vals[b][0] + GUN_INFO[weapon[in_hand]][25];
	adjustments = Sight_Vals[s] + hor_side + e + diff;
	new_ar = ((val * (response_curve + 3)) * 100) / 1000;
	val = ((new_ar * adjustments) / 100) + buffer + h;
	return val;
}
int pd;
function percent_diff(setting, base) {
	pd = (((base - setting) * 100) / ((setting + base) / 2));
	return pd;
}

