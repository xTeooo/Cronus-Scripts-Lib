// helpers
function finder(row,col,length) {
    return (row * length) + col;
}
function center_x( f_chars, f_font) {
	return (OLED_WIDTH / 2) - ((f_chars * f_font) / 2);
}       
function center_right( f_chars, f_font) {
	return OLED_WIDTH - (f_chars * f_font);
}  
function center_y(f_size) {
	//  center_y(font size);  
	//	    return Y for Centering String Vertically
	return (OLED_HEIGHT  / 2) - (f_size / 2);
}
function get_string(f_idx){
	if (STRINGS[f_idx + 1] != -1) 
		return STRINGS[f_idx + 1] - STRINGS[f_idx] - 1;
	return sizeof(STRINGS) - STRINGS[f_idx];
}
function get_aa(f_idx){
	if (AA_LIST[f_idx + 1] != -1) 
		return AA_LIST[f_idx + 1] - AA_LIST[f_idx] - 1;
	return sizeof(AA_LIST) - AA_LIST[f_idx];
}
function get_category(f_idx){
	if (CATEGORIES[f_idx + 1] != -1) 
		return CATEGORIES[f_idx + 1] - CATEGORIES[f_idx] - 1;
	return sizeof(CATEGORIES) - CATEGORIES[f_idx];		
}
function get_mod(f_idx){
	if (CATEGORIES[f_idx + 1] != -1) 
		return CATEGORIES[f_idx + 1] - CATEGORIES[f_idx] - 1;
	return sizeof(CATEGORIES) - CATEGORIES[f_idx];		
}
function press_hold(f_btn) { 
	//  return TRUE - event_press()  			    - every 250ms when button is held
	return event_press(f_btn) || get_val(f_btn) && get_ptime(f_btn) > 250 && get_ptime(f_btn) % (get_rtime() * 8) == 0;
}
function offset(Stick,Value) {
  set_val(Stick, clamp(Value * (100 - abs(get_val(Stick))) / 100 + get_val(Stick), -100, 100));
  return;
}
