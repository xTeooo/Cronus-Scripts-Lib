int double_tap_timer[38], double_tap_count[38];
function double_tap(f_btn) {
  if(double_tap_timer[f_btn])
    double_tap_timer[f_btn] -= get_rtime();
  if(event_press(f_btn))
  {
  	if(double_tap_count[f_btn] < 2 && f_btn < 39)
  		double_tap_count[f_btn] += 1;
  	double_tap_timer[f_btn] = 250;
  }
  if((double_tap_timer[f_btn] <= 50 && double_tap_count[f_btn] == 1) || double_tap_count[f_btn] == 2)
  {
  	return double_tap_count[f_btn];
  }
  return 0;
}
int just_selected;
function set_weapon(button, number, btn_count, t) {
	if(get_ival(button))
		set_val(button, 0);
	if(btn_count >= 1)
	{
		if(t == 10)
		{
			just_selected = TRUE;
			to_set = btn_count - 1;
			in_hand = btn_count - 1;
			weapon[to_set] = number;
		}
		if(t == 1 && weapon[to_set] != 0)
		{
			to_set = btn_count - 1;
			barrel[to_set] = number;
			if(barrel[to_set] > 3 || GUN_INFO[weapon[to_set]][2] > 1)
				barrel[to_set] = 0;
			color = barrel[to_set] + 3;
		}
		if(t == 2 && weapon[to_set] != 0)
		{
			to_set = btn_count - 1;
			sight[to_set] = number;
			if(sight[to_set] > 2 || (GUN_INFO[weapon[to_set]][2] && GUN_INFO[weapon[to_set]][2] <= 2 && sight[to_set] > 1) || GUN_INFO[weapon[to_set]][2] == 4)
				sight[to_set] = 0;
			color = sight[to_set] + 4;
		}
		if(t == 0 || t == 20)
		{
			in_hand = btn_count - 1;
			weapon[in_hand] = quick_gun[(in_hand * 4) + number];
			barrel[in_hand] = quick_barrel[(in_hand * 4) + number];
			sight[in_hand]  = quick_sight [(in_hand * 4) + number];
			display_title = TRUE;
		}
		if(t && t != 20)
			display_select = t;
		if(t == 10)
			reset = FALSE;
		double_tap_count[button] = 0;
	}
}
function kb_set_weapon(button, number, t) {
	//if(get_ival(button))
	//	set_val(button, 0);
	if(key_press(button))
	{
		to_set = in_hand;
		if(t == 10)
		{
			just_selected = TRUE;
			weapon[to_set] = number;
		}
		if(t == 1)
		{
			barrel[to_set] = number;
			if(barrel[to_set] > 3 || GUN_INFO[weapon[to_set]][2] > 1)
				barrel[to_set] = 0;
			color = barrel[to_set] + 3;
		}
		if(t == 2)
		{
			sight[to_set] = number;
			if(sight[to_set] > 2 || (GUN_INFO[weapon[to_set]][2] && GUN_INFO[weapon[to_set]][2] <= 2 && sight[to_set] > 1) || GUN_INFO[weapon[to_set]][2] == 4)
				sight[to_set] = 0;
			color = sight[to_set] + 4;
		}
		if(t == 0 || t == 20)
		{
			weapon[to_set] = quick_gun[(to_set + 1) * number];
			barrel[to_set] = quick_barrel[(to_set + 1) * number];
			sight[to_set]  = quick_sight [(to_set + 1) * number];
			display_title = TRUE;
		}
		if(t && t != 20)
			display_select = t;
		reset = FALSE;
	}
}
