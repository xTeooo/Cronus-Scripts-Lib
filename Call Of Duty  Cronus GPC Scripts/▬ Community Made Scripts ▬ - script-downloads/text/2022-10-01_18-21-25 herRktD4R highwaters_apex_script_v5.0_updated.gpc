function AimAssistActivation(x) {
	if(x == 0){ 
		if(get_ival(ADS) || get_ival(FIRE)) 
			return TRUE;
	} 
	if(x == 1){ 
		if(get_ival(ADS) && get_ival(FIRE)) 
			return TRUE;
	} 
	if(x == 2){ 
		if(get_ival(FIRE) && !get_ival(ADS))
			return TRUE;
	} 
	if(x == 3){ 
		if(get_ival(ADS) && !get_ival(FIRE))
			return TRUE;
	} 
	if(x == 4){
		if(right_stick_radius <= deadzone)
			return TRUE;
	}
	return 0;
} 
