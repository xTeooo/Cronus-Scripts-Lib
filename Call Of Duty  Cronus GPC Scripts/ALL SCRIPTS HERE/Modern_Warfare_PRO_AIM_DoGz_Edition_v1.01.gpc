		{
			value += increase_value;                           
			if(value > max ) value = max;   	               	            
		}                                                                       
		if(get_val(PS4_L2) && event_press(PS4_LEFT)) 
        {
			value -= increase_value;                           
			if(value < min ) value = min; 	        
		}
		if(get_val(PS4_L2) && event_press(PS4_UP))
		{
		   value += increase_value_more;
