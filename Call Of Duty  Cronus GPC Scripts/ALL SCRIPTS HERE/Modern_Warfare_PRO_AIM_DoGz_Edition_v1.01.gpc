function AIM() {
	if (D_AS) {
	if (D_AS && VM) {
		vm_tctrl(-2);
	}
		D__AS ++;
	if (D__AS >= D___AS)  D__AS = D__AS1;
		D__AS1 --;
	if (D__AS1 <= D_AS1)  D__AS1 = D__AS;
		D__AS ++;
	if (get_val(AIM_BTN)) {	
		set_val(PS4_RX,get_val(PS4_RX) + D__AS);
		set_val(PS4_RY,get_val(PS4_RY) + D__AS);
	}
	if (get_val(FIRE_BTN)) {	
		set_val(PS4_RX,get_val(PS4_RX) + D__AS);
		set_val(PS4_RY,get_val(PS4_RY) + D__AS);
		}
	}
	else
	vm_tctrl(0); 
} 
function PROGRESSION(VALUE,MIN,MAX,INCREMENT,FLOW,ID) {
if (FLOW) {
    VALUE += INCREMENT;
if (VALUE <= MAX)  
    return VALUE;
if (VALUE > MAX) {
        ID = 0;
    return VALUE - INCREMENT;}
	}
else {
    VALUE -= INCREMENT;    
if (VALUE >= MIN)  
    return VALUE;
if (VALUE < MIN) {
   	ID = 1;
    return VALUE + INCREMENT;
    	}
	} 
   		 return FALSE;    
}
 function DoGz_ARC () {
if (ARC){
	ARC__VAL   = ARC_VAL / DIVIDE;
	ARC____VAL = ARC_VAL / DIVIDE;
	ARC_VAL ++;
if (ARC_VAL + ARC_V >= ARC___VAL + ARC_V) {
	ARC_VAL = ARC__VAL;
	}
	ARC__VAL --;
if (ARC__VAL <= ARC____VAL) {
	ARC__VAL = ARC_VAL;
	}
	ARC_VAL ++;
