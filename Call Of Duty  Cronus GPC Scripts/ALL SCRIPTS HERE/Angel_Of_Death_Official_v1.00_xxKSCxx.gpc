	      						if(get_val(PS4_L2)> 95){       
	      											combo_run(AimAbuseScope);} 	
	      						if(event_release(PS4_L2)){ 
	      											combo_stop(AimAbuseScope);
		}
	}    
			//LOGO
			
		   if(display_title){           
	            cls_oled(0);            
	            DrawLogo(0, 0, 0);
	            display_title = FALSE;  
	            display_black = TRUE;
	}                             
	       if(display_black){                            
	           count_black += get_rtime();
	       if(count_black >= 9000){                        
	           cls_oled(0);         
	           count_black = 0;      
	           display_black = FALSE; 
	        	}
	    	}
		}
		
			if (PlayStationOnXbox && get_console() == PIO_XB360 && get_controller() == PIO_PS4) { 
	
		swap(PS4_TOUCH,PS4_SHARE);
    }
} 

	
