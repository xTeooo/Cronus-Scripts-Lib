	define LegacyRecoil = TRUE;			//Change to TRUE; if you want LEGACY ANTI RECOIL		//Change to FALSE; if you do not want LEGACY ANTI RECOIL
	define InvertedYAxis = FALSE;		//If you play inverted change to TRUE;					//If you do not play inverted change to FALSE;
	define LegacyHipfire = TRUE;		//Change to TRUE; if you want LEGACY HIP FIRE			//Change to FALSE; if you do not want LEGACY HIP FIRE
	
	//Aniti recoil values
	int LegacyRX = 1;			//RX = horizontal //If it pulls up add values, if it pulls down subtract the values. If you play inverted use inverted values
	int LegacyRY = 1; 			//RY = vertical	  //If you want to save your values change them here. Or just use the setting on the fly
	
//AIM ASSIST

	int ToggleTaylorAssist =  2;     // Choose your shape  
	int TaylorRadius       =  9;     // Choose your values      
	int TaylorAngle        = 12;	 // Choose your values
	int TaylorRadiusMin    =  7;	 // Choose your values
	
	// Toogle and Values for Boost...
	int ToggleRumbleBoost  = FALSE;		// Change to TRUE; , if you want BOOST		//Change to FALSE; if you do not want BOOST
	int TaylorAngleBoost   = 5;			// For Boost ONLY 
	int PolarBoost         = 10;		// For Boost ONLY 
	
	int RX,RY,angle,CosAngle,SinAngle,SpiralPosition1,SpiralPosition2,InnerSpiral,OuterSpiral,ActualMagnitude,MaxMagnitude,SmartRumble,PolarStrength,Angle;
	
//RAPID FIRE SINGLE/AUTO

	define RapidSingleShot = FALSE;		// With this mod we would have more precision at long distance, it will go shot by shot, the more we go by pulling the trigger-
										// -the faster it will go,when reaching the maximum trigger stop,it will be constant fire.
										
	define RapidAutoMatica = FALSE;		// For semi-automatic weapons we must put this int to FALSE;
	
	define FireHold = 40;				// This would be like the rapid fire values, for that the int RapidAutoMatica should be set to FALSE;
	define FireWait = 33;

	int Wait;
	
//AIM ABUSE

