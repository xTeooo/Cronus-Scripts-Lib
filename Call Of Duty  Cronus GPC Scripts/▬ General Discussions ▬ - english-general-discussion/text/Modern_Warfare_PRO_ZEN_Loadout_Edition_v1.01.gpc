	       combo_run(AUTO_PING);
	   if(event_press(Fire))
           combo_run(AUTO_PING);        			 
    }
    /*== Cancel Slide ==*/
    if(toggle_cancel_slide) 
    {                                                                                                  
	   if((get_val(PS4_LY) < -60) || (get_val(PS4_LY) > 60) || (get_val(PS4_LX) < -60) || (get_val(PS4_LX) > 60))
