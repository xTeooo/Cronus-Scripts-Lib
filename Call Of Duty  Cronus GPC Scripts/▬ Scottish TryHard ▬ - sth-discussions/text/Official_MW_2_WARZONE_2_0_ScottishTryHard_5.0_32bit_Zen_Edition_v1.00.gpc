function fireTypeSwitch(fIdx,fVar,fMax){ 
	if(modNameIdx == fIdx){ 
		if(event_press(PS4_R1)){
			fVar ++;
			updateDisplay = TRUE;
			if(!fVar)
				combo_run(rumbleOff);
			else
				combo_run(rumbleOn); 
		}
		if(event_press(PS4_L1)){ 
			fVar --;
			updateDisplay = TRUE;
			if(!fVar)
				combo_run(rumbleOff);
			else
				combo_run(rumbleOn); 
		}
		fVar    = rotateVar(fVar,0,fMax);  
		
		if(modNameIdx ==  mw2DropS){ 
			print(centerPosition(getStringLength(instaDropOpt[dropShotmw2FireType]) ,OLED_FONT_SMALL_WIDTH),34  ,OLED_FONT_SMALL , OLED_WHITE ,instaDropOpt[dropShotmw2FireType]);
		}
		else if(modNameIdx == legacyRumble || modNameIdx == polarRumbleNew){ 
			print(centerPosition(getStringLength(arActivation[standardARFireType]),OLED_FONT_SMALL_WIDTH),34,OLED_FONT_SMALL,OLED_WHITE,arActivation[standardARFireType])	
		}
		else if(modNameIdx == advancedAR){ 
			print(centerPosition(getStringLength(arActivation[progressiveARFireType]),OLED_FONT_SMALL_WIDTH),34,OLED_FONT_SMALL,OLED_WHITE,arActivation[progressiveARFireType])	
		}
		else if(modNameIdx == taylorAA){ 
			print(centerPosition(getStringLength(aimAssistType[taylorFireType]),OLED_FONT_SMALL_WIDTH),34,OLED_FONT_SMALL,OLED_WHITE,aimAssistType[taylorFireType])	
		}
		else if(modNameIdx == aaRotational){ 
			print(centerPosition(getStringLength(aimAssistType[rotationalFireType]),OLED_FONT_SMALL_WIDTH),34,OLED_FONT_SMALL,OLED_WHITE,aimAssistType[rotationalFireType])	
		}
		else if(modNameIdx == rapidRF){ 
			print(centerPosition(getStringLength(handGunFireType[rapidFireType]) ,OLED_FONT_SMALL_WIDTH),34 ,OLED_FONT_SMALL , OLED_WHITE ,handGunFireType[rapidFireType]);
		}
		else if(modNameIdx == qScope){ 
			print(centerPosition(getStringLength(quickScopeOpt[quickScopeActivation]) ,OLED_FONT_SMALL_WIDTH),34  ,OLED_FONT_SMALL , OLED_WHITE ,quickScopeOpt[quickScopeActivation]);
		}
		else if(modNameIdx == mw2snake){ 
			print(centerPosition(getStringLength(snakeShotOpt[snakeShotActivationmw2]) ,OLED_FONT_SMALL_WIDTH),34  ,OLED_FONT_SMALL , OLED_WHITE ,snakeShotOpt[snakeShotActivationmw2]);
		}
		else if(modNameIdx == crouchST){ 
			print(centerPosition(getStringLength(crouchShotOpt[crouchShotActivation]) ,OLED_FONT_SMALL_WIDTH),34  ,OLED_FONT_SMALL , OLED_WHITE ,crouchShotOpt[crouchShotActivation]);
		}
		else if(modNameIdx == jumpST){ 
			print(centerPosition(getStringLength(jumpShotOpt[jumpShotActivation]) ,OLED_FONT_SMALL_WIDTH),34  ,OLED_FONT_SMALL , OLED_WHITE ,jumpShotOpt[jumpShotActivation]);
		}
	}
	return fVar;
} 
