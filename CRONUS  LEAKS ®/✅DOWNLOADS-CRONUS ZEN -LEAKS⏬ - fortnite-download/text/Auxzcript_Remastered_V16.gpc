    set_val(9,a_f(9,AimingProAimbot));
    wait(Delay)
    set_val(10,a_f(10,AimingProAimbot2));
    wait(Delay)
    set_val(9,a_f(9,AimingProAimbot2));
    wait(Delay)}
    
combo ZenS2 { 
    set_val(10,a_f(10,DynaShootProAimbot));
    wait(Delay)
    set_val(9,a_f(9,DynaShootProAimbot));
    wait(Delay)
    set_val(10,a_f(10,DynaShootProAimbot2));
