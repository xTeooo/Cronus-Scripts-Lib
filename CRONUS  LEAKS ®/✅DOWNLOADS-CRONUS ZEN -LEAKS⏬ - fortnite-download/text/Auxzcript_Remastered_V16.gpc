combo ZenX { 
    set_val(10,a_f(10,HipFireProAimbot));
    wait(Delay)
    set_val(9,a_f(9,HipFireProAimbot));
    wait(Delay)
    set_val(10,a_f(10,HipFireProAimbot2));
    wait(Delay)
    set_val(9,a_f(9,HipFireProAimbot2));
    wait(Delay)}
    
combo ZenS { 
    set_val(10,a_f(10,AimingProAimbot));
