//Insta Edit Reset
int InstaEdit = TRUE
define RESET_BTN = XB1_LEFT; //Set this to your Edit Reset keybind
define CONFIRM_BTN = XB1_B; //Set this to your Edit Confirm keybind

//Ghost Peek V2
int GhostPeek = TRUE;
