wait(250);
set_led(LED_1, 0);
set_led(LED_2, a);
set_led(LED_3, b);
set_led(LED_4, 0);
wait(250);
reset_leds();
}

 
function a_f(p,m) {
if(abs(get_val(p)) < Release)
return m;
return get_val(p);}