int HipFireProAimbot = 11;
int HipFireProAimbot2 = -11;
int AimingProAimbot = 12;
int AimingProAimbot2 = -12;
int DynaShootProAimbot = 14;
int DynaShootProAimbot2 = -14;
int Delay = 20;
