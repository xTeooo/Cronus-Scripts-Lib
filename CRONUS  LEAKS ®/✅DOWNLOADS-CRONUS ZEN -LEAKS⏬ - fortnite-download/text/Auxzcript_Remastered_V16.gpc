wait(GPCooldown);
wait(200);}

combo Dropshot {
set_val(5,100);
wait(Dropshot_Crouching);
wait(Dropshot_Standing);
set_val(5,100);}

combo InstaEdit {
set_val(RESET_BTN,100);
wait(25);
wait(25);
set_val(RESET_BTN,100);
wait(25);
set_val(CONFIRM_BTN,100);
wait(115);}

combo AntiRecoil { 
if(get_val(FIRE_BTN)) {
Anti_Recoil = get_val(10) + AntiRecoil;
if(Anti_Recoil > 100) Anti_Recoil = 100;
set_val(10, Anti_Recoil);
}}

combo Notify {
set_led(LED_1, 0);
