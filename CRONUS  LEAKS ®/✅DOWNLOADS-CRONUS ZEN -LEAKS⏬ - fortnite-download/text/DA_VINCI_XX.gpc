// Aim Assist
int AimAssist = TRUE;
int AssistProfile = 32; // Assist = [Strength1 + Increment1] - (IGNORE IF YOU DON'T UNDERSTAND!)
int Strength1  = 30;
int Increment1 = 10; 
int deadZone   = 9; // Right Stick Deadzone

// Aim Assist Shape
int    TaylorShapes				=    1; // 0 = Disabled, 1 = Circular, 2 = Tall Oval, 3 = Wide Oval, 4 = Spiral, 5 = Helix, 6 = Spiral Oval
int    AngleDirection			= TRUE; // Reverses the Shape Direction
define TaylorRadius				=   12; // Set This Value About 1 Or 2 Higher Then My In-Game Deadzone 
define TaylorSpeed				=   15; // Speed - I Personally Either Use 15 or 20 But Again Feel Free To Play With

// Sticky AIM
define sticky_aim_assist = TRUE;
define _v                =   10; // Right Stick Deadzone

//HEAD SHOT ASSIST 2.0
int HEADSHOT = TRUE;
define HeadShotStrength = 10;  // Right Stick Deadzone
define HeadShotHoldTime = 1300;

