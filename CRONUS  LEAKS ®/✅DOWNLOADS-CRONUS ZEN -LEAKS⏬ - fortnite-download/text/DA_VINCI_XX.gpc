int RapidFireWait = 30;
define RapidFireHip = FALSE;

// Anti-Recoil
int LegacyARecoil   = FALSE;
int ArLegacyRY    = 15;
define LegacyARHip  = FALSE;
define LegacyInvert = FALSE;
