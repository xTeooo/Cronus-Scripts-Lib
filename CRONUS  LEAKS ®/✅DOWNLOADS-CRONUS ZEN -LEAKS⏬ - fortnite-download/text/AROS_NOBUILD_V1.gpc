//unmap XB1_XBOX;
define Build   = XB1_B;
define Wall    = XB1_RT;
define Floor   = XB1_RB;
define Ramp    = XB1_LT;
define Cone    = XB1_LB;
define Trap    = XB1_X;
define ADS     = XB1_LT;
define Fire    = XB1_RT;
define Pickaxe = XB1_Y;
define Edit    = XB1_LS
define Reset   = XB1_RS;
define Confirm = XB1_RB;
define Pickup = XB1_X;
define SNAKEAIMResetMacroButton = XB1_RS;
int SNAKEAIMAimBooster1            = TRUE;
int SNAKEAIMAimBooster1HipFire     = TRUE;
int SNAKEAIMAimBooster2            = TRUE;
int SNAKEAIMAimBooster3            = TRUE;
int SNAKEAIMAimBooster4            = TRUE;
int SNAKEAIMAimBooster5            = TRUE;
int SNAKEAIMAimBooster5HipFire     = TRUE;
int SNAKEAIMAimBooster6            = TRUE;
int SNAKEAIMAimBooster6HipFire     = TRUE;
int SNAKEAIMAimBooster7            = TRUE;
int SNAKEAIMAimBooster8            = TRUE;
int SNAKEAIMPolarAssist1ADS        = TRUE;
int SNAKEAIMPolarAssist1HipFire    = TRUE;
int SNAKEAIMPolarAssist2           = TRUE;
int SNAKEAIMPolarAssist3           = TRUE;
int SNAKEAIMPolarAssist3Timed      = FALSE;
int SNAKEAIMPolarAssist3HipFire    = TRUE;
int SNAKEAIMPolarAssist4           = TRUE;
int SNAKEAIMPolarAssist4ADS        = TRUE;
int SNAKEAIMPolarAssist4HipFire    = TRUE;
int SNAKEAIMAssistedTracking       = TRUE;
int SNAKEAIMShapeAssistBoostToggle = TRUE;
int SNAKEAIMBloomReducer           = TRUE;
int SNAKEAIMRecoilReducer          = FALSE;
int SNAKEAIMRecoilReducerHipFire   = FALSE;
int BuildTracker                   = FALSE;
int SNAKEAIMResetMacro             = FALSE;
int SNAKEAIMPickUpMacroToggle      = FALSE;
int SNAKEAIMTouchPadFix            = TRUE;
int SNAKEAIMAA1Power              = 4;
int SNAKEAIMAA2Power              = 5;
int SNAKEAIMAA2CutOut             = 45;
int SNAKEAIMAA2Range              = 3;
int SNAKEAIMAA2Speed              = 100;
int SNAKEAIMAA3Power              = 5;
int SNAKEAIMAA4Power              = 4;
int SNAKEAIMAA4PowerPlus1         = 5;
int SNAKEAIMAA4Speed              = 100;
int SNAKEAIMAimBooster5PowerX     = 4;
int SNAKEAIMAimBooster5PowerY     = 5;
int SNAKEAIMAA5Time               = 900;
int SNAKEAIMAA6Power              = 5;
int SNAKEAIMAA7Power              = 4;
int SNAKEAIMAA7CutOut             = 10;
int SNAKEAIMAA7Range              = 5;
int SNAKEAIMAA7Speed              = 100;
int SNAKEAIMAA8Sum                = 12;
int SNAKEAIMAA8X3                 = 9;
int SNAKEAIMAA8OneThird           = 3;
define SNAKEAIMConstantAAPowerX   = 5;
define FLIP1111CAPX                     = SNAKEAIMConstantAAPowerX;
define SNAKEAIMConstantAAPowerY   = 2;
define FLIP1111CAPY                     = SNAKEAIMConstantAAPowerY;
define SNAKEAIMConstantAAPowerX2  = 4;
define FLIP1111CAPX2                    = SNAKEAIMConstantAAPowerX2;
define SNAKEAIMConstantAAPowerY2  = 3;
define FLIP1111CAPY2                    = SNAKEAIMConstantAAPowerY2;
define SNAKEAIMConstantAAPowerX3  = 2;
define FLIP1111CAPX3                    = SNAKEAIMConstantAAPowerX3;
define SNAKEAIMConstantAAPowerY3  = 5;
define FLIP1111CAPY3                    = SNAKEAIMConstantAAPowerY3;
int SNAKEAIMConstantAADelay4      = 20;
define SNAKEAIMConstantAAPowerX4  = 4;
define FLIP1111CAPX4                    = SNAKEAIMConstantAAPowerX4;
define SNAKEAIMConstantAAPowerY4  = 3;
define FLIP1111CAPY4                    = SNAKEAIMConstantAAPowerY4;
int SNAKEAIMPolarAssist1Range     = 4;
int SNAKEAIMPolarAssist1Speed     = 100;
int SNAKEAIMPolarAssist2Range     = 4;
int SNAKEAIMPolarAssist2Speed     = 40;
int SNAKEAIMPolarAssist3Power     = 1;
int SNAKEAIMPolarAssist3Time      = 5000;
int SNAKEAIMPolarAssist4Range     = 5;
int SNAKEAIMPolarAssist4Speed     = 100;
int SNAKEAIMPolarAssist4Delay     = 100;
int SNAKEAIMAssistedTrackingPower = 100;
int SNAKEAIMShapeAssistSpeed      = 25;
int SNAKEAIMShapeAssistRange      = 4;
int SNAKEAIMShapeAssistRangeMin   = 2;
int SNAKEAIMShapeAssistSpeedBoost = 5;
int SNAKEAIMShapeAssistRangeBoost = 1;
int SNAKEAIMShapeAssist           = Helix;
int SNAKEAIMBloomReducerDelay               = 22;
define SNAKEAIMRecoilReducerVerticalPower   = 17;
define SNAKEAIMRecoilReducerHorizontalPower = 0;
int BuildSyncTime               = 1000;
int InstantResetDelay           = 10;
define SNAKEAIMResetMacroDelay  = 1;
int DeadZone                    = 10;
//100% Perfect Accuracy [ALWAYS ON] (L2 + R1)
	int PerfectAccuracy = TRUE;
main {
	vm_tctrl(-9);
	block_rumble();
	deadzone(XB1_LT,XB1_RT,100,100);
	if(get_val(Edit))    {
		combo_run(NonActivated);
			}
	if(get_val(Pickaxe)) {
		combo_run (Activated);
			}
	if(get_val(Confirm)) {
		combo_run (Activated);
			}
	if (SNAKEAIMTouchPadFix) {
		swap(PS4_SHARE, PS4_TOUCH);
			}
	if (SNAKEAIMResetMacro) {
		if (event_press(SNAKEAIMResetMacroButton)) {
			combo_run(SNAKEAIMFastReset);
					}
			}
	if (BuildTracker) {
		if (event_press(Build)) {
			BuildTrack = !BuildTrack;
					}
		if (BuildTrack)   {
			BuildCount += get_rtime();
			if (event_press(Pickaxe) || BuildCount == BuildSyncTime) {
				BuildTrack = Off;
				BuildCount = Off;
							}
			if (get_ival(Floor) || get_ival(Cone) || get_ival(Ramp) || get_ival(Wall) || get_ival(Build)) BuildCount = Off;
					}
			}
	if (!BuildTrack)  {
		FLIP1111AASI = (isqrt(pow(get_ival(PS4_RX), 2) + pow(get_ival(PS4_RY), 2)));
		X = get_ival(RX);
		Y = get_ival(RY);
		M = isqrt(pow(X, 2) + pow(Y, 2));
		MaxM = (M < 100);
		if (!(Time++ % SNAKEAIMPolarAssist1Delay)) {
			Angle += SNAKEAIMPolarAssist1Speed;
					}
		Angle = Angle % 360;
		Sin = SNAKEAIMPolarArray[Angle % 360];
		Cos = SNAKEAIMPolarArray[(Angle + 270) % 360];
		Cos = (Cos * SNAKEAIMPolarAssist1Range) / 100;
		Sin = (Sin * SNAKEAIMPolarAssist1Range) / 100;
		if ((SNAKEAIMPolarAssist1ADS && get_ival(ADS)) || (SNAKEAIMPolarAssist1HipFire && get_ival(Fire))) {
			if (M <= SNAKEAIMPolarAssist1Range) {
				Sin -= Y;
				Cos -= X;
							}
			else {
				Sin = (Sin * (200 - ((abs(Y) + M) / 10) * 10) / 200) * MaxM;
				Cos = (Cos * (200 - ((abs(X) + M) / 10) * 10) / 200) * MaxM;
							}
			set_val(RX, clamp(X + Cos, -100, 100));
			set_val(RY, clamp(Y + Sin, -100, 100));
					}
					if (get_val(PS4_L2) && get_val(PS4_R1)) {
PerfectAccuracy = !PerfectAccuracy;
combo_run(PerfectionAccuracy);
set_val(PS4_R1, 0);
} else {
combo_stop(PerfectionAccuracy);
}
		if(SNAKEAIMAimBooster1)      {
			if (get_ival(ADS) || (SNAKEAIMAimBooster1HipFire && get_ival(Fire))) {
				combo_run(SNAKEAIMAA1);
							}
			else {
				combo_stop(SNAKEAIMAA1);
							}
					}
		if(SNAKEAIMAimBooster2)      {
			if (get_ival(ADS)){
				combo_run(SNAKEAIMAA2);
				SNAKEAIMAA2PF();
							}
			if (abs(get_ival(AimX)) > SNAKEAIMAA2CutOut || abs(get_ival(AimY)) > SNAKEAIMAA2CutOut) {
				combo_stop(SNAKEAIMAA2);
							}
					}
		if(SNAKEAIMAimBooster3)      {
			if (get_val(Fire)> 1){
				combo_run(SNAKEAIMAA3);
							}
			if(abs(get_val(9)) > 19 || abs(get_val(10)) > 19) {
				combo_stop(SNAKEAIMAA3);
							}
					}
		if(SNAKEAIMAimBooster4)      {
			if (get_val(ADS)> 1){
				combo_run(SNAKEAIMAA4);
				SNAKEAIMAA1PF();
							}
			if(abs(get_val(9)) > 36 || abs(get_val(10)) > 36) {
				combo_stop(SNAKEAIMAA4);
							}
					}
		if(SNAKEAIMAimBooster5)      {
			if ((get_ival(ADS)) || (get_ival(Fire) && SNAKEAIMAimBooster5HipFire)) {
				if (abs(get_ival(RX)) <= SNAKEAIMAA5Time && abs(get_ival(RY)) <= SNAKEAIMAA5Time) {
					combo_run(SNAKEAIMAA5);
									}
							}
			else {
				combo_stop(SNAKEAIMAA5);
							}
					}
		if(SNAKEAIMAimBooster6)      {
			if (get_ival(ADS) || (SNAKEAIMAimBooster6HipFire && get_ival(Fire))) {
				combo_run(SNAKEAIMAA6);
							}
			else {
				combo_stop(SNAKEAIMAA6);
							}
					}
		if(SNAKEAIMAimBooster7)      {
			if (get_ival(ADS)){
				combo_run(SNAKEAIMAA7);
				SNAKEAIMAA3PF();
							}
			if (abs(get_ival(AimX)) > SNAKEAIMAA7CutOut || abs(get_ival(AimY)) > SNAKEAIMAA7CutOut) {
				combo_stop(SNAKEAIMAA7);
							}
					}
		if(SNAKEAIMAimBooster8)      {
			if (event_press(ADS)) {
				Power = On;
							}
			else if (!get_ival(ADS) && event_press(Fire)) {
				Power = On;
							}
			if (get_ival(ADS) || get_ival(Fire)) {
				AxisXY = isqrt(abs(get_ival(RX)) * abs(get_ival(RX)) + abs(get_ival(RY)) * abs(get_ival(RY)));
				if (SNAKEAIMAA8Sum == 1)FLIP1111AAF(SNAKEAIMAA8X3,SNAKEAIMAA8OneThird);
							}
			else {
				CountPower = 0;
				power = Off;
							}
					}
		if(SNAKEAIMPolarAssist2)     {
			if (get_ival(PS4_L2) || (get_ival(PS4_R2))) {
				if (FLIP1111AASI <= SNAKEAIMPolarAssist2Range) {
					rnd = random(mn, mx);
					RS_Gradient = (RS_Gradient + SNAKEAIMPolarAssist2Speed) % 360;
					set_polar(POLAR_RS, RS_Gradient, (SNAKEAIMPolarAssist2Range * rnd));
									}
							}
					}
		if(SNAKEAIMPolarAssist3)     {
			if (SNAKEAIMPolarAssist3Timed) {
				if (get_ival(ADS) && get_ptime(ADS) <= SNAKEAIMPolarAssist3Time || SNAKEAIMPolarAssist3HipFire && get_ival(Fire) && get_ptime(Fire) <= SNAKEAIMPolarAssist3Time) {
					FLIP1111AAF2();
									}
							}
			else if (get_ival(ADS)|| SNAKEAIMPolarAssist3HipFire && get_ival(Fire)) {
				FLIP1111AAF2();
							}
			if (!get_ival(ADS) && !get_ival(Fire)) {
				AAT = 0;
				Axis = 0;
							}
					}
		if(SNAKEAIMPolarAssist4)     {
			SNAKEAIMPolarAssist4AX = get_val(RX);
			SNAKEAIMPolarAssist4AY = get_val(RY);
			SNAKEAIMPolarAssist4AM = isqrt(pow(SNAKEAIMPolarAssist4AX, 2) + pow(SNAKEAIMPolarAssist4AY, 2));
			SNAKEAIMPolarAssist4MM = (SNAKEAIMPolarAssist4AM < 100);
			if(!(SNAKEAIMPolarAssist4T++ % SNAKEAIMPolarAssist4Delay)){
				SNAKEAIMPolarAssist4A += SNAKEAIMPolarAssist4Speed;
							}
			SNAKEAIMPolarAssist4A = SNAKEAIMPolarAssist4A % 360;
			SNAKEAIMPolarAssist4SA = SNAKEAIMPolarArray[SNAKEAIMPolarAssist4A % 360];
			SNAKEAIMPolarAssist4CA = SNAKEAIMPolarArray[(SNAKEAIMPolarAssist4A + 270) % 360];
			SNAKEAIMPolarAssist4CA = (SNAKEAIMPolarAssist4CA * SNAKEAIMPolarAssist4Range) / 100;
			SNAKEAIMPolarAssist4SA = (SNAKEAIMPolarAssist4SA * SNAKEAIMPolarAssist4Range) / 100;
			if((SNAKEAIMPolarAssist4ADS && get_val(ADS)) || (SNAKEAIMPolarAssist4HipFire && get_val(Fire))){
				if(SNAKEAIMPolarAssist4AM <= SNAKEAIMPolarAssist4Range){
					SNAKEAIMPolarAssist4SA -= SNAKEAIMPolarAssist4AY;
					SNAKEAIMPolarAssist4CA -= SNAKEAIMPolarAssist4AX;
									}
				else {
					SNAKEAIMPolarAssist4SA = (SNAKEAIMPolarAssist4SA * (200 - ((abs(SNAKEAIMPolarAssist4AY) + SNAKEAIMPolarAssist4AM) / 10) * 10) / 200) * SNAKEAIMPolarAssist4MM;
					SNAKEAIMPolarAssist4CA = (SNAKEAIMPolarAssist4CA * (200 - ((abs(SNAKEAIMPolarAssist4AX) + SNAKEAIMPolarAssist4AM) / 10) * 10) / 200) * SNAKEAIMPolarAssist4MM;
									}
				set_val(RX, clamp(SNAKEAIMPolarAssist4AX + SNAKEAIMPolarAssist4CA, -100, 100));
				set_val(RY, clamp(SNAKEAIMPolarAssist4AY + SNAKEAIMPolarAssist4SA, -100, 100));
							}
					}
		if(SNAKEAIMAssistedTracking) {
			if(get_val(XB1_LT) && get_val(XB1_RT)){
				sensitivity(XB1_RX,43,SNAKEAIMAssistedTrackingPower);
				sensitivity(XB1_RY,43,SNAKEAIMAssistedTrackingPower);
							}
					}
		if(SNAKEAIMBloomReducer)     {
			if (get_val(ADS) && (get_val(Fire))) {
				combo_run(SNAKEAIMBloomReducer);
							}
			if(abs(get_val(XB1_LX)) > 10 || abs(get_val(XB1_LY)) > 10){
				combo_stop(SNAKEAIMBloomReducer);
							}
					}
		if(SNAKEAIMRecoilReducer)    {
			if(get_ival(ADS) && get_ival(Fire) || SNAKEAIMRecoilReducerHipFire && get_ival(Fire)) {
				combo_run(SNAKEAIMRecoilReducer);
							}
					}
		if(SNAKEAIMShapeAssist)      {
			FLIP1111SARX = get_val(RX);
			FLIP1111SARY = get_val(RY);
			FLIP1111SAAM = isqrt(pow(FLIP1111SARX,2) + pow(FLIP1111SARY,2));
			FLIP1111SAMM = (FLIP1111SAAM < 100);
			FLIP1111SAA += SNAKEAIMShapeAssistSpeed + FLIP1111SAA2;
			FLIP1111SAA = FLIP1111SAA % 360;
			FLIP1111SACA = SNAKEAIMPolarArray[(FLIP1111SAA + 270) % 360];
			FLIP1111SASA = SNAKEAIMPolarArray[FLIP1111SAA % 360];
			if (SNAKEAIMShapeAssistBoostToggle) {
				if (get_ival(Fire)) {
					FLIP1111SASR = get_rumble(SNAKEAIMRTUFunction());
					FLIP1111SAA2 = SNAKEAIMShapeAssistSpeedBoost;
					if (FLIP1111SASR > 10 && FLIP1111SASR < 90) {
						FLIP1111SAPS = (SNAKEAIMShapeAssistRange + SNAKEAIMShapeAssistRangeBoost + SNAKEAIMShapeAssistSpeedBoost + FLIP1111SASR / 4);
						FLIP1111SASR   = Off;
											}
									}
							}
			if (get_ival(ADS) || get_ival(Fire)) {
				if (FLIP1111SAAM < SNAKEAIMShapeAssistRange) {
					FLIP1111SASA -= FLIP1111SARY;
					FLIP1111SACA -= FLIP1111SARX;
									}
				else {
					FLIP1111SASA = (FLIP1111SASA * (200 - ((abs(FLIP1111SARY) + FLIP1111SAAM) / 10) * 10) / 200) * FLIP1111SAMM;
					FLIP1111SACA = (FLIP1111SACA * (200 - ((abs(FLIP1111SARX) + FLIP1111SAAM) / 10) * 10) / 200) * FLIP1111SAMM;
									}
				if (!get_rumble(SNAKEAIMRTUFunction())) SNAKEAIMSFunction(SNAKEAIMShapeAssistRange);
				else SNAKEAIMSFunction(FLIP1111SAPS);
							}
			FLIP1111SAPS = On;
			FLIP1111SASR   = On;
					}
		if(SNAKEAIMPickUpMacroToggle && get_val(Pickup)) {
			combo_run(SNAKEAIMPickUpMacro);
					}
		combo_run(SNAKEAIMConstantAimBooster1);
		combo_run(SNAKEAIMConstantAimBooster2);
		combo_run(SNAKEAIMConstantAimBooster3);
		combo_run(SNAKEAIMConstantAimBooster4);
		combo_run(SNAKEAIMRBGFlow);
			}
	}
combo NonActivated {
	SNAKEAIMPickUpMacroToggle = FALSE;
	SNAKEAIMRecoilReducerHipFire = FALSE;
	SNAKEAIMRecoilReducer = FALSE;
	SNAKEAIMAimBooster1 = FALSE;
	SNAKEAIMAimBooster1HipFire = FALSE;
	SNAKEAIMAimBooster2 = FALSE;
	SNAKEAIMAimBooster3 = FALSE;
	SNAKEAIMAimBooster4 = FALSE;
	SNAKEAIMAimBooster5 = FALSE;
	SNAKEAIMAimBooster5HipFire = FALSE;
	SNAKEAIMAimBooster6 = FALSE;
	SNAKEAIMAimBooster6HipFire = FALSE;
	SNAKEAIMAimBooster7 = FALSE;
	SNAKEAIMAimBooster8 = FALSE;
	SNAKEAIMPolarAssist1ADS = FALSE;
	SNAKEAIMPolarAssist1HipFire = FALSE;
	SNAKEAIMPolarAssist2 = FALSE;
	SNAKEAIMPolarAssist3 = FALSE;
	SNAKEAIMPolarAssist3 = FALSE;
	SNAKEAIMPolarAssist3Timed = FALSE;
	SNAKEAIMPolarAssist3HipFire = FALSE;
	SNAKEAIMAssistedTracking = FALSE;
	SNAKEAIMShapeAssistBoostToggle = FALSE;
	SNAKEAIMBloomReducer = FALSE;
	}
combo Activated {
	SNAKEAIMPickUpMacroToggle = FALSE;
	SNAKEAIMRecoilReducerHipFire = FALSE;
	SNAKEAIMRecoilReducer = FALSE;
	SNAKEAIMAimBooster1 = TRUE;
	SNAKEAIMAimBooster1HipFire = TRUE;
	SNAKEAIMAimBooster2 = TRUE;
	SNAKEAIMAimBooster3 = TRUE;
	SNAKEAIMAimBooster4 = TRUE;
	SNAKEAIMAimBooster5 = TRUE;
	SNAKEAIMAimBooster5HipFire = TRUE;
	SNAKEAIMAimBooster6  = TRUE;
	SNAKEAIMAimBooster6HipFire = TRUE;
	SNAKEAIMAimBooster7 = TRUE;
	SNAKEAIMAimBooster8 = TRUE;
	SNAKEAIMPolarAssist1ADS = TRUE;
	SNAKEAIMPolarAssist1HipFire = TRUE;
	SNAKEAIMPolarAssist2 = TRUE;
	SNAKEAIMPolarAssist3 = TRUE;
	SNAKEAIMPolarAssist3 = TRUE;
	SNAKEAIMPolarAssist3Timed = FALSE;
	SNAKEAIMPolarAssist3HipFire = TRUE;
	SNAKEAIMAssistedTracking = TRUE;
	SNAKEAIMShapeAssistBoostToggle = TRUE;
	SNAKEAIMBloomReducer = FALSE;
	}
combo SNAKEAIMConstantAimBooster1  {
	set_val(AimY,y_val(AimY, FLIP1111CAPY));
	wait(SNAKEAIMConstantAADelay);
	set_val(AimX,x_val(AimX, FLIP1111CAPX));
	wait(SNAKEAIMConstantAADelay);
	set_val(AimY,y_val(AimY, FLIP1111CAPY * -1));
	wait(SNAKEAIMConstantAADelay);
	set_val(AimX,x_val(AimX, FLIP1111CAPX * -1));
	wait(SNAKEAIMConstantAADelay);
	}
combo SNAKEAIMConstantAimBooster2  {
	set_val(AimY,y_val(AimY, FLIP1111CAPY2));
	wait(SNAKEAIMConstantAADelay2);
	set_val(AimX,x_val(AimX, FLIP1111CAPX2));
	wait(SNAKEAIMConstantAADelay2);
	set_val(AimY,y_val(AimY, FLIP1111CAPY2 * -1));
	wait(SNAKEAIMConstantAADelay2);
	set_val(AimX,x_val(AimX, FLIP1111CAPX2 * -1));
	wait(SNAKEAIMConstantAADelay2);
	}
combo SNAKEAIMConstantAimBooster3  {
	set_val(AimY,y_val(AimY, FLIP1111CAPY3));
	wait(SNAKEAIMConstantAADelay3);
	set_val(AimX,x_val(AimX, FLIP1111CAPX3));
	wait(SNAKEAIMConstantAADelay3);
	set_val(AimY,y_val(AimY, FLIP1111CAPY3 * -1));
	wait(SNAKEAIMConstantAADelay3);
	set_val(AimX,x_val(AimX, FLIP1111CAPX3 * -1));
	wait(SNAKEAIMConstantAADelay3);
	}
combo SNAKEAIMConstantAimBooster4  {
	set_val(AimY,y_val(AimY, FLIP1111CAPY4));
	wait(SNAKEAIMConstantAADelay4);
	set_val(AimX,x_val(AimX, FLIP1111CAPX4));
	wait(SNAKEAIMConstantAADelay4);
	set_val(AimY,y_val(AimY, FLIP1111CAPY4 * -1));
	wait(SNAKEAIMConstantAADelay4);
	set_val(AimX,x_val(AimX, FLIP1111CAPX4 * -1));
	wait(SNAKEAIMConstantAADelay4);
	}
combo SNAKEAIMAA7 {
	set_val(AimY,SNAKEAIMAA7Power);
	wait(SNAKEAIMAA7Delay);
	set_val(AimX,SNAKEAIMAA7Power);
	wait(SNAKEAIMAA7Delay);
	set_val(AimY,SNAKEAIMAA7Power * -1);
	wait(SNAKEAIMAA7Delay);
	set_val(AimX,SNAKEAIMAA7Power * -1);
	wait(SNAKEAIMAA7Delay);
	}
combo SNAKEAIMAA6 {
	set_val(RY,AxisRelease(RY,SNAKEAIMAA6Power));
	wait(SNAKEAIMAA6Delay);
	set_val(RX,AxisRelease(RX,SNAKEAIMAA6Power));
	wait(SNAKEAIMAA6Delay);
	set_val(RY,AxisRelease(RY,inv(SNAKEAIMAA6Power)));
	wait(SNAKEAIMAA6Delay);
	set_val(RX,AxisRelease(RX,inv(SNAKEAIMAA6Power)));
	wait(SNAKEAIMAA6Delay);
	}
combo SNAKEAIMAA5 {
	SNAKEAIMSetValFunction(RX,SNAKEAIMAimBooster5PowerX);
	wait(SNAKEAIMAA5Delay);
	SNAKEAIMSetValFunction(RY,SNAKEAIMAimBooster5PowerY);
