			if(event_press(PS4_TRIANGLE)){
				Rumble = TRUE;
				STEP = 0;
			}
				
			if(event_press(PS4_CROSS))	{
					Rumble = FALSE;	
				}
			
			}
		if(Block){
			block_btn();
			}
			
		if (CODE){
				List();
				block_btn();
				if(get_controller() == PIO_PS4)
				{
					combo_run(codelockPS);
				}
						else
						{
						combo_run(codelockXB);
						}
		
				}	
		
		
		if(get_val(PS4_L1) && event_press(PS4_UP)){
			gen_idx = cycle(gen_idx + 1,0,1);
		}
		
		if(!gen_idx){
			delay = 0;
		}
			else{
				delay = 40;	
			}
		
		if(get_val(PS4_L2) && event_press(PS4_OPTIONS)){
			cls_oled(0);
			Title = !Title;
			Menu  = !Menu;
			Page1 = TRUE;
			
				if(!Edit){
					ZenUsers = 1;
					User = 1;
					Edit = 1;
				}
			}
	
			if (save >= 10){
				Save();
				save = 0;
			}
		
		if(Check && STEP < 4 && Rumble){
			RumbleCheck();
			
			}
			
				
		
		
		}

	if(Menu){
	
	display_menu();
	block_btn();
	if(Page1){
		
		if(event_release(PS4_CROSS)){
			cls_oled(0);
			Page1 = FALSE;
			Page2 = TRUE;
		
		}
	
	
	}
	
	
	
	if(Page2){
	
		if(event_release(PS4_LEFT)){
			cls_oled(0);
			ZenUsers = cycle(ZenUsers - 1 , 1,15);	
		}
		
		if(event_press(PS4_RIGHT)){
			cls_oled(0);
			ZenUsers = cycle(ZenUsers + 1 , 1,15);	
		}
	
		if(event_press(PS4_CROSS)){
			cls_oled(0);
			Page2 = FALSE;
			Page3 = TRUE;
		
		}
	
	
	}
	
	if(Page3){
	
		if(event_press(PS4_LEFT)){
			cls_oled(0);
			User = cycle(User - 1 , 1,ZenUsers);	
		}
		
		if(event_press(PS4_RIGHT)){
			cls_oled(0);
			User = cycle(User + 1 , 1,ZenUsers);	
		}
	
		if(event_press(PS4_PS)){
			cls_oled(0);
			Page3 = FALSE;
			Menu  = FALSE;
			Title = TRUE;
			code  = (10000 / ZenUsers) * (User - 1); 
			Entered = 0;
			Save();
		}
	
	
	}
	
	}


}


function Digit(f_list){

Digit[0] =  f_list / 1000;
Digit[1] = (f_list - (Digit[0] * 1000)) / 100 ;
Digit[2] = (f_list - (Digit[0] * 1000) - (Digit[1] * 100)) / 10  ;
Digit[3] =  f_list - (Digit[0] * 1000) - (Digit[1] * 100) - (Digit[2] * 10)  ;
}

function cycle(f_val, f_lo, f_hi) {
	if(f_val > f_hi) return f_lo;
	if(f_val < f_lo) return f_hi;
	return f_val;
}

function List(){

		if (code < 1000)
			{
				Digit(List_Zero[code]);
			}
		
		if (code >= 1000 && code < 2000)
			{
				Digit(List_One[code - 1000]);
			}
		
		if (code >= 2000 && code < 3000)
			{
				Digit(List_Two[code - 2000]);
			}
		
		if (code >= 3000 && code < 4000)
			{
				Digit(List_Three[code - 3000]);
			}
		
		if (code >= 4000 && code < 5000)
			{
				Digit(List_Four[code - 4000]);
			}
		
		if(code >= 5000 && code < 6000)
			{
				Digit(List_Five[code - 5000]);
			}
		
		if(code >= 6000 && code < 7000)
			{
				Digit(List_Six[code - 6000]);
			}
		if(code >= 7000 && code < 8000)
			{
				Digit(List_Seven[code - 7000]);
			}
		
		if(code >= 8000 && code < 9000)
			{
				Digit(List_Eight[code - 8000]);
			}
		
		if(code >= 9000 && code < 10000)
			{
				Digit(List_Nine[code - 9000]);
			}


}


