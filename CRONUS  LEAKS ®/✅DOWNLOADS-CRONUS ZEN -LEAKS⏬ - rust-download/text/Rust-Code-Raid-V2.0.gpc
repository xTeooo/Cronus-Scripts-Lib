function number_to_string(f_val,f_digits) {

    i = 1;  c_val = 10000;
	if(f_val < 0) //--neg numbers
	{          
         putc_oled(i,45);    //--add leading "-"
         i += 1;
         f_val = abs(f_val);
	
	} 
	for(c = 5; c >= 1; c--) 
	{
	    if(f_digits >= c) 
	    {
            putc_oled(i,ASCII_NUM[f_val / c_val]);
            f_val = f_val % c_val;
            i +=  1; 
            /*if(c == 4) 
            {
                putc_oled(i,44);   //--add ","
                i += 1;
            }*/
        
        }
        c_val /= 10;
    
    }
    	
    if(Menu){
    	puts_oled(center_x(i - 1,OLED_FONT_MEDIUM_WIDTH),36,OLED_FONT_MEDIUM,i - 1,OLED_WHITE); // adjustable value centered in X - medium size
	}
	
	if(Title){
		puts_oled(center_x(i -10,OLED_FONT_MEDIUM_WIDTH),29,OLED_FONT_SMALL,i - 1,OLED_WHITE); 
	
	}
}




function find_digits(f_num) {

    f_num = abs(f_num);
    if(f_num / 10000 > 0) return 5;
    if(f_num /  1000 > 0) return 4;
    if(f_num /   100 > 0) return 3;
    if(f_num /    10 > 0) return 2;
                          return 1;
} 



function center_x(f_chars,f_font) {

    return (OLED_WIDTH / 2) - ((f_chars * f_font) / 2);
} 

