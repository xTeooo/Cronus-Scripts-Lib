int PolarVIBoost      =  On;
int DamPolarAssist    = Off;
int DamPolarBoost     =  On;
int BattsStickyAA     = Off;
int FallOffAssist     = Off;
int LegacyAssist      = Off;
int LegacyRecoil      =  On;
int OnTheFlyRecoil    =  On;
int RapidFire         = Off;
int SlideCancelCW     = Off;
int SlideCancelMW     =  On;
int BunnyHop          = Off;
int AutoPing          = Off;
int BlockRumble       =  On;

