int ModDigit;
int PolarRadius      = 15;
int PolarAngle       = 20;
int PolarRadiusBoost = 5;
int PolarAngleBoost  = 10;
int PolarRelease     = 20;
int DAMRadius        = 15;
int DAMAngle         = 20;
int DAMRadiusBoost   = 6;
int DAMAngleBoost    = 10;
int BattStickyPower  = 20;
int BattStickyWait   = 20;
