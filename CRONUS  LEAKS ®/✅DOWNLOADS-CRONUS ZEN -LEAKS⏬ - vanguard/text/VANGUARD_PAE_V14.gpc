int i_s, c, c_val;
function n_2_s(f_val, f_digits, f_pos,x) {
	i_s = 1;
	c_val = 10000;
	if (f_val < 0) {
		putc_oled(i_s, 45);
		i_s += 1;
		f_val = abs(f_val);
	}
	for (c = 5; c >= 1; c--) {
		if (f_digits >= c) {
			putc_oled(i_s, A_N[f_val / c_val]);
			f_val = f_val % c_val;
			i_s += 1;
			if (c == 4) {
				putc_oled(i_s, 44);
				i_s += 1;
			}
		}
		c_val /= 10;
	}
	puts_oled(x, f_pos, OLED_FONT_MEDIUM, i_s - 1, OLED_WHITE);
}

