function AntiRecoil (axis, recoil){  
RY = current_y;
RX = current_x;

if (g(FIRE) && (isqrt(RX*RX + RY*RY)) <= abs(recoil))
{
if(abs(RY) <= abs(recoil))
{
s(axis,((recoil * (100 - abs(g(axis)))) / 100 + g(axis)) *invert);
}
}
}
