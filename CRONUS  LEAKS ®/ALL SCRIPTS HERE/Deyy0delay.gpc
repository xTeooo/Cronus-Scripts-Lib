define Build = PS4_RIGHT;

int AA1    			       = TRUE;           
int AA3  				   = TRUE;
int NOADSAA				   = TRUE;
int aaon 				   = FALSE;
int Antirecoil             = TRUE;
int Strafe     			   = FALSE;
int RapidFire	  		   = FALSE;
int DropShot 			   = FALSE;
int BuildTracker           = TRUE;
int AA_Strength  		   = 1; 
int NOADSAA_Strength       = 1; 
int tt_aim_speed           = 100;
int AA4_HOLD       		   = 360;
int AA4_RELEASE   	       = 5;
int AntiRecoil             = 17;
int ast    			       = 74;
int aaValue				   = 8;     
int SPEED 				   = 100;       
int BuildSyncTime          = 0;
int SnapShot_hold; 
int SnapShot_rest;
int Anti_Recoil; 
int interval_t, angle, angle2;
int x_stick, y_stick, cos_angle, sin_angle;
int BuildTrack,BuildCount;
int edit = XB1_RIGHT;
int confirm = XB1_RIGHT;
int pickaxe = XB1_Y;

main{
block_rumble();
if(BuildTracker) {
if(event_press(Build)) {
BuildTrack = !BuildTrack;}
if(BuildTrack) {
BuildCount += get_rtime();
if(event_press(PS4_TRIANGLE) || BuildCount == BuildSyncTime) {
BuildTrack = FALSE;
BuildCount = FALSE;}
if(get_ival(PS4_L1) || get_ival(PS4_L2) || get_ival(PS4_R1) || get_ival(PS4_R2)) { 
BuildCount = FALSE;
}}}  
if(!BuildTrack) {    
if(AA3){
if(get_val(XB1_LT) && get_val(XB1_RT)){
sensitivity(XB1_RX,43,tt_aim_speed); 
sensitivity(XB1_RY,43,tt_aim_speed);}}
if(AA1){   
if(get_val(XB1_LT)> 1){ combo_run(Auto_Aim); 
}
if(abs(get_val(8)) > 8 || abs(get_val(8)) > 10) 
combo_stop(Auto_Aim);}
if(DropShot){ 
if(get_val(XB1_RS) && get_ptime(XB1_RS) > 125){  
combo_run(CROUCH);}}
if(NOADSAA){   
if(get_val(XB1_RT)> 1){ combo_run(Auto_Aim2);}
if(abs(get_val(9)) > 19 || abs(get_val(10)) > 19) 
combo_stop(Auto_Aim2);}
if(Antirecoil){                               
if(get_val(XB1_LT) && get_val(XB1_RT)){      
combo_run(AntiRecoil); }               
if( abs(get_val(PS4_RY)) > 36 || abs(get_val(PS4_RX)) > 36) { 
combo_stop (AntiRecoil); }               }
if(event_release(XB1_LT)){ 
if(get_val(XB1_LT) && event_press(XB1_RIGHT)){
RapidFire =! RapidFire;
if(RapidFire == TRUE){}
if(RapidFire == FALSE){} }
if(RapidFire){
if(get_val(XB1_RT)){combo_run(Rapiid_Fire);}}
if(Strafe){
if(get_val(XB1_LT)> 95 && get_val(XB1_RT)){        
combo_run(Auto_Strafe);}
if(abs(get_val(XB1_LX)) > 40 || abs(get_val(XB1_LY)) > 40){
combo_stop(Auto_Strafe);}}}}}
combo Auto_Aim {          
set_val(10, (AA_Strength)); wait(1)
set_val(9, (AA_Strength)); wait(1)
set_val(10, AA_Strength * -1); wait(1)
set_val(9, AA_Strength * -1); wait(1)}
combo Auto_Aim2 {          
set_val(10, (NOADSAA_Strength)); wait(1)
set_val(9, (NOADSAA_Strength)); wait(1)
set_val(10, NOADSAA_Strength * -1); wait(1)
set_val(9, NOADSAA_Strength * -1); wait(1)}       
combo SnapShot { 
set_val(XB1_LT, 100);    
wait(SnapShot_hold);                
set_val(XB1_LT, 0);      
wait(SnapShot_rest);}
combo AA4 {//.
set_val(XB1_LT, 100);
wait(AA4_HOLD);
set_val(XB1_LT, 0);
wait(AA4_RELEASE);}
combo AntiRecoil { 
if(get_val(XB1_RT)) {
Anti_Recoil = get_val(10) + AntiRecoil;
if(Anti_Recoil > 100) Anti_Recoil = 100;
set_val(10, Anti_Recoil);}}
combo Auto_Strafe {	
set_val(XB1_LX,-30);
wait(ast);
set_val(XB1_LX, 30);
wait(ast);
if(ast < 160){ast = ast + 10;}
else {ast = 100;}}
combo CROUCH {
set_val(XB1_RS,100);
wait(75);
set_val(XB1_RS,0);
wait(50);}
combo Rapiid_Fire {
set_val(XB1_RT, 100); 
set_val(XB1_RT, 0); 
wait(1);}
combo waiit {
wait(500);
Antirecoil             = TRUE;
Strafe				   = FALSE;
RapidFire	  		   = FALSE;
NOADSAA				   = TRUE;
DropShot 			   = TRUE;}
combo off {
Antirecoil             = TRUE;
Strafe     			   = FALSE;
RapidFire	  		   = FALSE;
NOADSAA				   = FALSE;
DropShot 			   = FALSE;
wait(100);}
