$$INSTRUCTIONS$$



						(XBOX)                                                               (PSN)
-Hold X to shoot Greens. It will release off vibration             -Hold Square to shoot Greens. It will release off vibration

-L3 left hand curry slide ( When ball in left hand )               -L3 left hand curry slide ( When ball in left hand )

-R3 right hand curry slide ( When Ball in right hand )             -R3 right hand curry slide ( When Ball in right hand )

- X + DPad Left = Adjust Delay For Slightly Late By 1             - Square + DPad Left = Adjust Delay For Slightly Late By 1

- X + DPad Right = Adjust Delay For Slightly Earlies By 1         - Square + DPad Right = Adjust Delay For Slightly Earlies By 1

- LB + Dpad Left = Adjust Fade Delay For Slightly Late By 1       - L1 + Dpad Left = Adjust Fade Delay For Slightly Late By 1

- LB + Dpad Right = Adjust Delay For Slightly Earlies By 1        - L1 + Dpad Right = Adjust Delay For Slightly Earlies By 1

- X + DPad Up = Reset Delay                                        - Square + DPad Up = Reset Delay

- LB + Dpad Up = Reset Fade Delay                                 - L1 + Dpad Up = Reset Fade Delay



																																										*/


int Release = 21; 
int FadeRelease = 11

main {

	// Release Adjustment -
	if(get_val(PS4_SQUARE) && event_press(PS4_LEFT)) {
		Release--;
		combo_run(ValueChange);
	}
	
	// Release Adjustment +
	if(get_val(PS4_SQUARE) && event_press(PS4_RIGHT)) {
		Release++;
		combo_run(ValueChange);
	}
	
	// Release Reset
	if(get_val(PS4_SQUARE) && event_press(PS4_UP)) {
		Release = 21;
		combo_run(Reset_Values);
	}


// Fade Adjustment -
	if(get_val(PS4_L1) && event_press(PS4_LEFT)) {
		FadeRelease--;
		combo_run(ValueChange);
	}
	
	// Fade Adjustment +
	if(get_val(PS4_L1) && event_press(PS4_RIGHT)) {
		FadeRelease++;
		combo_run(ValueChange);
	}
	
	// Fade Reset
	if(get_val(PS4_L1) && event_press(PS4_UP)) {
		FadeRelease = 11;
		combo_run(Reset_Values);
	}
	
    
    if(get_rumble(RUMBLE_A) || get_rumble(RUMBLE_B)) {
    	combo_run(wait_time); 
    }
    
    if((get_val(XB1_LX)< -50))
	{
		if(get_rumble(RUMBLE_A) || get_rumble(RUMBLE_B)) 
	    {
	    	combo_run(FADE);
	    }
	}
    
    if(event_press(PS4_R3))
	{
		combo_run(RHCurry);
	}
	
	if(event_press(PS4_L3))
	{
		combo_run(LHCurry);
	}
 	
    
}

combo ValueChange{
	set_rumble(RUMBLE_A, 100);
	set_rumble(RUMBLE_B, 100);
	wait(225);
	set_rumble(RUMBLE_A, 0);
	set_rumble(RUMBLE_B, 0):
}

combo Reset_Values{
	set_rumble(RUMBLE_A, 100);
	set_rumble(RUMBLE_B, 100);
	wait(225);
	set_rumble(RUMBLE_A, 0);
	set_rumble(RUMBLE_B, 0):
	wait(300);
	set_rumble(RUMBLE_A, 100);
	set_rumble(RUMBLE_B, 100);
	wait(225);
	reset_rumble();	
}

combo wait_time{
	wait(Release);
	set_val(PS4_L2, 100);
	set_val(PS4_SQUARE, 0):
	wait(100);
	set_val(PS4_L2, 0);
	wait(200):
}

combo FADE
{
	wait(FadeRelease)
	set_val(XB1_LT, 100);
	set_val(XB1_X, 0);
	wait(100);
	set_val(XB1_LT, 0);
}

combo LHCurry
{
	set_val(XB1_RT, 100);
	wait(50);
	set_val(XB1_RT, 100);
	set_val(PS4_RX, 75);
	set_val(PS4_RY, 75);
	wait(70);
	set_val(XB1_RT, 100);
	set_val(PS4_RX, 0);
	set_val(PS4_RY, 0);

}

combo RHCurry{
	
	set_val(XB1_RT, 100);
	wait(50);
	set_val(XB1_RT, 100);
	set_val(PS4_RX, -75);
	set_val(PS4_RY, 75);
	wait(70);
	set_val(XB1_RT, 100);
	set_val(PS4_RX, 0);
	set_val(PS4_RY, 0);
}
























































































































































































































