		{ Jumper = FALSE;
		Moving = TRUE;  
		}
    
    if(get_ival(XB1_X)){
    	set_val(XB1_X,0);
	    if(Jumper) {
	    	combo_run(Green1);
    	}
	    if(Moving) {
	    	combo_run(Green2);
    	}
    }

} 

combo rumble{
    wait(100);
    set_rumble(RUMBLE_A,80);
    wait(100);
    reset_rumble();
}

combo Green1{
	set_val(XB1_X,100);
	wait(JumperTime);
	set_val(XB1_X,0);
	wait(20);
}
combo Green2{
	set_val(XB1_X,100);
	wait(MovingTime);
	set_val(XB1_X,0);
	wait(20);
}
combo QuickStopLeftHand{ 
	set_val(PS4_RX, 100);
	wait(100);
}
combo QuickStopRightHand{ 
	set_val(PS4_RX, -100);
	wait(100);
}

combo Takeover
{
	set_val(PS4_R3, 100);
	wait(100);
}	
 