function Exo_num_8_string(SYN_pressvald,SYN_pressdigitd) {

        i = 1;  c_val = 10000;
     
    	if(SYN_pressvald < 0) //
    	{          
             putc_oled(i,45);    
             i += 1;
             SYN_pressvald = abs(SYN_pressvald);
    	} 
     
    	for(c = 5; c >= 1; c--) 
    	{
    	    if(SYN_pressdigitd >= c) 
    	    {
                putc_oled(i,numConvMethodOL[SYN_pressvald / c_val]);
                SYN_pressvald = SYN_pressvald % c_val;
                i +=  1; 
                if(c == 4) 
                {
                    putc_oled(i,44);  
                    i += 1;
                }
            }
            c_val /= 10;
        }   
        puts_oled(center_x(i - 1,OLED_FONT_LARGE_WIDTH),37,OLED_FONT_LARGE,i - 1,OLED_WHITE); 
}      
function find_digits(f_num) {     
    f_num = abs(f_num);
    if(f_num / 10000 > 0) return 5;
    if(f_num /  1000 > 0) return 4;
    if(f_num /   100 > 0) return 3;
    if(f_num /    10 > 0) return 2;
                          return 1;
}          
function center_x(f_chars,SYN_font_selection_motherfucker) {
    
     
     
        return (OLED_WIDTH / 2) - ((f_chars * SYN_font_selection_motherfucker) / 2);
} 
     
function center_y(f_size) {

 
 
    return (OLED_HEIGHT  / 2) - (f_size / 2);
