function Set_polar(stick, angle, CrypPolarAA2Range){
	x_stick = 8 + stick;
	y_stick = 9 + stick;
	if(angle < 0) angle = 360 + (angle % 360);
	angle = (angle + 90) % 360;
	CrypPolarAA2Range = clamp(CrypPolarAA2Range, 0, 100);
	sin_angle = CrypPolarArray[angle];
	cos_angle = CrypPolarArray[(angle + 90) % 360];
	offset(x_stick, inv(CrypPolarAA2Range * cos_angle / 100));
	offset(y_stick, inv(CrypPolarAA2Range * sin_angle / 100));
	return;
	}
function offset(int axis, int offset_val) {
	set_val(axis, clamp(offset_val * (100 - abs(get_val(axis))) / 100 + get_val(axis), -100, 100));
	return
}
