function CrypPolarV3 (Current,Received) {
	YROutput = Current / 10 + Received / 10;
	if(YROutput <= -3276) return -32768;
	if(YROutput >=  3276) return  32767;
	return Current + Received;
	}
function CrypPolarity (Rotation,Radial) {
	set_polar(PS,Rotation,Radial * 327);
	set_val(PX,CrypPolarV3(get_val(PX) + YRValueX * 327,get_ival(PX)));
	set_val(PY,CrypPolarV3(get_val(PY) + YRValueY * 327,get_ival(PY)));
	}
