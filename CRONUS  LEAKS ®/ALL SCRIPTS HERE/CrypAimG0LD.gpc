function Set_polarA(angle,CrypPolarAA2Range){
	RX = AimX;
	RY = AimY;
	if(angle < 0) angle = 360 + (angle % 360);
	angle = (angle + 90) % 360;
	CrypPolarAA2Range = clamp(CrypPolarAA2Range, 0, 100);
	Sin = CrypPolarArray[angle];
	Cos = CrypPolarArray[(angle + 90) % 360];
	sV(RX,inv(CrypPolarAA2Range * Cos / 100));
	sV(RY,inv(CrypPolarAA2Range * Sin / 100));
	return;
	}
