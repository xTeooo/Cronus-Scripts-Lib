function RumbleToUse() {
	if (get_rumble(RumA) >= get_rumble(RumB)) return RumA;
	if (get_rumble(RumA) < get_rumble(RumB)) return RumB;
	return Off;
	}
function GetShapes(f_var) {
	if (!SpiralPosition1) {
		InnerSpiral -= 1;
		if (InnerSpiral < CrypShapeRangeMin)SpiralPosition1 = On;
			}
	else {
		InnerSpiral += 1;
		if (InnerSpiral > f_var)SpiralPosition1 = Off;
			}
	if (!SpiralPosition2) {
		OuterSpiral -= 1;
		if (OuterSpiral < CrypShapeRangeMin)SpiralPosition2 = On;
			}
	else {
		OuterSpiral += 1;
		if (OuterSpiral > f_var)SpiralPosition2 = Off;
			}
	if (CrypShape == Circular) {
		RYT = (get_val(RY)) + ((SinAngle * f_var))/100;
		RXT = (get_val(RX)) + ((CosAngle * f_var))/100;
			}
	if (CrypShape == TallOval) {
		RYT = (get_val(RY)) + ((SinAngle * f_var))/100;
		RXT = (get_val(RX)) + ((CosAngle * CrypShapeRangeMin))/100;
			}
	if (CrypShape == WideOval) {
		RXT = (get_val(RX)) + ((CosAngle * f_var))/100;
		RYT = (get_val(RY)) + ((SinAngle * CrypShapeRangeMin))/100;
			}
	if (CrypShape == Spiral) {
		RYT = (get_val(RY)) + ((SinAngle * InnerSpiral))/100;
		RXT = (get_val(RX)) + ((CosAngle * OuterSpiral))/100;
			}
	if (CrypShape == Helix) {
		if (PositionT <= 1) {
			RXT = (get_val(RX)) + ((CosAngle * f_var))/100;
					}
		if (PositionT > 1) {
			RYT = (get_val(RY)) + ((SinAngle * f_var))/100;
					}
		PositionT++;
		if (PositionT == 4) PositionT = Off;
			}
	set_val(RX,RXT);
	set_val(RY,RYT);
	}
