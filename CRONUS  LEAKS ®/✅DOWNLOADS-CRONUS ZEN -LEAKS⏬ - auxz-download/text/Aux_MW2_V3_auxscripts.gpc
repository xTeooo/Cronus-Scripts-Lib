//𝗔𝗜𝗠𝗕𝗢𝗧 𝗦𝗘𝗧𝗧𝗜𝗡𝗚𝗦:

int ProAim_MW2 = 19;
int ProAim_MW2neg = -19;

int ProAim_MW2aiming = 23;
int ProAim_MW2hipfire = -23;

int DynamicAim_MW2 = 25;
int DynamicAim_MW2neg = -25;

int Latency = 16;
int Restart = 14;

//𝗔𝗡𝗧𝗜-𝗥𝗘𝗖𝗢𝗜𝗟 𝗫 𝗦𝗘𝗧𝗧𝗜𝗡𝗚𝗦:

int AntiRecoil_Ver = 16;        //Anti Recoil Vertical Strength
int AntiRecoil_Hor = 0;        //Anti Recoil Horizontal Strength
int AntiRecoil_Invert = 1;     //Change 1 to -1 If you play with Inverted
int ARS;

//𝗥𝗔𝗣𝗜𝗗𝗙𝗜𝗥𝗘 𝗦𝗘𝗧𝗧𝗜𝗡𝗚𝗦:
int RapidFire = FALSE;
define SPEED = 20;             //Rounds per Second

//𝗔𝗨𝗧𝗢 𝗛𝗢𝗟𝗗-𝗕𝗥𝗘𝗔𝗧𝗛:
int HoldBreath = TRUE;

//𝗠𝗢𝗩𝗘𝗠𝗘𝗡𝗧 𝗠𝗢𝗗𝗦 𝗦𝗘𝗧𝗧𝗜𝗡𝗚𝗦:
int Dropshot = FALSE;
int AntiHit_drop = 40;         //If dropshotting performs incorrectly - raise these values by +10
int AntiHit_stand = 55;
int Jumpshot = FALSE

/*
✧⋄⋆⋅⋆⋄✧⋄⋆⋅⋆⋄✧⋄⋆⋅⋆⋄✧⋄⋆⋅⋆⋄✧✧⋄⋆⋅⋆⋄✧⋄⋆⋅⋆⋄✧⋄⋆⋅⋆⋄✧⋄⋆⋅⋆⋄✧✧⋄⋆⋅⋆⋄✧⋄⋆⋅⋆⋄✧⋄⋆⋅⋆⋄✧⋄⋆⋅⋆⋄✧
𝗕𝗨𝗧𝗧𝗢𝗡 𝗟𝗔𝗬𝗢𝗨𝗧 
If you have a specific button layout - you can rebind your layout here:
- Don't worry that all the controls are for Xbox, they will still translate onto your PS4 controllers! ;) */

//𝗧𝗥𝗜𝗚𝗚𝗘𝗥𝗦
define FIRE_BTN = XB1_RT;
define ADS_BTN = XB1_LT;

//𝗕𝗨𝗠𝗣𝗘𝗥𝗦
define TACTICAL = XB1_LB;
define LETHAL = XB1_RB;

//𝗦𝗧𝗜𝗖𝗞𝗦
define CROUCH_BTN = XB1_RS;
define SPRINT_BTN = XB1_LS;

//𝗕𝗨𝗧𝗧𝗢𝗡𝗦
define RELOAD_BTN = XB1_X
define JUMP_BTN = XB1_A;
define MELEE_BTN = XB1_B;
define SWAP_BTN = XB1_Y;

//𝗗𝗣𝗔𝗗
define UP = XB1_UP;
define LEFT = XB1_LEFT;
define RIGHT = XB1_RIGHT;
define DOWN = XB1_DOWN;

//𝗜𝗚𝗡𝗢𝗥𝗘 𝗧𝗛𝗘𝗦𝗘
define RX = XB1_RX;
define RY = XB1_RY;
define LX = XB1_LX;
define LY = XB1_LY;
function a_f(p,m) {
if(abs(get_val(p)) < Restart)
return m;
return get_val(p);}

main{

combo_run (ProAim_MW2X);

if(get_val(ADS_BTN)) combo_run(ProAim_MW2S);

if(get_val(FIRE_BTN)) combo_run(ProAim_MW2Dynamic);

if(get_val(ADS_BTN) && get_val(FIRE_BTN)) combo_run(ProAim_MW2Dynamic);

if (RapidFire)
if (get_val(FIRE_BTN)) {
combo_run(RAPID_FIRE);
}

if (HoldBreath)
if((get_val(ADS_BTN)) >= 95) {
set_val(SPRINT_BTN, 100);
}
        
if(get_val(ADS_BTN) && event_press(RIGHT)){
Dropshot =! Dropshot;}
if(Dropshot){
if(get_val(ADS_BTN)&&get_val(FIRE_BTN)){combo_run(Dropshot);
}}
        
if (Jumpshot)  
if(get_val(FIRE_BTN) > 99){
combo_run(JUMPSHOT);
}

if(get_val(ADS_BTN) && get_val(FIRE_BTN))
    combo_run(AntiRecoil_Ver);
if(abs(get_val(RY)) > AntiRecoil_Ver + 2 || abs(get_val(RX)) > AntiRecoil_Ver + 2) {
    combo_stop(AntiRecoil_Ver);}
    
    }
    
combo ProAim_MW2X { 
    set_val(10,a_f(10,ProAim_MW2));
    wait(Latency)
    set_val(9,a_f(9,ProAim_MW2));
    wait(Latency)
    set_val(10,a_f(10,ProAim_MW2neg));
    wait(Latency)
    set_val(9,a_f(9,ProAim_MW2neg));
    wait(Latency)}
    
combo ProAim_MW2S { 
    set_val(10,a_f(10,ProAim_MW2aiming));
    wait(Latency)
    set_val(9,a_f(9,ProAim_MW2aiming));
    wait(Latency)
    set_val(10,a_f(10,ProAim_MW2hipfire));
    wait(Latency)
    set_val(9,a_f(9,ProAim_MW2hipfire));
    wait(Latency)}
    
combo ProAim_MW2Dynamic { 
    set_val(10,a_f(10,DynamicAim_MW2));
    wait(Latency)
    set_val(9,a_f(9,DynamicAim_MW2));
    wait(Latency)
    set_val(10,a_f(10,DynamicAim_MW2neg));
    wait(Latency)
    set_val(9,a_f(9,DynamicAim_MW2neg));
    wait(Latency)}
    
combo AntiRecoil_Ver {
    ARS = get_val(10) + AntiRecoil_Ver;
    if(ARS > 100) ARS = 100;
    set_val(10,ARS * AntiRecoil_Invert);
    ARS = get_val(9) + AntiRecoil_Hor;
    if(ARS > 100) ARS = 100;
    set_val(9,ARS);}

combo RAPID_FIRE {
	set_val(XB1_RT,100);
	wait(SPEED);
	set_val(XB1_RT,0);
	wait(SPEED);}
	
combo Dropshot {
    set_val(CROUCH_BTN,100);
    wait(AntiHit_drop);
    wait(AntiHit_stand);
    set_val(CROUCH_BTN,100);}
    
combo JUMPSHOT {
    set_val(JUMP_BTN, 100);
    wait(100); 
    wait(300);}