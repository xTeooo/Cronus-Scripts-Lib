}
    if  (get_val(BOOST_BUTTON) && !combo_running(TIMER_FOR_DODGE)&& !combo_running(TIMER_AFTER_DODGE) && !combo_running(DRIFT_AFTER_JUMP) && !combo_running(DRIFT_AFTER_DODGE)){
