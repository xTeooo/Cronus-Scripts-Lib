		}       
	    if (combo_running(TIMER_FOR_DODGE) || combo_running(TIMER_AFTER_DODGE) || !combo_running(TIMER_FOR_DODGE) && !combo_running(AUTO_TAKEOFF) && !combo_running(TIMER_AFTER_DODGE) && !get_val(BOOST_BUTTON)){
	    	combo_stop(BOOST_SAFER);
		}       
    }

