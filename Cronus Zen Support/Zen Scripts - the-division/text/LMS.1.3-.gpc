function pick_load(choice){
	if(choice==0 ) pik=get_pvar(SPVAR_49,0,3,0);          //prifile
	if(choice==1 ) pik=0                                  // sens
	if(choice==2 ) pik=get_pvar(SPVAR_54,0,5,0)           //strf
	if(choice==3 ) pik=get_pvar(SPVAR_52,40,100,90)       // angle
	if(choice==4 ) pik=get_pvar(SPVAR_57,0,3,0);          //recoil
	if(choice==5 ) pik=get_pvar(SPVAR_62,0,1,0);          // sens boost
	if(choice==6 ) pik=get_pvar(SPVAR_55,0,1,0)           //aim assist
	if(choice==7 ) pik=get_pvar(SPVAR_50,0,1,0);          // swap
	if(choice==8 ) pik=get_pvar(SPVAR_53,50,100,50)       //rapid
	//sticks checer == 9
	
	step=Step_Min_Max_Options[choice][0]
	min=Step_Min_Max_Options[choice][1]
	max=Step_Min_Max_Options[choice][2]
}
function pik_save(){
	if(choice==0 ){
		profile=pik
		load_recoil()
		unpacking()
		}
	if(choice==1) {
		if(x_ads) sens_scope_rx=sens_pik
		if(y_ads) sens_scope_ry=sens_pik
		if(x_cam) sens_cam_rx=sens_pik
		if(y_cam) sens_cam_ry=sens_pik
	}
	if(choice==2 )run_fire=pik
	if(choice==3 )angle=pik
	if(choice==4 )recoil=pik
	if(choice==5 )sens_boost=pik
	if(choice==6 )aim_assist=pik                                                                  //
	if(choice==7 )button_swap=pik
	if(choice==8 )hold_time=pik
	// sticks cheker
	
}
function choice(choice){
	rect_oled(0, 19, 128, 63, 1, 1);
	rect_oled(12, 21, 104, 31,0, 0);
	printf( 7, 55 , 0, 0, save[0])
	if(pik>=max)pik=max
	if(pik<=min)pik=min
	if(pik>min)printf( 1, 28 , 1, press_left, arrows[0])
	if(pik<max)printf( 117, 28 , 1, press_right, arrows[1])
	if(choice==0 )printf( 55, 28 , 1, 0, num[pik + 101])                  //prifile
	if(choice==1 )printf(15, 28 , 1, 0, sensetivity[pik])                 //sens
	if(choice==2 )printf( 15, 28 , 1, 0, runplus[pik])	                  //strf
	if(choice==3 )
	{
		if(pik==40) printf( 15, 28 , 1, 0, recoils[0])
		else printf( 55, 28 , 1, 0, num[100+pik])  
	}//angle
	if(choice==4 )printf(15, 28 , 1, 0, recoils[pik])                     //antirecoil
	if(choice==5) printf( 52, 28 , 1, 0, yes_no[pik]);                    //sens boost
	if(choice==6) printf( 52, 28 , 1, 0, yes_no[pik]);                    //aim assist
	if(choice==7) printf( 52, 28 , 1, 0, yes_no[pik]);                    //swap
	if(choice==8) printf( 55, 28 , 1, 0, num[100+pik])                    //rapid
	 // choise=9 sticks cheker
	
	if(event_press(PS4_CIRCLE))choice_settings=FALSE
	if(event_press(PS4_LEFT))
	{
			pik-=step
			combo_run(press_button_left)
	}
	if(event_press(PS4_RIGHT))
	{
			pik+=step
			combo_run(press_button_right)
	}
	if(event_press(PS4_TRIANGLE) ){
			if(choice==1 ) 
			{
				sens_settings=TRUE
				x_cam=FALSE
				y_cam=FALSE
				x_ads=FALSE
				y_ads=FALSE
			}
			else
			{
			choice_settings=FALSE
			pik_save()
			combo_run(success)	
			}
		}
}
function settings(){
	
	block_all_input()
	cls_oled(0);
	led_white();
	rect_oled(0, 0, 128, 19, 0, 1);
	rect_oled(0, 0, 128, 64, 0, 1);
	printf( 10, 6 , 0, 1, settings_str[choice]);
	printf( 10, 25 , 1, 1, settings_down1_str[choice]);
	printf( 5, 45 , 1, 1, settings_down2_str[choice]);
	if(choice<=0)choice=0
	if(choice>=9)choice=9
	if(!choice_settings)
	{
		if(choice<9)printf( 117, 1 , 1, press_left, arrows[1])
		if(choice>0)printf( 1, 1 , 1, press_right, arrows[0])
		if(event_press(PS4_LEFT))
		{
			choice-=1
			combo_run(press_button_right)
		}
		if(event_press(PS4_RIGHT))
		{
			choice+=1
			combo_run(press_button_left)
		}
		if(event_press(PS4_CIRCLE))
		{
			settings=FALSE
			combo_run(screen_cleaner)
		}
		if(event_press(PS4_CROSS) ){                                                                                                      
			if(choice==9 ) 
			{
				sticks_checker=TRUE
				circle=FALSE
			}
			else
			{
				choice_settings=TRUE
				pick_load(choice)
			}
		}
	}
	if(choice_settings) choice(choice)
}
