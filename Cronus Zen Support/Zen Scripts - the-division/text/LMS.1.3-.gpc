function smart_sens_anti_recoil()                            
{
	x1=x+get_val(PS4_RX)
	y1=y+get_val(PS4_RY)
	if (x1 < -100) x1= -100
	if (x1 > 100) x1= 100
	if(get_val(PS4_RY)<0 && get_val(PS4_RY)> inv(y)) y1=y+get_val(PS4_RY)*2
	if(get_val(PS4_RY)<= inv(y)) y1=get_val(PS4_RY)
	if(get_val(PS4_RY)>0 && get_val(PS4_RY)< 2*y) y1=y+get_val(PS4_RY)/2
	if(get_val(PS4_RY)>= 2*y) y1=get_val(PS4_RY)
	set_val(PS4_RX,x1)
	set_val(PS4_RY,y1)
}
function casual_anti_recoil()                                
{
	x1=x
	y1=y
	if (x1 < -100) x1= -100
	if (x1 > 100) x1= 100
	if (y1 < -100) y1= -100
	if (y1 > 100) y1= 100
	if(abs(get_val(PS4_RY))> abs(y)) y1=get_val(PS4_RY)
	if(abs(get_val(PS4_RX))> abs(x)) x1=get_val(PS4_RX)
	set_val(PS4_RX,x1)
	set_val(PS4_RY,y1)
}
function center_shift_anti_recoil()                                
{
	x1=x+get_val(PS4_RX)
	y1=y+get_val(PS4_RY)
	if (x1 < -100) x1= -100
	if (x1 > 100) x1= 100
	if(get_val(PS4_RY)<=  -80) y1=get_val(PS4_RY)
	if(get_val(PS4_RX)>90) x1=100
	if(get_val(PS4_RX)<-90) x1=-100
	set_val(PS4_RX,x1)
	set_val(PS4_RY,y1)
}
function progressive_anti_recoil()                                  
{
	sensitivity(PS4_RY, 50, 100+ y )
	sensitivity(PS4_RX, 50, 100+ x )
}

