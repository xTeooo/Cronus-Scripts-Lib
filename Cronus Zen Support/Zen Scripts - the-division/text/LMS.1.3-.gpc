		{
			killer=FALSE;
			combo_run(screen_cleaner);
		}
	}
	
	if ((pow(get_val(PS4_LX),2) + pow(get_val(PS4_LY),2)<900) && (pow(get_val(PS4_RX),2) + pow(get_val(PS4_RY),2)<900) && !get_val(PS4_CROSS) && ads_time>25){    
		combo_run(anti_recoil)
	}
	else combo_stop(anti_recoil)
