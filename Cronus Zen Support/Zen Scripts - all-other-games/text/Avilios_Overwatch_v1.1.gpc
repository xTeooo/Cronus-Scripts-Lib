function Center_X(f_chars,f_font){  //--  center_x(number of chars,font size);                                                 
return (OLED_WIDTH / 2) - ((f_chars * f_font) / 2); //-- return X for Centering String Horizontally
}
int q, c_Val,c;
function NumberToString(Value, f_Digits, X) {
    q     =     1;
    c_Val = 10000;

    if(Value < 0) //--neg numbers
    {          
        putc_oled(q, 45);    //--add leading "-"
        q += 1;
        Value = abs(Value);
    } 

    for(c = 5; c >= 1; c--) 
    {
        if(f_Digits >= c) 
        {
            putc_oled(q,ASCII_NUM[Value / c_Val]);
            Value = Value % c_Val;
            q +=  1; 

            if(c == 4) 
            {
                putc_oled(q, 44);   //--add ","
                q += 1;
            }
        }
            c_Val /= 10;
    }
        puts_oled(Center_X(q - 7, OLED_FONT_LARGE_WIDTH), X, OLED_FONT_SMALL, q - 1, OLED_WHITE); // adjustable value centered in X and Y - medium size
}
