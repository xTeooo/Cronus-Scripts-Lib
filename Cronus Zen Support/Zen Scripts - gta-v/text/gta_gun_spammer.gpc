    set_val(PS4_RX, -85)
    set_val(PS4_RY, -85)
    wait(45)
    set_val(PS4_RX, 0)
    set_val(PS4_RY, 0)
    set_val(PS4_L1, 0)
    wait(75)
    
    //switch back to Pistol
    set_val(PS4_L1, 100)
    wait(80)
    set_val(PS4_L1, 100)
    set_val(PS4_RY, -100)
    wait(45)
    set_val(PS4_L1, 0)
    wait(100)
}


combo SNIPER

{   
    ///Wait for shot
    wait(175)
    
    //switch to grenade
    set_val(PS4_L1, 100)
    wait(100)
    set_val(PS4_L1, 100)
    set_val(PS4_RX, -85)
    set_val(PS4_RY, -85)
    wait(45)
    set_val(PS4_RX, 0)
    set_val(PS4_RY, 0)
    set_val(PS4_L1, 0)
    wait(75)
    
    //switch back to SNIPER
    set_val(PS4_L1, 100)
    wait(80)
    set_val(PS4_L1, 100)
    set_val(PS4_RX, 85)
    set_val(PS4_RY, 85)
    wait(45)
    set_val(PS4_L1, 0)
    wait(100)
    }
    
    
    combo Shotgun

{   
    ///Wait for shot
    wait(175)
    
    //switch to grenade
    set_val(PS4_L1, 100)
    wait(100)
    set_val(PS4_L1, 100)
    set_val(PS4_RX, -85)
    set_val(PS4_RY, -85)
