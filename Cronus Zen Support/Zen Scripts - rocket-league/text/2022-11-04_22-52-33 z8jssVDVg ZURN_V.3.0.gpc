int Combo,Combo1,Combo2,Combo3,Combo4,Combo5,Combo6,Combo7,Combo8,Combo9,Combo10,Combo11,Combo12,Combo13;
int Once,Once1,Once2,Once3,Once4,Once5,Once6,Once7,Once8,Once9,Once10,Once11,Once12,Once13;

function Combos(Varvar,Var1,BtnOff,BtnGet1,BtnGet2,/**/Var2,VarVal,BtnGet3,BtnGet4,/**/ComboA,Active){

	if (Active == ComboA) {
	    
	    if (Varvar == Var1 && get_val(BtnGet1) && BtnGet2 == OFF && !get_val(BtnOff) || Varvar == Var1 && get_val(BtnGet1) && get_val(BtnGet2) && !get_val(BtnOff) || Var2 == VarVal && (get_val(BtnGet3) && BtnGet4 == 300 || get_val(BtnGet3) && get_val(BtnGet4))) { 
	    
	    	
	    	if (event_press(BtnGet1) || event_press(BtnGet3)) Once[ComboA] = 1;
	    	
	    		Combo[ComboA] = 1;
	    		set_val(PRIMAR_POWER_BUTTON_1, 0);
				set_val(PRIMAR_POWER_BUTTON_2, 0);
				set_val(PRIMAR_POWER_BUTTON_3, 0);
				set_val(PRIMAR_POWER_BUTTON_4, 0);
				set_val(PRIMAR_POWER_BUTTON_5, 0);
				set_val(PRIMAR_POWER_BUTTON_6, 0);
				set_val(PRIMAR_POWER_BUTTON_7, 0);
				set_val(PRIMAR_POWER_BUTTON_8, 0);
	} else if (Varvar == Var1 && !get_val(BtnGet1) && BtnGet2 == OFF && !get_val(BtnOff) || Varvar == Var1 && !get_val(BtnGet1) && !get_val(BtnGet2) && !get_val(BtnOff) || Var2 == VarVal && !get_val(BtnGet3) && !get_val(BtnGet4)) {
	    	
	    	
	    	Combo[ComboA] = 0;
	}	
	if (event_release(BtnGet1) || event_release(BtnGet3))  Once[ComboA] = 0;
	}
}
function Adjust(Val) {

	if (event_press(XB1_UP)) {
		Val += 1;
		if (Val > 1)Val = 0;
		build = 1;
	}
	if (event_press(XB1_DOWN)) {
		Val -= 1;
		if (Val < 0)Val = 1;
		build = 1;
	}
	return Val;
}
function ValAdjust(Vals,Min,Max,Men,ValT) {
		
		if(event_press(PS4_LEFT)) {
		ValT = 0;
		Vals = clamp(Vals - 1,Min,Max);
		Men = 1;
	
	}
		if( event_press(PS4_RIGHT)) {
		ValT = 0;
		Vals = clamp(Vals + 1,Min,Max);
		Men = 1;
	
	}
		if(event_press(PS4_LEFT) && get_ival(PS4_R2)) {
		ValT = 0;
		Vals = clamp(Vals - 9,Min,Max);
		Men = 1;
	
	}
		if( event_press(PS4_RIGHT) && get_ival(PS4_R2)) {
		ValT = 0;
		Vals = clamp(Vals + 9,Min,Max);
		Men = 1;
	
	}
	return Vals;
}	
