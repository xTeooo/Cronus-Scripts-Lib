			AdjustRecoilValue(2);
			BlockAllButtons();
		}
		if(ButtonPress(UP_BUTTON)){
			AdjustRecoilValue(3);
			BlockAllButtons();
		}
		if(ButtonPress(DOWN_BUTTON)){
			AdjustRecoilValue(4);	
			
		}
		if(ButtonPress(BACK_BUTTON)){
