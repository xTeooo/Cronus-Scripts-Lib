int JumperTime = 525;		
int MovingTime = 750;
int LowStaminaTime = 650

int Jumper;
int Moving;

init{

	JumperTime  = get_pvar(SPVAR_1, 300, 1000, JumperTime);
	MovingTime = get_pvar(SPVAR_2, 300, 1000, MovingTime);
	MovingTime = get_pvar(SPVAR_3, 300, 1000, LowStaminaTime);
	
}
main {
	
	set_val(TRACE_1,JumperTime);
	set_val(TRACE_2,MovingTime);
	set_val(TRACE_3,LowStaminaTime);
	
	if(event_press(XB1_RB))
		{   
			combo_run(LowStamina);
		}
	
	if(get_val(XB1_X)){
		if(event_press(XB1_RIGHT)){
			JumperTime += 1; 
			combo_run(rumble);
		}
		if(event_press(XB1_LEFT)){
			JumperTime -= 1; 
			combo_run(rumble);
		}
		if(event_press(XB1_UP)){
			JumperTime += 5; 
			combo_run(rumble);
		}
		if(event_press(XB1_DOWN)){
			JumperTime -= 5; 
			combo_run(rumble);
		}
	}
	
	if(get_val(XB1_RT)){
		if(event_press(XB1_RIGHT)){
			MovingTime += 1; 
			combo_run(rumble);
		}
		if(event_press(XB1_LEFT)){
			MovingTime -= 1; 
			combo_run(rumble);
		}
		if(event_press(XB1_UP)){
			MovingTime += 5; 
			combo_run(rumble);
		}
		if(event_press(XB1_DOWN)){
			MovingTime -= 5; 
			combo_run(rumble);
		}
	}
	
	if(get_val(XB1_RB)){
		if(event_press(XB1_RIGHT)){
			MovingTime += 1; 
			combo_run(rumble);
		}
		if(event_press(XB1_LEFT)){
			MovingTime -= 1; 
			combo_run(rumble);
		}
		if(event_press(XB1_UP)){
			MovingTime += 5; 
			combo_run(rumble);
		}
		if(event_press(XB1_DOWN)){
			MovingTime -= 5; 
			combo_run(rumble);
		}
	}
	
	if(get_val(XB1_MENU)){
		if(event_press(XB1_UP)){
