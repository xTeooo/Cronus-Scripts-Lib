			JumperTime += 150;
			LowStaminaTime += 150;
			combo_run(rumble);
		}
		if(event_press(XB1_DOWN)){
			MovingTime -= 150;
			JumperTime -= 150;
			LowStaminaTime -= 150;
			combo_run(rumble);
		}
		
	}
    
   if(event_press(PS4_L3))
