	define MAGNITUDE 		  = 85; 				  

	define QUICK_TOGGLES	  = TRUE;       
	define KILL_SWITCH_BUTTON = PS4_OPTIONS; 
	define RAPID_FIRE_BUTTON  = PS4_LEFT;	 
	define DROP_SHOT_BUTTON   = PS4_RIGHT;	 	
	int Slide_Cancel_Mode	  =  1; 
	define MW_Slide_Delay	  = 80;	
	define THRESHOLD          = 85; 

	int	Rumble_Block		  = TRUE;
	int Hair_Triggers		  = TRUE;
	int rapid_fire_toggle     = TRUE;
	define rapid_fire_speed   =    5;  
	int DropShot              = FALSE; 
	int DropShotSpeed         =    35; 
	define Hold_Breath		  = TRUE;


	int ADS;
	int FIRE;
    int q;
    int double__tap[30];
  	int KS;
	int wait_time_correction;
	int update_timer;
	int C_RX;
	int C_RY;
	int C_LX;
	int C_LY;
