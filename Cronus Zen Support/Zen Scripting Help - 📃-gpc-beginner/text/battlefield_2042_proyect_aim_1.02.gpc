		 		if(get_ival(Fire))
		 			combo_run(RAPIDFIRE);
		 		else
		    		combo_stop(RAPIDFIRE);
			    
			    LedBlink(150,300);   
			}
		    if(ToggleStrafeShot[ProfileIdx] == 1) 
		    {
