		    if(event_press(PS4_UP)) 
		    {
		    	f_index = cycle(f_index - 1, f_min , f_max);
		    	DisplayMenu = TRUE;	
		    }
		    if(event_press(PS4_DOWN)) 
		    {
		        f_index = cycle(f_index + 1, f_min , f_max);
		        DisplayMenu = TRUE;	
		    } 
		}
		if(!ModOrEditMenu[0])
		{
			if(event_press(PS4_RIGHT))
			{  
				f_index = cycle(f_index + 1,f_min,f_max); 
