						AntirecoilStrength = VerticalStrength + SmartRumble / 2;
					} 	
					if(SmartRumble < 10) 
					{
						SetVal(PS4_RY,VerticalStrength);
						SetVal(PS4_RX,HorizontalStrength);
					}
					else
					{ 	
						SetVal(PS4_RY,AntirecoilStrength);
						SetVal(PS4_RX,HorizontalStrength);	
					}
