 combo AutoBreath 
{
	wait(200);// adjust this timing to when the aim cross apear
    set_val(HOLD_BREATH,100);
    wait(4000);
}
 
 combo DROP_SHOT{   
 	set_val(PRONE_BTN, 100);
	wait(800);
	set_val(PRONE_BTN, 0);
	} 
 
    combo LT_C {
       set_val(RY, negative_value_lt_first);          
       wait(delay)
       set_val(RX, positive_value_lt_first); 
       set_val(RY, positive_value_lt_second); 
       wait(delay) 
       set_val(RY, positive_value_lt_second); 
       wait(delay)                  
       set_val(RX, negative_value_lt_first);
       set_val(RY, positive_value_lt_second);
       wait(delay)
    }
 
    combo RT_C {
       set_val(RY, negative_value_rt_first);              
       wait(delay)
       set_val(RX, positive_value_rt_first);
       set_val(RY, positive_value_rt_second);
       wait(delay) 
       set_val(RY, positive_value_rt_second);
       wait(delay)                  
       set_val(RX, negative_value_rt_first);
       set_val(RY, positive_value_rt_second);
       wait(delay)
    }
    
    combo aimasistcombo {
	set_val(10,xy_val(10,aa_p));
	wait(aa_delay);
	set_val(9,xy_val(9,aa_p));
	set_val(11,xy_val(11,aa_p));
	wait(aa_delay);
	set_val(10,xy_val(10,aa_n));
	wait(aa_delay);
	set_val(9,xy_val(9,aa_n));
	set_val(11,xy_val(11,aa_n));
	wait(aa_delay);
}

function xy_val(f_axis,f_val) {
	if(abs(get_val(f_axis)) < release)
	return f_val;
	return get_val(f_axis);
}


