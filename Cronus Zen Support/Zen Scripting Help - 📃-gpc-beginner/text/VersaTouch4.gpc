	RS    = POLAR_RS,		  Right = PS4_RIGHT,     F2X = PS4_FINGER2X,    	F2  = PS4_FINGER2,		  	 DZ    = DZ_CIRCLE,	  		RY    = PS4_RY;	
	
	
	
int TR_TPAD,TL_TPAD,BL_TPAD,BR_TPAD;			

int VersaTouch4 = On;

main {

	set_val(TRACE_1,TL_TPAD);	set_val(TRACE_2,TR_TPAD);	set_val(TRACE_3,BL_TPAD);	set_val(TRACE_4,BR_TPAD);	set_val(TRACE_5,F2T);	set_val(TRACE_6,VersaTouch4);


	if(VersaTouch4)
	{
		VersaTouch4 ()
;		
		if(TL_TPAD == 1)
		{	
		combo_run(TopRIGHT);	
		}
		
		if(TR_TPAD == 1)
		{	
		combo_run(TopLEFT);		
		}
       	
		if(BL_TPAD == 1)
		{	
		combo_run(BottomLEFT);
		}
		
		if(BR_TPAD == 1)
		{	
		combo_run(BottomRIGHT);
		}
	}




}//	âŒ« ï¼¥ï¼®ï¼¤ ï¼¯ï¼¦ ï¼­ï¼¡ï¼©ï¼®
combo TopRIGHT {

	set_val(PS4_L1,100);
	wait(100);
	TL_TPAD = 0;
	
}

combo TopLEFT {

	set_val(PS4_R1,100);
	wait(100);
	TL_TPAD = 0;
	
}

combo BottomLEFT {

	set_val(PS4_R3,100);
	wait(100);
	BL_TPAD = 0;
	
}
combo BottomRIGHT {

	set_val(PS4_L3,100);
	wait(100);
	BR_TPAD = 0;
	
}


function VersaTouch4 () 
{

	if(ps4_touchpad(PS4T_P1) && !ps4_touchpad(PS4T_P2) && event_press(TPAD))
	{ 
	
		if(get_lval(F1X) >  0 && get_lval(F1Y) < 0 && get_lval(F1X) < 110 && get_lval(F1Y) > -110 )		// ï¼´ï¼¯ï¼° ï¼²ï¼©ï¼§ï¼¨ï¼´  
    	{
    	TR_TPAD = 1; 						
    	} 	
 		else
		if(get_lval(F1X) < -0 &&  get_lval(F1Y) < -0  &&  get_lval(F1Y) > -110 )						// ï¼´ï¼¯ï¼° ï¼¬ï¼¥ï¼¦ï¼´
    	{  
    	TL_TPAD = 1; 						
    	} 	 		
		else
		if (get_lval(F1Y) < 100 && get_lval(F1Y) > 1 && get_lval(F1X) > -100 && get_lval(F1X) < -1)	    // ï¼¢ï¼¯ï¼´ï¼´ï¼¯ï¼­ ï¼¬ï¼¥ï¼¦ï¼´ 		
    	{
    	BL_TPAD = 1;						
    	}	
  		else 
    	if (get_lval(F1Y) < 130 && get_lval(F1Y) > 1 && get_lval(F1X) > 1 && get_lval(F1X) < 100)		// ï¼¢ï¼¯ï¼´ï¼´ï¼¯ï¼­ ï¼²ï¼©ï¼§ï¼¨ï¼´
		{	
		BR_TPAD = 1; 															
    	}
    		
	}

     				
}


























































