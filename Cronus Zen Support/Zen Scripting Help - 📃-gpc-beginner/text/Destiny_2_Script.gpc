 -----------------------------------------------------------------------------------------------------------------
 === The Define Section ===
 -----------------------------------------------------------------------------------------------------------------
 */
 
    define PS = PS4_PS; 			
    define SHARE = PS4_SHARE; 		
    define OPTIONS = PS4_OPTIONS; 	
    define R1 = PS4_R1; 			
    define R2 = PS4_R2; 			
    define R3 = PS4_R3; 			
    define L1 = PS4_L1; 			
    define L2 = PS4_L2; 			
    define L3 = PS4_L3; 			
    define RX = PS4_RX; 			
    define RY = PS4_RY; 			
    define LX = PS4_LX; 			
    define LY = PS4_LY; 			
    define UP = PS4_UP; 			
    define DOWN = PS4_DOWN;		
    define LEFT = PS4_LEFT;			
    define RIGHT = PS4_RIGHT;		
    define TRIANGLE = PS4_TRIANGLE; 
    define CIRCLE = PS4_CIRCLE; 	
    define CROSS = PS4_CROSS; 		
    define SQUARE = PS4_SQUARE 		
    define TOUCHPAD = PS4_TOUCH;
    
	        // i use 7 sens in destiny, i suggest 9 sens reduction is perfect speed with smooth hip fire control.
    define sen_reduc = 0; // ADS SENS CONFIG. (HIGHER THE NUMBER THE MORE REDUCTION THE LOWER THE NUMBER THE LESS REDUCTION)
    
    define WALK  = 0; // Auto run code
    define SPRINT  = 1; // Auto run code
    define E_Slide_Delay = 80; // Slide code
    define A_Slide_Delay = 80; // Slide code
	
	define ON = TRUE; // 1
    define OFF = FALSE; //0
    
 /*
 -----------------------------------------------------------------------------------------------------------------
 === The Int Section ===
 -----------------------------------------------------------------------------------------------------------------
 */
    
    int USE_SLIDE_CANCEL = FALSE; // Slide Code
    int slide_enable = FALSE; // Slide Code
    int state; // Slide Code
    int USE_AUTO_RUN = FALSE; // Auto Run Code, (FALSE if you dont want it TRUE if you do.)
    
    int AR = 0; // Start Value of Vertical Recoil.
    int AR_H = 0; // Start Value of Horizontal Recoil.
    int AR_I = 1; // Change 1 to -1 If you play with Inverted.
    int ARS,AR_ONOFF,ARH_ONOFF; // Anti Recoil combo & Toggle.
    
	int turbo_crouch = FALSE; // Tea bag aim crouch spam (FALSE if you dont want it TRUE if you do.) can still be toggled with button combo
	
	int aa = 25; // aim assist, to much shake suck's.
    int aa_delay = 20; // aim assist, increase if laggy
	int AA_L2 = FALSE; // SWITCH TO TRUE IF YOU WANT AIM ASSIST L2 ON BY DEFAULT.
	int AA_R2 = FALSE; // SWITCH TO TRUE IF YOU WANT AIM ASSIST R2 ON BY DEFAULT.
    
	int RATE_OF_FIRE = 8; // RANGE 0-25 RPS (ROUNDS PER SECOND).
	int rapid_onoff = FALSE; // SWITCH TO TRUE IF YOU WANT RAPIDFIRE ON BY DEFAULT.
	int hold_time; // Part of RapidFire Code.
    int rest_time; // Part of RapidFire Code.
	
	int easyfire = FALSE; // Part of Easy Fire.
	int auto_reload = FALSE; // Part of Auto Reload.
	
	int timer; // Part of Aim Code.
    int master_onoff = FALSE; // Part of Aim Code.
    
    int melee_time = 100;  // SWITCH VALUE TO 500 IF USING WARD OF DAWN TITAN BUBBLE.
    int rapid_melee = FALSE; // Toggle Rapid Melee on by default or not.
	
	// right here for picture code if you want it.
	
 /*
 -----------------------------------------------------------------------------------------------------------------
 === The Coding Section ===
 -----------------------------------------------------------------------------------------------------------------
 */
 
    main {
 
    // ADS SENSITIVITY W/ TIMER
    if(!master_onoff)
    {
    if(get_val(L2))
    {
     timer = timer + get_rtime();
     if(timer>= 000) 
    { // Increase time for delay on ADS sens
    sensitivity(RX, NOT_USE, 100 - sen_reduc);
    sensitivity(RY, NOT_USE, 100 - sen_reduc);
    }
    }          
    else 
    {
    timer =0; 
    sensitivity(RX, NOT_USE, 100 );
    sensitivity(RY, NOT_USE, 100 ); 
    }
    }   
	 
	// Auto Sprint 
	if(USE_AUTO_RUN && (!combo_running(CANCEL_SLIDE_A) || combo_running(CANCEL_SLIDE_E)) )
	{
	if(state == WALK)
	{			
	if(get_val(PS4_LY)< -80 && get_val(PS4_R2) < 10 && get_val(PS4_L2) < 10)
	{
	combo_run(sprint);	
	state = SPRINT;
	}	
	}
	else if( state == SPRINT)
	{	
	if(get_val(PS4_R2) >= 10 || get_val(PS4_L2) >= 10 || (get_val(PS4_LY) >= -80 ))
    {
    state = WALK;
    combo_stop(sprint);	
    }
    }
    }
	 
	// Slide Strafe 
	if(get_val(PS4_L1) && event_press(PS4_LEFT))
	{
	combo_run (vibrate);	
    USE_SLIDE_CANCEL = !USE_SLIDE_CANCEL; 
    } 
	 
    if(USE_SLIDE_CANCEL) 
    {
    if(
	(get_val(PS4_LY) < -85) || (get_val(PS4_LY) > 85) || 
	(get_val(PS4_LX) < -85) || (get_val(PS4_LX) > 85)
    ) 
    {
    slide_enable = 0;
	            
	if(event_release(CIRCLE))
	{
    if (USE_SLIDE_CANCEL == 1) 
	{
	slide_enable = 1;
	}
	else if (USE_SLIDE_CANCEL == 2) 
	{
	if(
	((get_rumble(RUMBLE_A) == 87 || get_rumble(RUMBLE_A) == 26))
    )
    {
    slide_enable = 1;
    }
    }
    else if (USE_SLIDE_CANCEL == 3) 
    {
    slide_enable = 2;
    }
    else if (USE_SLIDE_CANCEL == 4) 
    {
    if(
    ((get_rumble(RUMBLE_A) == 100 || get_rumble(RUMBLE_A) == 36))
    )
    {
    slide_enable = 2;
    }
    }
    }
	           
	if(slide_enable)
	{
	combo_stop(sprint);    
	state = WALK;
	                
	if(slide_enable == 1)
	combo_run(CANCEL_SLIDE_A);
	else if(slide_enable == 2)
	combo_run(CANCEL_SLIDE_E);        
	}
	}                
	}

  	//Teabag  
	if(get_val(PS4_L1) && event_press(PS4_UP))
	{
	combo_run (vibrate);	
    turbo_crouch = !turbo_crouch; 
    } 
	 
    if(turbo_crouch)
    { 
    if(get_val(CIRCLE)&& !combo_running(TEA))
    combo_restart(TEA); 
    } 
	 
	//RAPID MELEE  
	if(get_val(PS4_L1) && event_press(PS4_RIGHT))
	{
	combo_run (vibrate);	
    rapid_melee = !rapid_melee; 
    } 
	 
    if(rapid_melee)
    {
    if(get_val(R1) && get_ptime(R1) > melee_time)
    combo_run(PUNCH); 
    } 
	 
	// Easy Fire
	if(get_val(PS4_L2) && event_press(PS4_LEFT))
	{
	combo_run (vibrate);	
    easyfire = !easyfire; 
    }
   	
	if(easyfire)
	{
	if(get_val(PS4_R2) || get_val(PS4_L2))
    set_val(PS4_L2, 100);
    if(event_release(PS4_R2))
    set_val(PS4_L2, 0);
    }
	 
	// Auto Reload
	if(get_val(PS4_L2) && event_press(PS4_RIGHT))
	{
	combo_run (vibrate);	
    auto_reload = !auto_reload; 
    }
   	
    if(auto_reload) 
	{
    if(event_release(PS4_R2))
	combo_run(AUTORELOAD);
	}
	  
    // Aim Assist L2
	if(get_val(PS4_TOUCH) && event_press(PS4_L2))
	{
	combo_run (vibrate);	
    AA_L2 = !AA_L2; 
    } 
	 
    if(AA_L2)
	{
 	if(get_val(PS4_L2)) 		
    combo_run(AA_1);
    else 
    combo_stop(AA_1); 
	}
	
	// Aim Assist R2
	if(get_val(PS4_TOUCH) && event_press(PS4_R2))
	{
	combo_run (vibrate);	
    AA_R2 = !AA_R2; 
    } 
	 
    if(AA_R2)
	{
	if(get_val(PS4_R2)) 		
    combo_run(AA_2);
    else 
    combo_stop(AA_2); 
	}
	
	// RAPIDFIRE                           
    if (rapid_onoff) 
    {
    hold_time = 500 / RATE_OF_FIRE;      
    rest_time = hold_time - 20;
    if(rest_time < 0) rest_time = 0;

    if(get_val(PS4_R2)) 
    {
    combo_run (RapidFire); 
    } 
    }  
	
    // Anti Recoil	
    if(get_val(PS4_L2) && get_val(PS4_R2))
    combo_run(AR);
 
    if(get_val(PS4_L2) && event_press(PS4_UP)) 
    { 
    AR_ONOFF=!AR_ONOFF; 
    ARH_ONOFF = OFF; 
    LED(0,0,3,0);
    }
    
    if(AR_ONOFF) 
    {
    if(event_press(PS4_R1)) 
    {
    AR = AR + 1;
    combo_run(Increase);
    }
    
    if(event_press(PS4_L1)) 
    {
    AR = AR - 1;
    combo_run(Decrease);
    }
    
    set_val(PS4_R1,0); set_val(PS4_L1,0);
    }
 
    if(get_val(PS4_L2) && event_press(PS4_DOWN)) 
    { 
    ARH_ONOFF=!ARH_ONOFF; 
    AR_ONOFF = OFF; 
    LED(0,0,0,3);
    }
    
    if(ARH_ONOFF) 
    {
    if(event_press(PS4_R1)) 
    {
    AR_H = AR_H + 1;
    combo_run(Increase);
    }
    
    if(event_press(PS4_L1)) 
    {
    AR_H = AR_H - 1;
    combo_run(Decrease);
    }
    
    set_val(PS4_R1,0); set_val(PS4_L1,0);
    }
 
    if(!AR_ONOFF && !ARH_ONOFF) 
    {
    LED(0,0,0,0);
    }
 
    if(abs(get_val(PS4_RY)) > AR + 2 || abs(get_val(PS4_RX)) > AR + 2) 
    {
    combo_stop(AR);
    }
    }
    
 /*
 -----------------------------------------------------------------------------------------------------------------
 === The Combo Section ===
 -----------------------------------------------------------------------------------------------------------------
 */
 
    // Rumble
 	combo vibrate 
 	{                                      
    set_rumble(1, 100);                    
    wait(300);                                       
    reset_rumble();                                  
	}
	
	// SPRINT
	combo sprint
    {
	set_val(PS4_L3, 100);
	wait(128);
	set_val(PS4_L3, 0);
	wait(128);
	set_val(PS4_L3, 100);
	wait(128);
	set_val(PS4_L3, 0);
	wait(128);
    }
    
    combo CANCEL_SLIDE_E 
    {
	set_val(CIRCLE, 100);
    wait(E_Slide_Delay);
	set_val(CIRCLE, 0);
	wait(20);
	set_val(L3, 0);
	set_val(L2, 100);
	wait(10);
	set_val(L2, 0);
	set_val(CIRCLE, 100);
	set_val(L3, 100);
	wait(20);
	set_val(CIRCLE, 0);
	set_val(L3, 0);
	wait(20);
    }

    combo CANCEL_SLIDE_A 
    {
	set_val(CIRCLE, 100);
	wait(A_Slide_Delay); 
	set_val(CIRCLE, 0);
	wait(60);
	set_val(CIRCLE, 100);
	wait(50);
	set_val(CIRCLE, 100);
	set_val(CROSS, 100);
	wait(60);
	set_val(CIRCLE, 0);
	set_val(CROSS, 100);
	wait(10);
	set_val(CROSS, 0);
    }
	
	// Rapid Melee
	combo PUNCH 
	{
    set_val(R1, 100);
    wait(40);
    set_val(R1, 0);
    wait(40);
    set_val(R1, 0); }
	
	// Aim Assist L2
	combo AA_1 
	{			
    set_val(PS4_RY,xy_val(PS4_RY,aa));
    wait(aa_delay);
    set_val(PS4_RX,xy_val(PS4_RX,aa));
    set_val(PS4_LX,xy_val(PS4_LX,aa));
    wait(aa_delay);
    set_val(PS4_RY,xy_val(PS4_RY,inv(aa))); 
    wait(aa_delay);
    set_val(PS4_RX,xy_val(PS4_RX,inv(aa)));
    set_val(PS4_LX,xy_val(PS4_LX,inv(aa)));
    wait(aa_delay);
    }
	
	// Aim Assist R2
	combo AA_2 
	{			
    set_val(PS4_RY,xy_val(PS4_RY,aa));
    wait(aa_delay);
    set_val(PS4_RX,xy_val(PS4_RX,aa));
    set_val(PS4_LX,xy_val(PS4_LX,aa));
    wait(aa_delay);
    set_val(PS4_RY,xy_val(PS4_RY,inv(aa))); 
    wait(aa_delay);
    set_val(PS4_RX,xy_val(PS4_RX,inv(aa)));
    set_val(PS4_LX,xy_val(PS4_LX,inv(aa)));
    wait(aa_delay);
    }
    
	// Rapid Fire
	combo RapidFire 
	{
    set_val(4,100);
    wait(hold_time); 
    set_val(4,  0); 
    wait(rest_time); 
    }
	
	// Anti Recoil
    combo AR 
    {
    ARS = get_val(PS4_RY) + AR;
    if(ARS > 100) ARS = 100;
    set_val(PS4_RY,ARS * AR_I);
    ARS = get_val(PS4_RX) + AR_H;
    if(ARS > 100) ARS = 100;
    set_val(PS4_RX,ARS);
    }
 
    // Anti Recoil Values +1
    combo Increase 
    {
    set_rumble(RUMBLE_A,100);
    wait(100);
    reset_rumble();
    }
    
	// Anti Recoil Values -1
    combo Decrease 
    {
    set_rumble(RUMBLE_B,100);
    wait(50);
    reset_rumble();
    }
	
	// Auto Reload
    combo AUTORELOAD 
    {
	wait(200);
    set_val(PS4_SQUARE,100);
    wait(40);
    set_val(PS4_SQUARE,0);
    }
	
	// Teabag
	combo TEA 
	{    
    set_val(CIRCLE, 100);
    wait(70);
    set_val(CIRCLE, 0);
    wait(70);
    set_val(CIRCLE, 0); 
    }
    
 /*
 -----------------------------------------------------------------------------------------------------------------
 === The Function Section ===
 -----------------------------------------------------------------------------------------------------------------
 */
 
    // Aim Assist Function
 	function xy_val(f_axis,f_val) 
 	{		
    if(abs(get_val(f_axis)) < aa + 1) 
    return f_val;
    return get_val(f_axis);
    } 
    
	// LED Function
    function LED(a,b,c,d) 
    {
    set_led(LED_1,a);
    set_led(LED_2,b);
    set_led(LED_3,c);
    set_led(LED_4,d);
    }

