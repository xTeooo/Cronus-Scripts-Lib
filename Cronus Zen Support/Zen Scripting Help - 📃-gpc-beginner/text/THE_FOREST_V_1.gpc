main {

	if(get_val(PS4_R1))//log dupe
	combo_run(ED)
	
	if(get_val(PS4_LY) <-95)//auto sprint
	combo_run(ER)
	
	if(get_val(PS4_R2))//R2 spam
	combo_run(EI)
	
	if(get_val(PS4_L1))//fast X and ⬛
	combo_run(EX)
	
	if(get_val(PS4_CROSS))// X Spammer
	combo_run(XP)
}

combo ED {
	set_val(PS4_CROSS, 100);
	wait(1);
	set_val(PS4_CROSS, 0);
	wait(1);
	set_val(PS4_CIRCLE, 100);
	wait(1);
	set_val(PS4_CIRCLE, 0);
}
combo ER {
	set_val(PS4_L3, 100);
}
combo EI {
	set_val(PS4_R2, 100);
	wait(2);
	set_val(PS4_R2, 0);
}
combo EX {
	set_val(PS4_CROSS, 100);
	wait(2);
	set_val(PS4_SQUARE, 100);
	wait(2);
}
combo XP {
	set_val(PS4_CROSS, 100);
	wait(2);
	set_val(PS4_CROSS, 0);
}