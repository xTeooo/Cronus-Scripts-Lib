function Spiral(Rad){ 
	if(!(cycleRefresh++ % timeInterval)){
        if(!spiralPosition1){
            innerSpiral -= 327
            if(innerSpiral < 654)
                spiralPosition1 = 1;   
        }
        else{
            innerSpiral += 327;
            if(innerSpiral > Rad)
                spiralPosition1 = 0;
        }
        if(!spiralPosition2){
            outerSpiral -= 327;
            if(outerSpiral < 654)
                spiralPosition2 = 1;
        }
        else{
            outerSpiral += 327;
            if(outerSpiral > Rad)
                spiralPosition2 = 0
                
        }
        cycleRefresh = 0;
	}    
}			
