.darkMode #mobile-menu .mobileMenu li > *:hover svg .fill, .darkMode #mobile-menu .mobileMenu li.mDark svg .fill, .darkMode.onHome #mobile-menu .mobileMenu li.mHome svg .fill{fill:var(--dark-text)}
/* Mobile Menu */
#mobile-menu{display:none}
