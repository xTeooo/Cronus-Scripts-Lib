	Set_Val(Strafe, inv(100));
	wait(50);
	wait(5);
	Set_Val(Walk, inv(100));
	wait(20);
	Set_Val(Strafe, 100);
	wait(50);
	wait(5);
	Set_Val(Walk, 100);
	wait(20);
}

