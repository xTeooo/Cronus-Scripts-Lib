//---------------------------------combo code-----------------------------------
//------------------------------------------------------------------------------
//------------------------------------------------------------------------------
combo AR {
	   set_val(AIM_Y,36);
	wait(10);
	   set_val(AIM_Y,0);
	wait(10);
	}
//------------------------------------------------------------------------------
combo BattsStickyAssist {
set_val(AIM_Y,AxisRelease(AIM_Y,BattStickyPower));
wait(BattStickyWait);
set_val(AIM_X,AxisRelease(AIM_X,BattStickyPower));
set_val(XB1_LX,AxisRelease(XB1_LX,BattStickyPower));
wait(BattStickyWait);
set_val(AIM_Y,AxisRelease(AIM_Y,inv(BattStickyPower)));
wait(BattStickyWait);
set_val(AIM_X,AxisRelease(AIM_X,inv(BattStickyPower)));
set_val(XB1_LX,AxisRelease(XB1_LX,inv(BattStickyPower)));
wait(BattStickyWait);
}
//------------------------------------------------------------------------------
combo PickupSpam {
	   set_val(INTERACT, 100);
	wait(80);
	   set_val(INTERACT, 0);
	}
//------------------------------------------------------------------------------
combo rapidFire {
set_val(XB1_RT,100);
wait(40);
set_val(XB1_RT,0);
wait(40);
}
//------------------------------------------------------------------------------
combo Confirm {

   set_val(CONFIRM,100);
    wait(50);
    set_val(CONFIRM,0);
    wait(100);
}
//------------------------------------------------------------------------------
combo FastReset {

 
    wait(50)
    set_val(RESET,100);        
    wait(50);
    combo_run(Confirm);
}
//------------------------------------------------------------------------------
combo InstantReset {

    wait(20)
    combo_run(Confirm);
}
//------------------------------------------------------------------------------
combo cPerfectionAccuracy {
		set_val(FIRE, 100);
		wait(50);
		wait(360);
}
//------------------------------------------------------------------------------
combo u {
	Dhruvskiownsu(AIM_X,rx)wait(AresTIME)Dhruvskiownsu(AIM_X,0)wait(AresTIME)Dhruvskiownsu(AIM_X,-rx)wait(AresTIME)Dhruvskiownsu(AIM_X,0)wait(AresTIME)
}
//------------------------------------------------------------------------------
combo Ais {
		Dhruvskiownsu(AIM_Y,RIZ)
		Dhruvskiownsu(AIM_X,-RIZ)
		wait(1)
		Dhruvskiownsu(AIM_Y,-RIZ)
		Dhruvskiownsu(AIM_X,RIZ)
		wait(1)
		} 
//------------------------------------------------------------------------------
combo Auto_Strafe {	
set_val(XB1_LX,-30);
wait(ast);
set_val(XB1_LX, 30);
wait(ast);
if(ast < 160){ast = ast + 10;}
else {ast = 100;}}
//------------------------------------------------------------------------------
combo XXXENEMY_TRACKER { 
	    
	  	set_val(AIM_Y,xy_val(AIM_Y, _v)); 
		wait(Delay);
		set_val(AIM_X,xy_val(AIM_X, _v)); 
		set_val(LX,xy_val(LX, _strafe));
		wait(Delay); 
		set_val(AIM_Y,xy_val(AIM_Y, _v * -1)); 
		wait(Delay);
		set_val(AIM_X,xy_val(AIM_X, _v * -1)); 
		set_val(LX,xy_val(LX, _strafe * -1)); 
		wait(Delay);
}
//------------------------------------------------------------------------------
function AxisRelease(AxisXY,AxisVal) {
    
    if (abs(get_ival(AxisXY)) <= BattStickyPower) 
        
        return AxisVal;
    
    return get_ival(AxisXY); 
}
//------------------------------------------------------------------------------
function EditAssist() {
    EditT += get_rtime();

	if(get_ival(EditAssist_BUTTON))
	{
		if(EditT >= EditDelay)
			EditPress(SELECT);
	}
}
//------------------------------------------------------------------------------
function EditPress(Button) {
    if(get_ival(Button))
        return;
	set_val(Button, 100);
}
//------------------------------------------------------------------------------
function xy_val(f_axis,f_val) { 
	
	if(abs(get_val(f_axis)) < Release) 
	
		return f_val; 
		return get_val(f_axis); 
}
//------------------------------------------------------------------------------
function valuefinder(){
RIGHTOFFSETX = -1
RIGHTOFFSETY = -1
if (AIM_X > 0 ) {
RIGHTOFFSETX = 1
}
if (AIM_Y > 0 ) {
RIGHTOFFSETY = 1
}
rx = (AIM_X + SHAKEX) * RIGHTOFFSETX
ry = (AIM_Y + SHAKEY) * RIGHTOFFSETY
LEFTOFFSETX = -1
LEFTOFFSETY = -1
if ( LX > 0 ) {
LEFTOFFSETX = 1
}
if ( LY > 0 ) {
LEFTOFFSETY = 1
}
lx =  (LX + STRAFEX) * LEFTOFFSETX
ly =  (LY + STRAFEY) * LEFTOFFSETY

}
//------------------------------------------------------------------------------
function Dhruvskiownsu( axis,  offset_val) {
  set_val(axis, clamp(offset_val * (100 - abs(get_val(axis))) / 100 + get_val(axis), -100, 100));
  return;
}
