// VARIABLES //
int Edit1,Edit2,BuildTrack,Shunt,BuildCount,OneShot,BloomReducer,EditAssist,EditT;
int TargetList[2],targetLoop,index,Dhruvski,Ares,power;
int AresTIME = 3,deltax,deltay,hyp,Hadesx,Hadesy;
int B = 350;
int A = 450;
int rnd; int RIGHTOFFSETY,RIGHTOFFSETX;int LEFTOFFSETX,LEFTOFFSETY;
int ry;int rx; int lx; int Poisedon,Apollosum,ZUESPI;	int Artemisn;
int ly; int RAXIS;int delta;int RIZ = 2;

//Ares AA //
int DZ = 10; // deadzone in game
int SHAKEX = -9; // deadzone value or in game ADS value
int STRAFEX = -9; // deadzone value or in game ADS value 
int SHAKEY = -9; // deadzone value or in game ADS value 
int STRAFEY = -9; // deadzone value or in game ADS value

// EDIT MODS //
define FastReset = XB1_LT
int EditSensitivity =  FALSE;
int InstaFastReset  =  TRUE;
int InstaEditReset  =  TRUE;
define EditHold     =  FALSE;
int EditHoldTime   = 0;
define EditAssist_BUTTON	= PS4_CIRCLE; // ingame edit button. if odnt want to use then put as off
define EditDelay			=	  20;  

// Perfect Accuracy //
define PerfectAccuracy		= TRUE;

//Bloom Reducer//
int ast    			       = 100;

// Pickup Marco //
int PickUpMacro          = FALSE;

//BUILD TRACKER
int BuildTracker    =  TRUE;
int BuildSyncTime = 4000;

main {
vm_tctrl(-5); // (-5) RECCOMENED FOR COMP (-9) FOR CREATIVE
//------------------------------------------------------------------------------
 if (BattsStickyAA) {
if (get_ival(ADS) || (BattsAAHip && get_ival(FIRE))) {
combo_run(BattsStickyAssist);
} else {
combo_stop(BattsStickyAssist); 
}
}
//------------------------------------------------------------------------------
{
