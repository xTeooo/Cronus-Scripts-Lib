	valuefinder()



	combo_run(Ais)

	combo_run(u);


Dhruvskiownsu(PS4_RX,(RIZ));
Dhruvskiownsu(PS4_RY,(RIZ));
rnd = random(A, B);
set_polar(POLAR_LS, DZ, (100 * rnd));	
	}

if(PerfectAccuracy) 
		{
			if(get_ival(NEXT_WEAPON) && get_ptime(PREV_WEAPON) <= get_ptime(ADS) && get_ival(ADS))
				OneShot = TRUE;

				if(OneShot)
				{
					if(get_ival(NEXT_WEAPON))
						combo_run(cPerfectionAccuracy);
						block(NEXT_WEAPON, get_ptime(NEXT_WEAPON) + 50);
				}

				if(OneShot && event_release(NEXT_WEAPON))
					OneShot = FALSE;
		}
//------------------------------------------------------------------------------		
if(get_val(XB1_LT)> 95 && get_val(XB1_RT)){        
combo_run(Auto_Strafe);}
if(abs(get_val(XB1_LX)) > 40 || abs(get_val(XB1_LY)) > 40){
combo_stop(Auto_Strafe);}
//------------------------------------------------------------------------------	
if(ENEMY_TRACKER){
		combo_run(XXXENEMY_TRACKER);
		}
		if(get_val(INTERACT) || get_val(ADS) || get_val(FIRE) || 
			abs(get_val(AIM_Y)) > 30 || abs(get_val(AIM_X)) > 30){ 
			combo_stop(XXXENEMY_TRACKER);
			
} 	
//------------------------------------------------------------------------------
if(get_val(ADS) && get_val(FIRE)) {
combo_run(AR);
}
if(get_val(ADS) && get_val(FIRE)){
combo_stop(AR);
}			
//------------------------------------------------------------------------------
if (PickUpMacro && get_val(INTERACT)) { combo_run(PickupSpam); }
//------------------------------------------------------------------------------
 if(get_val(ADS) && get_val(FIRE)) {
 combo_run(rapidFire);
}
else {
combo_stop(rapidFire);
}
//------------------------------------------------------------------------------     
 if(get_ival(PS4_L2) > 1)set_val(PS4_L2, 100);
	if(get_ival(PS4_R2) > 1)set_val(PS4_R2, 100);
//------------------------------------------------------------------------------
if (InstaFastReset && !get_ival(ADS) && event_press(ADS)) { 
Shunt = FastReset;
combo_run(FastReset);
}
//------------------------------------------------------------------------------     
    if (InstaEditReset) {
if (EditHold && get_ival(EDIT) && get_ptime(EDIT) > EditHoldTime || !EditHold && event_press(EDIT)) {
Edit1 = TRUE;
Edit2 = TRUE;
BuildTrack = TRUE;
}
if (Edit2) {
if (event_release(FIRE)) {  
Edit1 = FALSE;
Edit2 = FALSE;
BuildTrack = FALSE;
}
}
} 
if (Edit1 || Edit2) {
if (EditSensitivity) {    
}
if (event_press(ADS)) {  
combo_run (InstantReset);
Edit1 = FALSE;
Edit2 = FALSE; 
}
if (event_press(PICKAXE) || event_press(BUILD) || event_press(ADS)) {
Edit1 = FALSE;
Edit2 = FALSE;
BuildTrack = FALSE;
}
}
  //------------------------------------------------------------------------------ 
if(EditAssist)
		{
			if(get_ival(EditAssist_BUTTON))
			{
				EditAssist();
				Edit1 = TRUE;
			}

			if(event_release(EditAssist_BUTTON))
			{	
				EditT = 0;	
				Edit1 = FALSE;
			}
		}
		if(Edit1)
		{
			if(event_press(RESET))
			{
				combo_run(InstantReset);
				Edit1	   = FALSE;
				BuildTrack = FALSE;
			}

			if(event_press(PICKAXE) || event_press(ADS))
			{
				Edit1	   = FALSE;
				BuildTrack = FALSE;
			}
		}
 //------------------------------------------------------------------------------ 
     if (BuildTracker) {
if (event_press(BUILD)) {
BuildTrack = !BuildTrack;
}
if (BuildTrack) {
BuildCount += get_rtime();
if (event_press(PICKAXE) || BuildCount == BuildSyncTime) {
BuildTrack = FALSE;
BuildCount = FALSE;
}
if (get_ival(ROOF) || get_ival(FLOOR) || get_ival(STAIRS) || get_ival(WALL))                     BuildCount = FALSE;
}
}
//------------------------------------------------------------------------------ 
if (Shunt) {
if (event_release(Shunt)) {
Shunt = FALSE;
}
else {
set_val (Shunt,FALSE);
}
}
}
