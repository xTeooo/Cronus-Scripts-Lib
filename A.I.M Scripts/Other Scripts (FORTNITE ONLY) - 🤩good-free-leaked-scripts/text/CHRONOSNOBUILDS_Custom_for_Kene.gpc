function active(mode) {

	if (mode == 0) {
	
		if (get_ival(ADS) && get_ival(FIRE)) {
		
			return On;
		}
	}
	
	if (mode == 1) {
	
		if (get_ival(FIRE)) {
		
			return On;
		}
	}
	
	if (mode == 2) {
	
		if (!get_ival(ADS) && get_ival(FIRE)) {
		
			return On;
		}
	}
	
	return Off;
}
