    define ON = TRUE;
    define OFF = FALSE;
 
    int Sprint_Cancel,Speed =22; 
    
    main {
 
    if(get_val(12)<-90 && event_press(8))
    Sprint_Cancel= ON; else if(get_val(12)>-88) Sprint_Cancel = OFF;   
    if(Sprint_Cancel && event_press(4)) combo_run(Cancel);
    
    if(!Sprint_Cancel && get_val(4)) {
    set_val(4,0); combo_run(Jitter);}}

    combo Jitter {
    set_val(20,100);
    set_val(4,100);
    wait(11);
    set_val(6,100);
    wait(11);
    set_val(17,100);
    wait(Speed);}
    
    combo Cancel {
    set_val(12,0);
    set_val(4,0);
    wait(11);
    Sprint_Cancel = OFF;}

  

    

