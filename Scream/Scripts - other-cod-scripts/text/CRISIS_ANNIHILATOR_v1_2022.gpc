			{
				if (abs(get_ival(Bu[ttons][id]) > 10))
				{
					print(cX(gSL(BtnID[id]), OfMW), 35, OfM, Ow, (BtnID[id]))
				}
			}
		}

		rainbow();
		block_btn = On;
	}
	else
	{
		block_btn = Off;
	}	//→ end boot btn checker
	if (wpn_update)
	{
		cls_oled(0);
		if (!Secondary_WPN)
		{
			print(cX(gSL(miscS[1]), OfSW), 15, OfS, Ow, miscS[1]);
		}
		else
		{
			print(cX(gSL(miscS[2]), OfSW), 15, OfS, Ow, miscS[2]);
		}

		weapon_idx = profile_idx;
		if (rf[profile_idx] == 3)
		{
			print(cX(gSL(miscS[44]), OfMW), 27, OfM, Ow, miscS[44]);
		}
		else
		{
			print(cX(gSL(Weapons[weapon_idx]), OfMW), 27, OfM, Ow, Weapons[weapon_idx]);
		}

		if (rf[profile_idx] == 1)
		{
			print(cX(gSL(miscS[42]), OfSW), 46, OfS, Ow, miscS[42]);
			print(cX(gSL(miscS[43]), OfSW), 55, OfS, Ow, miscS[43]);
		}

		wpn_update = FALSE;

		display_black = TRUE;
	}

	if (display_black)
	{
		checkifpressed();

		count_black += get_rtime();
		if (count_black >= 2500)
		{
			cls_oled(0);
			count_black = 0;
			if (save)
			{
				displayValue = pin;
