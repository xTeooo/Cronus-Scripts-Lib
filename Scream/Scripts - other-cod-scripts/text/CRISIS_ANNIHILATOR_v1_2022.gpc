int p_current_slot, p_current_bit, p_current_value;

function reset_spvar() {

        p_current_slot = SPVAR_17;
        p_current_bit = 0;
        p_current_value = 0;

}
int p_temp;

function get_bit_count(b_val) {

        if (b_val < 0)
                return get_bit_count(abs(b_val + 1));

        p_temp = 0;

        do {
                p_temp++;
                b_val = b_val >> 1;
        }
        while (b_val);

        return p_temp;

}

function get_bit_count_2(f_min, f_max) {

        p_temp = max(get_bit_count(f_min), get_bit_count(f_max));

        if (is_signed_2(f_min, f_max))
                p_temp++;

        return p_temp;

}

function is_signed_2(f_min, f_max) {

        return f_min < 0 || f_max < 0;

}
int P_Full_Bitmask = -1;

function make_full_mask(f_bits) {

        if (f_bits == 32)
                return P_Full_Bitmask;

        return (P_Full_Bitmask & (~(1 << (32 - 1)))) >> (32 - f_bits - 1);

}

function make_sign(f_bits) {

        return 1 << clamp(f_bits - 1, 0, 32 - 1);

}

function make_sign_mask(f_bits) {

        return ~make_sign(f_bits);

}

function pack_unsigned(b_val, f_bits) {

        return b_val & make_full_mask(f_bits);

}

function pack_signed(b_val, f_bits) {

        if (b_val < 0)
                return (abs(b_val) & make_sign_mask(f_bits)) | make_sign(f_bits);

        return b_val & make_sign_mask(f_bits);

}

function unpack_signed(b_val, f_bits) {

        if (b_val & make_sign(f_bits))
                return 0 - (b_val & make_sign_mask(f_bits));

        return b_val & make_sign_mask(f_bits);

}

function read_spvar_slot(f_slot) {

        return get_pvar(f_slot, 0 - (P_Full_Bitmask & (1 << (32 - 1))), 0 - ((0 - (P_Full_Bitmask & (1 << (32 - 1)))) + 1), 0);

}
int p_bits;

function save_spvar(b_val, f_min, f_max, f_save) {

        p_bits = get_bit_count_2(f_min, f_max);

        if (is_signed_2(f_min, f_max))
                b_val = pack_signed(b_val, p_bits);
        b_val = pack_unsigned(b_val, p_bits);

        if (p_bits > 32 - p_current_bit) {
                p_current_value = p_current_value | (b_val << p_current_bit);
                set_pvar(p_current_slot, p_current_value);
                p_current_slot++;
                p_bits -= (32 - p_current_bit);
                b_val = b_val >> (32 - p_current_bit);
                p_current_bit = 0;
                p_current_value = 0;
        }

        p_current_value = p_current_value | (b_val << p_current_bit);
        p_current_bit += p_bits;

        if (p_current_bit >= 32) {
                p_current_bit -= 32;
                set_pvar(p_current_slot, p_current_value);
                p_current_slot++;
                if (!p_current_bit)
                        p_current_value = 0;
        }

        if (f_save)
                set_pvar(p_current_slot, p_current_value);

}

function read_spvar(f_min, f_max, f_default) {

        p_current_value = read_spvar_slot(p_current_slot);
        p_bits = get_bit_count_2(f_min, f_max);
        p_current_value = pack_unsigned(p_current_value >> p_current_bit, p_bits);

        if (p_bits > 32 - p_current_bit) {
                p_current_slot++;
                p_temp = read_spvar_slot(p_current_slot);
                p_temp = pack_unsigned(p_temp, p_bits - (32 - p_current_bit));
                p_temp = p_temp << (32 - p_current_bit);
                p_current_value = pack_unsigned(p_current_value, (p_bits - (32 - p_current_bit)));
                p_current_value = p_current_value | p_temp;
                p_current_bit = p_current_bit - 32;
        }

        p_current_bit += p_bits;

        if (p_current_bit >= 32) {
                p_current_bit -= 32;
                p_current_slot++;
        }

        p_current_value = pack_unsigned(p_current_value, p_bits);

        if (is_signed_2(f_min, f_max))
                p_current_value = unpack_signed(p_current_value, p_bits);

        if (p_current_value < f_min || p_current_value > f_max)
                return f_default;

        return p_current_value;

