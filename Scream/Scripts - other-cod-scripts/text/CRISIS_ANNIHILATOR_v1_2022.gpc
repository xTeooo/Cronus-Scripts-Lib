									y_input[Yidx2] = current_y;
									Yidx2 += 1;
									if (Yidx2 == 25)
									{
										for (Yidx2 = 0; Yidx2 <= 24; Yidx2++)
										{
											ry_avg2 += y_input[Yidx2];
										}

										ry_avg2 /= 25;
										sY_eY_cor2 += ry_avg2 / 10;
										ry_avg2 = 0;
										Yidx2 = 0;
									}
								}
							}
						}

						if (run_time <= full_time)
						{
							if (abs(current_x) >= x_ambit) /*horizontal value*/
							{
								x_input[Xidx] = current_x;
								Xidx += 1;
								if (Xidx == 25)
								{
									for (Xidx = 0; Xidx <= 24; Xidx++)
									{
										rx_avg += x_input[Xidx];
									}

									rx_avg /= 25;
									sX_cor += rx_avg / 10;
									rx_avg = 0;
									Xidx = 0;
								}
							}
						}
						else
						if (run_time > full_time && twice)
						{
							notify(2);
							twice--;
						}
					}

					if (!get_val(ADS) || hold_ads)
					{
						if (get_val(XB1_B) && event_release(UP))
						{
							sY_eY_cor2--;
						}
						else if (get_val(XB1_B) && event_release(DOWN))
						{
							sY_eY_cor2++;
						}
						else if (event_release(UP))
						{
							sY_eY_cor--;
						}
						else if (event_release(DOWN))
						{
							sY_eY_cor++;
						}
						else if (event_release(RIGHT))
						{
							sX_cor--;
						}
						else if (event_release(LEFT))
						{
							sX_cor++;
						}
					}

					if (get_val(RELOAD) && get_ptime(RELOAD) > 500 && !record_rt)
					{
						record_rt = TRUE;
						notify(record_rt);
					}

					if (record_rt)
					{
						record_reload = get_ptime(RELOAD);
						y5 = record_reload;
						if (event_release(RELOAD))
						{
							notify(record_rt);
							reload[profile_idx] = record_reload;
							y5 = reload[profile_idx];
							record_rt = FALSE;
						}
					}

					AR_s[profile_idx] = sY_eY_cor;
					AR_e[profile_idx] = sY_eY_cor2;
					AR_h[profile_idx] = sX_cor;
					y1 = AR_s[profile_idx];
					y2 = AR_e[profile_idx];
					y3 = AR_h[profile_idx];
				}

				b_btn(XB1_B);
				b_btn(UP);
				b_btn(DOWN);
				b_btn(RIGHT);
				b_btn(LEFT);
				b_btn(HOME);
			}
		}	//→ end !bootimer
