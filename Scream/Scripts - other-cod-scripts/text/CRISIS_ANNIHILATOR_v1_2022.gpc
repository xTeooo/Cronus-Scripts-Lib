                cls_oled(0);

                displayValue = vc_val;
                value_update = TRUE;

        } else if (event_press(RIGHT)) {
                vc_inc = vc_1;
                vc_val += vc_1;
                if (vc_val < vc_mn)
                        vc_val = vc_mn;
                if (vc_val > vc_mx)
                        vc_val = vc_mx;

                cls_oled(0);

                displayValue = vc_val;
                value_update = TRUE;

        } else if (event_press(UP)) {
                vc_inc = vc_10;
                vc_val += vc_inc;
                if (vc_val < vc_mn)
                        vc_val = vc_mn;
                if (vc_val > vc_mx)
                        vc_val = vc_mx;

                cls_oled(0);

                displayValue = vc_val;
                value_update = TRUE;

        } else if (event_press(DOWN)) {
                vc_inc = vc_10;
                vc_val -= vc_inc;
                if (vc_val < vc_mn)
                        vc_val = vc_mn;
                if (vc_val > vc_mx)
                        vc_val = vc_mx;

                cls_oled(0);

                displayValue = vc_val;
                value_update = TRUE;

        }

        return vc_val;
}
