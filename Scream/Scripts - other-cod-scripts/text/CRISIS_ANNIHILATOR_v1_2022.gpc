//→ Loadshit function by CRISIS to save & load all profile values succesfully with Swizzy`s bit packer
function Loadshit() {
  for (profile_idx = 0; profile_idx <= 9; profile_idx++) {

    AR_s[profile_idx] = PROFILE[profile_idx][0];
    AR_e[profile_idx] = PROFILE[profile_idx][1];
    AR_t[profile_idx] = PROFILE[profile_idx][2];
    AR_h[profile_idx] = PROFILE[profile_idx][3];
    reload[profile_idx] = PROFILE[profile_idx][4];
    rf[profile_idx] = PROFILE[profile_idx][5];
    sniper[profile_idx] = PROFILE[profile_idx][6];
    custom_ads_sense[profile_idx] = PROFILE[profile_idx][7];
  }
} //→ end Loadshit
