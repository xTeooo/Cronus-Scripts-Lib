//→ function Number To String
function NumberToString(n_val, n_digits) {

        ic = 1;
        c_val = 10000;

        if (n_val < 0) {
                putc_oled(ic, 45);
                ic += 1;
                n_val = abs(n_val);
        }

        for (c_ = 5; c_ >= 1; c_--) {
                if (n_digits >= c_) {
                        putc_oled(ic, AII__[n_val / c_val]);
                        n_val = n_val % c_val;
                        ic += 1;

                }
                c_val /= 10;
        }

        puts_oled(cX(ic - 1, OfMW), 28, OfM, ic - 1, Ow);
} //→ end function Number To String

int foot, vagina, penis;

function n_2_s(orgasm, ass, oral, anal) {
        foot = 1;
        vagina = 10000;
        if (orgasm < 0) {
                putc_oled(foot, 45);
                foot += 1;
                orgasm = abs(orgasm);
        }
        for (penis = 5; penis >= 1; penis--) {
                if (ass >= penis) {
                        putc_oled(foot, AII__[orgasm / vagina]);
                        orgasm = orgasm % vagina;
                        foot += 1;
                        if (penis == 4) {
                                putc_oled(foot, 44);
                                foot += 1;
                        }
                }
                vagina /= 10;
        }
        puts_oled(anal, oral, dspl_font_size, foot - 1, Ow);
}
