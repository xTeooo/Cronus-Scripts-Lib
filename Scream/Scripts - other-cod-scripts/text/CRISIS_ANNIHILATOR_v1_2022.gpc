//→ function to remove rounding differences after scaling
function ry(value, precision) { // credits Swizzy & CRISIS
        precision = pow(10, precision);
        if (value * precision / 10 > 5) {
                return value + precision - value % precision;
        }
        return value - value % precision;
} //→ end rounding function   

//→ function Find Digits
function FindDigits(f_num) {
        f_num = abs(f_num);
        if (f_num / 1000 > 0) return 4;
        if (f_num / 100 > 0) return 3;
        if (f_num / 10 > 0) return 2;
        return 1;
} //→ end function FD
