combo Perfection{ 
    
    EvilXL = EvilX
    EvilYL = EvilY
    wait(SamplingTime);
    EvilX = get_lval(AimX)- LearnRX;
    EvilY = get_lval(AimY)- LearnRY; 
    
    if (Sample) {
    
        Perfection(EvilXL,EvilX,1,0,1,0); 
        Perfection(EvilYL,EvilY,1,0,0,1);
    } 
    
    EvilXL = EvilX;
    EvilYL = EvilY;
    wait(SamplingTime);
    EvilX = get_lval(AimX)- LearnRX;
    EvilY = get_lval(AimY)- LearnRY; 
    
    if (Sample) {
    
        Perfection(EvilXL,EvilX,0,1,1,0); 
        Perfection(EvilYL,EvilY,0,1,0,1);
    }
    
    Sample = On; 
    wait(SamplingTime);
}        

combo FineTune {
    
    set_val(AimX,(15 - FinePulse));
    set_val(Strafe,(-15 + FinePulse));
    wait(SamplingTime);
    wait(SamplingTime);
    wait(SamplingTime);
    set_val(AimX,(15 - FinePulse));
    set_val(AimY,(10 - FinePulse));
    set_val(Strafe,(-5 + FinePulse));
    wait(SamplingTime);
    wait(SamplingTime);
    wait(SamplingTime); 
    set_val(AimY,(10 - FinePulse));
    wait(SamplingTime);
    wait(SamplingTime);
    wait(SamplingTime);
    wait(SamplingTime);
    set_val(AimX,(-15 + FinePulse));
    set_val(AimY,(10 - FinePulse));
    set_val(Strafe,(5 - FinePulse))
    wait(SamplingTime);
    wait(SamplingTime)
    wait(SamplingTime)
    set_val(AimX,(-15 + FinePulse));
    set_val(Strafe,(15 - FinePulse))
    wait(SamplingTime);
    wait(SamplingTime);
    wait(SamplingTime);
    set_val(AimX,(-15 + FinePulse));
    set_val(AimY,(-10 + FinePulse));
    set_val(Strafe,(5 - FinePulse))
    wait(SamplingTime);
    wait(SamplingTime);
    wait(SamplingTime);
    set_val(AimY,(-10 + FinePulse)); 
    wait(SamplingTime);
    wait(SamplingTime);
    wait(SamplingTime);
    wait(SamplingTime);
    set_val(AimX,(15 - FinePulse));
