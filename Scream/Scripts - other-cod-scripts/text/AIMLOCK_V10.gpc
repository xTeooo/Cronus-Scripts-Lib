function set_Val(Input,Output) {

    set_val(Input,clamp(Output * (100 - abs(get_ival(Input))) / 100 + get_ival(Input),-100,100));
        
    return; 
}

function GetShapes(TaylorValue) {    
   
    if (!ZENPosition1) {
        
        InnerZEN -= 2;
        
        if (InnerZEN < TaylorMinRadius)
            
            ZENPosition1 = On;
    
    } else {
        
        InnerZEN += 1;
        
        if (InnerZEN > TaylorValue)
            
            ZENPosition1 = Off;
    }
    
    if (!ZENPosition2) {
    
        OuterZEN -= 1;
        
        if (OuterZEN < TaylorMinRadius)
            
            ZENPosition2 = On;
    
    } else {
        
        OuterZEN += 1;
        
        if (OuterZEN >  TaylorValue)
            
            ZENPosition2 = Off;
    } 
    
    if (TaylorAssist == 1) { 
    
        RYTAA = get_val(AimY) + (ShapeSin * TaylorValue) / 100;
        RXTAA = get_val(AimX) + (ShapeCos * TaylorValue) / 100;  
    } 
    
    if (TaylorAssist == 2) { 
    
        RYTAA = get_val(AimY) + (ShapeSin * TaylorValue) / 100;
        RXTAA = get_val(AimX) + (ShapeCos * TaylorMinRadius) /100; 
    }
    
    if (TaylorAssist == 3) { 
    
        RXTAA = get_val(AimX) + (ShapeCos * TaylorValue) / 100;  
        RYTAA = get_val(AimY) + (ShapeSin * TaylorMinRadius) / 100;
       
    }
    
    if (TaylorAssist == 4) {
    
        RYTAA = get_val(AimY) + (ShapeSin * InnerZEN) / 100;
        RXTAA = get_val(AimX) + (ShapeCos * OuterZEN) / 100;  
    }
    
    if (TaylorAssist == 5) {
        
        if (TaylorPosition == 0 || TaylorPosition == 1) {  
        
            RXTAA = get_val(AimX) + (ShapeCos * TaylorValue) / 100;
        }
        
        if (TaylorPosition == 2 || TaylorPosition == 3) { 
        
            RYTAA = get_val(AimY) + (ShapeSin * TaylorValue) / 100;
        }
            TaylorPosition++;        
            
        if (TaylorPosition == 4) {
            
            TaylorPosition = 0;
        }
    } 
    
    set_val(AimX,RXTAA);
    set_val(AimY,RYTAA);
}

function SteadyAtan(SteadyY,SteadyX) {

    if (SteadyX >= 0 && SteadyY > 0) 
        
        SteadyAngle = -90;
  
    else if (SteadyX  < 0 && SteadyY >= 0) 
        
        SteadyAngle = 90;
    
    else if (SteadyX <= 0 && SteadyY < 0) 
        
        SteadyAngle = -270;
  
      else 
      
          SteadyAngle = 270;
  
      SteadyX = abs(SteadyX);
      SteadyY = abs(SteadyY);
  
      if (SteadyX < SteadyY) 
      
          SteadyAngle += (SteadyX * 45 / SteadyY);
  
      else if (SteadyX > SteadyY) 
      
          SteadyAngle += 90 - (SteadyY * 45 / SteadyX);
  
      else 
      
          SteadyAngle += 45;
  
      SteadyAngle = abs(SteadyAngle % 360);
  
      return (SteadyAngle);
}

function SteadyMap(inMin,inMax,outMin,outMax,SteadyX) {

    return (SteadyX - inMin) * (outMax - outMin) / (inMax - inMin) + outMin;
}

function SteadyPolar(Stick,SaAngle,SaRadius,Offset) {
  
      if (SaAngle < 0) SaAngle = 360 + (SaAngle % 360);
      
          SaAngle = (SaAngle + 90) % 360;
  
      SteadySin = DamSteady[SaAngle];
      SteadyCos = DamSteady[(SaAngle + 90) % 360];
  
      if (Offset){
    
        set_Steady(7 + Stick, (SteadyCos * SaRadius) / 100);
        set_Steady(8 + Stick, (SteadySin * SaRadius) / 100);
  
      } else {
    
        set_val(7 + Stick,clamp((SteadyCos * SaRadius) / 100, -100, 100));
        set_val(8 + Stick,clamp((SteadySin * SaRadius) / 100, -100, 100));
      }
  
      return;
}

function set_Steady(SteadyAxis,SteadyOutput) {
  
      set_val(SteadyAxis,clamp(SteadyOutput * (100 - abs(get_val(SteadyAxis))) / 100 + get_val(SteadyAxis), -100, 100));
  
      return;
}

function Perfection(LastValue,CurrentValue,Boost,Correction,AxisX,AxisY) {
    
    if (abs(LastValue - CurrentValue) < PerfectionLimit) { 
        
        if (LastValue < CurrentValue) {
            
            if (Boost) {
                
                if (AxisX)
                    
                    set_val(AimX,(CurrentValue + AimBoost)); 
    
                if (AxisY)
                    
                    set_val(AimY,(CurrentValue + AimBoost));
            
            } else if (Correction) {
    
                if (AxisX)
    
                    set_val(AimX,(CurrentValue - AimCorrection)); 
                
                if (AxisY)
    
                    set_val(AimY,(CurrentValue - AimCorrection));
            }
        
        } else {
    
            if (Boost) {
    
                if (AxisX)
    
                    set_val(AimX,(CurrentValue - AimBoost)); 
    
                if (AxisY)
                    
                    set_val(AimY,(CurrentValue - AimBoost));
            
            } else if (Correction) {
            
                if (AxisX)
                    
                    set_val(AimX,(CurrentValue + AimCorrection)); 
                
                if (AxisY)
                    
                    set_val(AimY,(CurrentValue + AimCorrection));
            }
        }
    }
} 

