		        	else
		            	ar_y = VerticalEnd[ProfileIdx];
		          
	           		if(y_val() < 100)
				    	set_val(PS4_RY,y_val()); 
				    	
				    // Horizontal
				    ph = p_complete_x();
       			 	if(ph != -1)
    	            	ar_x = p_val_x();
