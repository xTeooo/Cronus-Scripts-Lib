function MenuScroll() {
    introScrollTimer += get_rtime();
    if(introScrollTimer >= introScrollPauseTime) {
        introScrollPauseTime = 30;
        for (introScrollTextIndex = 1; introScrollTextIndex <= 18; introScrollTextIndex++) {
        	if(introScroll) {
	        	if (introScrollTextIndex + introScrollTextOffset >= sizeof(INTRO_SCROLL_TEXT)) {
	                	putc_oled(introScrollTextIndex, INTRO_SCROLL_TEXT[introScrollTextIndex + (introScrollTextOffset - sizeof(INTRO_SCROLL_TEXT))]);
	            	}
	           	    else {
	                	putc_oled(introScrollTextIndex, INTRO_SCROLL_TEXT[introScrollTextIndex + introScrollTextOffset]);
	            }
	        } 
        	if(ModOrEditMenu[0] || ModOrEditMenu[1] && ModORVal[1] != 21 &&  ModORVal[1] != 22 && ModORVal[1] != 23 &&  ModORVal[1] != 24 && ModORVal[1] != 25 &&  ModORVal[1] != 26){  
        		if (introScrollTextIndex + introScrollTextOffset >= sizeof(MENU_SCROLL_TEXT)) {
                	putc_oled(introScrollTextIndex, MENU_SCROLL_TEXT[introScrollTextIndex + (introScrollTextOffset - sizeof(MENU_SCROLL_TEXT))]);
            	}
           	    else {
                	putc_oled(introScrollTextIndex, MENU_SCROLL_TEXT[introScrollTextIndex + introScrollTextOffset]);
            	}
            } 
            if(ModOrEditMenu[2] || ModOrEditMenu[1] && ModORVal[1] >= 21 && ModORVal[1] <= 26)
            {
            	if (introScrollTextIndex + introScrollTextOffset >= sizeof(EDITMENU_SCROLL_TEXT)) {
                	putc_oled(introScrollTextIndex,EDITMENU_SCROLL_TEXT[introScrollTextIndex + (introScrollTextOffset - sizeof(EDITMENU_SCROLL_TEXT))]);
            	}
           	    else {
                	putc_oled(introScrollTextIndex, EDITMENU_SCROLL_TEXT[introScrollTextIndex + introScrollTextOffset]);
            	}    
            }
        }
        if(introScroll || ModOrEditMenu[0] ||  ModOrEditMenu[1] && ModORVal[1] != 21 &&  ModORVal[1] != 22 && ModORVal[1] != 23 &&  ModORVal[1] != 24 && ModORVal[1] != 25 &&  ModORVal[1] != 26)
        	puts_oled(1, 52,OLED_FONT_SMALL, introScrollTextIndex, OLED_WHITE);
        else if(ModOrEditMenu[2] || ModOrEditMenu[1] && ModORVal[1] >= 21 && ModORVal[1] <= 26)
        	puts_oled(1, 5,OLED_FONT_SMALL, introScrollTextIndex, OLED_WHITE);
        introScrollTextOffset++;
        if(introScroll)
        {
        	if (introScrollTextOffset >= sizeof(INTRO_SCROLL_TEXT)) {
           	 	introScrollTextOffset = 0;
        	}
        }
        if(ModOrEditMenu[0] || ModOrEditMenu[1])
        {
        	if (introScrollTextOffset >= sizeof(MENU_SCROLL_TEXT)) {
           	 	introScrollTextOffset = 0;
        	}
        }
        if(ModOrEditMenu[2])
        {
        	if (introScrollTextOffset >= sizeof(EDITMENU_SCROLL_TEXT)) {
           	 	introScrollTextOffset = 0;
        	}
        } 
        introScrollCharIndex--;
        if (introScrollCharIndex < 1) {
            introScrollCharIndex = 18;
            introScrollPauseTime = 1500;

        }
        introScrollTimer = 0;
    }
}
