combo Confirm {

    set_val(Confirm,100);
    wait(50);
    set_val(Confirm,0);
    wait(100);
}

combo FastReset {

    set_val(EditAssist,100);
    wait(40)
    set_val(Reset,100);        
    wait(40);
    combo_run(Confirm);
}

combo InstantReset {

    wait(15)
    combo_run(Confirm);
}

combo AntiRecoil { 
    
    if(get_val(Fire)) {
    Anti_Recoil = get_val(10) + AntiRecoil;
    if(Anti_Recoil > 100) Anti_Recoil = 100;
    set_val(10, Anti_Recoil);
}
}

combo Auto_Strafe { // auto strafe variable (200ms to 320ms).
	
	//-- macros
	set_val(LX,-40);
	wait(ast);
	set_val(LX, 40);
	wait(ast);
	//-- check
	if(ast < 160){ast = ast + 10;}
	else {ast = 100;}
}

