main {

	if (PSonXBOX && get_console() == PIO_XB360 && get_controller() == PIO_PS4) {    
        
        swap(PS4_TOUCH,PS4_SHARE);
}
    
	if (Edit1 && event_press(Reset)) {  
         combo_run(InstantReset);
          Edit1 = Off;
}
	
    if (InstaFastReset) {
    if (!get_ival(Ads) && event_press(FastReset)) { 
        Shunt = FastReset;
        combo_run(FastReset);
     }  
}
    
   if (QuickEditAssist) {
	 	if (!get_val(Ads) && get_val(EditAssist)) {
			EditAssist();
			Edit1 = On;
            BuildTracker = On;
}

			if (event_release(EditAssist)) {
				EditT = Off;
				Edit1 = Off;
	            BuildTracker = Off;
	  }
}
   
   if (BuildTracker) {
        
        if (event_press(Build)) {
            BuildTrack = !BuildTrack;
        }
    
        if (BuildTrack) {
            BuildCount += get_rtime();
            
            if (event_press(Pickaxe) || BuildCount == BuildSyncTime) {
                BuildTrack = Off;
                BuildCount = Off;
            }
            
                if (get_ival(Roof) || get_ival(Floor) || get_ival(Ramp) || get_ival(Wall)) 
                    BuildCount = Off;
        }
    }

    if (!BuildTrack) { 
	
	
	 if (Antirecoil){                               
     
     if (get_val(Ads) && get_val(Fire)) {      
       
        combo_run(AntiRecoil); 
   }               

     if (abs(get_val(aimY)) > 36 || abs(get_val(aimX)) > 36) { 
        
        combo_stop (AntiRecoil); 
   }               
}
 
	
	 if (AimAssist) {
	 
	    if (event_press(Ads)) { 
	    	
	    	Power = On;
}
	    
	    else if (!get_ival(Ads) && event_press(Fire)) {
	    	
	    	Power = On;
}
	    
	    if (get_ival(Ads) || get_ival(Fire)) {
	    	
	    	AxisXY = isqrt(abs(get_ival(aimX)) * abs(get_ival(aimX)) + abs(get_ival(aimY)) * abs(get_ival(aimY)));
	    	
	    	if (AssistProfile == 1)
	    		
	    		AimAssist(Strength1,Increment1);
	    		
	    	 } else {
	    	
	    	CountPower = 0;
	    	power = Off;
}
	    
	    if (Shunt) {
        
        if (event_release(Shunt)) {
            
            Shunt = Off;
        
        } else {
                
            set_val (Shunt,Off);
       }
    }
}

      if(Strafe){
         	
      if(get_val(Ads) && get_val(Fire) || get_val(Fire)){        
         combo_run(Auto_Strafe);
       }
      
       if(abs(get_val(LX)) > 40 || abs(get_val(LY)) > 40){
		 combo_stop(Auto_Strafe);
}
     
     
     if(display_title){           
         cls_oled(0);            
         DrawLogo(0, 0, 0);
         display_title = FALSE;  
         display_black = TRUE;
}                            

      if(display_black){                            
         count_black += get_rtime();

      if(count_black >= 9000){                        
         cls_oled(0);         
         count_black = 0;      
         display_black = FALSE; 
       }
     }
   }
  }
}


