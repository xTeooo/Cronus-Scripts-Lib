function center_x(f_chars,f_font,f_location) {

	if(f_font == OLED_FONT_SMALL) {Font = OLED_FONT_SMALL_WIDTH;}	    
    if(f_font == OLED_FONT_MEDIUM){Font = OLED_FONT_MEDIUM_WIDTH;}
    if(f_font == OLED_FONT_LARGE) {Font = OLED_FONT_LARGE_WIDTH;}
    
    return (32 * f_location) - ((f_chars * Font) / 2);
   
}


function number_to_string(f_val,f_digits) {

    i = 1;  c_val = 10000;
    
	if(f_val < 0) 
	{          
         putc_oled(i,45);    
         i += 1;
         f_val = abs(f_val);
	} 
	
	for(c = 5; c >= 1; c--) 
	{
	    if(f_digits >= c) 
	    {
            putc_oled(i,ASCII_NUM[f_val / c_val]);
            f_val = f_val % c_val;
            i +=  1; 
            if(Menu_1){
	            if(c == 3) 
	            {
	                putc_oled(i,46);   
	                i += 1;
	            }
	        	 if(c == 1) 
	            {
	                putc_oled(i,37);   
	                i += 1;
	            }
        	}
        
        }
        c_val /= 10;
    }
    if(Menu_1){
    
    	puts_oled(center_x(i - 1,OLED_FONT_SMALL,2),52,OLED_FONT_SMALL,i - 1,ALT_FILL[3]); 
	}
	if(Menu_3){
    
    	puts_oled(center_x(i - 1,OLED_FONT_SMALL,2),52,OLED_FONT_SMALL,i - 1,ALT_FILL[3]); 
	}
	
}
 
function find_digits(f_num) {

    f_num = abs(f_num);
    if(f_num / 10000 > 0) return 5;
    if(f_num /  1000 > 0) return 4;
    if(f_num /   100 > 0) return 3;
    if(f_num /    10 > 0) return 2;
                          return 1;
}

