function Value_Change(f_idx,f_val,f_change,f_change2,f_min,f_max){
	
	if(Menu_1){
	
		if(sight_val_idx == f_idx){
			
			if(event_press(PS4_LEFT) && f_val > f_min){
				f_val -= f_change;
				
			} 
		
			if(event_press(PS4_RIGHT) && f_val < f_max){
				f_val += f_change;
				
			}
		
		}
	}	
	
	if(Menu_3){
	
		if(mod_edit_idx == f_idx){
			
			if(event_press(PS4_LEFT) && f_val > f_min){
				f_val -= f_change;
				
			} 
		
			if(event_press(PS4_RIGHT) && f_val < f_max){
				f_val += f_change;
				
			}
			
		}
	}	
	
	
	if(Menu_3){
	
		if(mod_edit_idx == f_idx){
			if(get_val(PS4_LEFT) && get_ptime(PS4_LEFT) > 200 + (hold_count * 100)){
				 if(f_val > f_min + f_change2){
					f_val -= f_change2;
					hold_count++
					Display = TRUE
				}
				else{
					if(f_val > f_min){
						f_val -= f_change;
						hold_count++
						Display = TRUE	
					}
				}
			}
			
			if(get_val(PS4_RIGHT) && get_ptime(PS4_RIGHT) > 200 + (hold_count * 100)){
				
				if(f_val < f_max - f_change2){
					f_val += f_change2;
					hold_count++
					Display = TRUE
				}
				else{
					if(f_val < f_max){
						f_val += f_change;
						hold_count++
						Display = TRUE	
					}
				}
			}
		}
	}
	
	
	if(Menu_1){
	
		if(sight_val_idx == f_idx){
			if(get_val(PS4_LEFT) && get_ptime(PS4_LEFT) > 200 + (hold_count * 100)){
				if(Menu_1 && hold_count > 20 && f_val > f_min + (f_change2 * 2)){
					f_val -= f_change2 * 2 ;
					hold_count++
					Display = TRUE
				}
				
				else if(f_val > f_min + f_change2){
					f_val -= f_change2;
					hold_count++
					Display = TRUE
				}
				else{
					if(f_val > f_min){
						f_val -= f_change;
						hold_count++
						Display = TRUE	
					}
				}
			}
			
			if(get_val(PS4_RIGHT) && get_ptime(PS4_RIGHT) > 200 + (hold_count * 100)){
				if(Menu_1 && hold_count > 20 && f_val < f_min -( f_change2 * 2)){
					f_val += f_change2 * 2;
					hold_count++
					Display = TRUE
				}
				else if(f_val < f_max - f_change2){
					f_val += f_change2;
					hold_count++
					Display = TRUE
				}
				else{
					if(f_val < f_max){
						f_val += f_change;
						hold_count++
						Display = TRUE	
					}
				}
			}
		}
	}
	if(event_release(PS4_LEFT) || event_release(PS4_RIGHT)){
		hold_count = 1;
	}
	
	
	return f_val;
}

int right_stick;

function speed_toggle(f_hold, f_press, f_time, f_val) {  
		
		if(get_val(f_hold) && abs(get_val(PS4_RX)) > right_stick ){
			right_stick = abs(get_val(PS4_RX));
		}
		
		if(get_val(f_hold) && get_val(f_press))
		{
			if(get_ival(PS4_UP)){
				set_val(PS4_UP,0);
			}
			if(get_ival(PS4_DOWN)){
				set_val(PS4_DOWN,0);
			}
			if(get_ival(PS4_LEFT) && right_stick < 20){
				set_val(PS4_LEFT,0);
			}
			if(get_ival(PS4_RIGHT) && right_stick < 20){
				set_val(PS4_RIGHT,0);
			}
			if(get_ival(PS4_LEFT) && right_stick > 20 && get_ptime(PS4_LEFT) > 100 ){
				set_val(PS4_LEFT,0);
				
			}
			if(get_ival(PS4_RIGHT) && right_stick > 20 && get_ptime(PS4_RIGHT) > 100){
				set_val(PS4_RIGHT,0);
			}
			
			if(get_ival(f_press) && get_ptime(f_press) == f_time) 
			{
				
				Mod_Toggle[f_val] = !Mod_Toggle[f_val];	
				menu_5 = FALSE;	
				menu_6 = FALSE;
				timer = 0;
				cls_oled(0)
				PRINT(thirty_five,	f_val,2,20,OLED_FONT_SMALL,1)
				PRINT(thirty_three,	Mod_Toggle[f_val],2,40,OLED_FONT_MEDIUM,1)
				timer = 0;
				toggle_screen = TRUE;
				
			} 
		
		}
		if(toggle_screen){
			timer += get_rtime();
			if(timer == 100){
				combo_run(rumble);
				
			}
					
			if(timer == 10000){
				toggle_screen = FALSE;
				menu_5 = TRUE;
				timer = 0;
				Display = TRUE;
			}
		
		}

}

