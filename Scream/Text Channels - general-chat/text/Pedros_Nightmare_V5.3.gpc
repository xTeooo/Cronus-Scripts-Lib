int SmartSens;

function SmartSens(f_val, f_axis) { 

    SmartSens = ( (f_val * ( 32767 - abs( get_val(f_axis) ) ) ) / 32767); 

    return clamp(get_val(f_axis) + SmartSens, -32767, 32767);
}


function anti_recoil (f_hori,f_ver,f_time){
	
	
		if(!Mod_Toggle[1]){	
		AY = f_ver  * 32767 / 100;
		}
		else{
			AY = inv(f_ver  * 32767 / 100);
		}
		AX = f_hori * 32767 / 100;
		WT = f_time;
		
		if(!Re_Map[0]){
			if(get_ival(PS4_L2) > 1){
				set_val(PS4_L2, 100);
			}
		}
		
		if(Re_Map[1] == 2){
			if(get_ival(PS4_R2) > 1){
				set_val(PS4_R2, 100);
			}
		}
		
		if (!RM && get_rumble(RUMBLE_A) > 10)
		{
			RM = TRUE;
		}
		
		if(get_val(PS4_R2) && !stoprecoil){
			RTT += get_rtime();   
			if(RTT >= WT){ 
				VP++;
				HP++;
				TA++;
				STP++;
				
			}
			if(RTT >= STOPPER[profile_idx]){
				stoprecoil = TRUE;
			} 
			
						
			if(get_val(PS4_L2) && get_val(PS4_R2)){
				if(sight_idx == 1){
					YS = IronVert[profile_idx]
					XS = IronHori[profile_idx]
				}
				
				if(sight_idx == 2){
					YS = HoloVert[profile_idx]
					XS = HoloHori[profile_idx]
				}
				
				if(sight_idx == 3){
					YS = X4Vert[profile_idx]
					XS = X4Hori[profile_idx]
				}			
						
				
					AYU = (AY * (10000 + YS)) / 10000;
	    			AXU = (AX * (10000 + XS)) / 10000;
					
				if(isqrt(pow(abs(get_ival(POLAR_RX)),2) + pow(abs(get_ival(POLAR_RY)),2)) <=  (50 * 327) ){
					set_val(POLAR_RY, clamp(get_val(POLAR_RY) + AYU, -32767, 32767));
					set_val(POLAR_RX, clamp(get_val(POLAR_RX) + AXU, -32767, 32767));
				}
				else{
					set_val(POLAR_RY,SmartSens(AYU,POLAR_RY));
					set_val(POLAR_RX,SmartSens(AXU,POLAR_RX));	
				}
			}
			
			if(!get_val(PS4_L2) && get_val(PS4_R2)){
			
				if(RM || Mod_Value[12]){
					AYU = (AY2 * (10000 + HFVert [profile_idx])) / 10000;
				    AXU = (AX2 * (10000 + HFHori [profile_idx])) / 10000;
					AY2 = (AY * (10000 + HipFire [profile_idx])) / 10000;
				    AX2 = (AX * (10000 + HipFire [profile_idx])) / 10000;
					set_val(POLAR_RY,SmartSens(AYU,POLAR_RY));
					set_val(POLAR_RX,SmartSens(AXU,POLAR_RX));
				}
			}
			
			
			if (profile_idx >= 7 && !stoprecoil) {
        		if (get_val(PS4_R2)) {
            		combo_run(RapidFire);    
        		} 
        		else {
            		combo_stop(RapidFire);
        		}
    		}
		
		}

		if(Re_Map[1] == 2){
			if(!get_val(PS4_R2) && get_brtime(PS4_R2) > 100){ 
				VP   = 0;
			    HP   = 0;
