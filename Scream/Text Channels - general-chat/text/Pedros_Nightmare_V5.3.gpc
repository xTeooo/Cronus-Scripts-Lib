function PRINT_SCROLL (f_string,f_idx){

	Transfer(f_string,f_idx);
		
	for(Buffer = 1 ; Buffer <= Size_Transfer; Buffer++){
		putc_oled(Buffer, Transfer[Buffer + Increase]);
	}
	puts_oled(Position,1,OLED_FONT_SMALL,Buffer,OLED_WHITE)	


Time += get_rtime();
	if(Time > 20){
		Position-- 
		Time = 0;
	}
	if (Position == 0){
		Position = 7;
		Increase ++;
	}

	if( Increase == Size_Transfer - 17 ){
		Position = 120;
		Increase = 0;
	}


}
