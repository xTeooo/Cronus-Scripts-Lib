// Variables \\
int Positions,SpiralPosition1,SpiralPosition2,InnerSpiral,OuterSpiral; 
int MinimumRadius = 1; 
int time;                                             
int cos, sin;                      
int actual_X, actual_Y;

function GetShapes(f_var) {     
   
    if(!SpiralPosition1)                                
    {
        InnerSpiral -= 1;
        if(InnerSpiral < MinimumRadius)
            SpiralPosition1 = TRUE;
    }
    else
    {
        InnerSpiral += 14;
        if(InnerSpiral > f_var)
            SpiralPosition1 = FALSE;
    }
    if(!SpiralPosition2)
    {
        OuterSpiral -= 1;
        if(OuterSpiral < MinimumRadius)
            SpiralPosition2 = TRUE;
    }
    else
    {
        OuterSpiral += 1;
        if(OuterSpiral >  f_var)
            SpiralPosition2 = FALSE;
    } 
  
    if( ToggleShapes == 1)                                     
    { 
        cos = (cos * f_var) /100;  
        sin = (sin * f_var)/100;  
    }
    if( ToggleShapes == 2)        
    { 
        sin = (sin * f_var)/100 *2;
        cos = (cos * MinimumRadius)/100;   
    }
    
    if( ToggleShapes == 3)                     
    { 
        cos = (cos * OuterSpiral) /100;  
        sin = (sin * f_var)/100;  
    }
  
    if( ToggleShapes == 4)                  
    {
         sin = (sin * InnerSpiral)/100         
         cos = (cos * InnerSpiral)/100 *2;
    }
    
    
    
    if( ToggleShapes == 5)
    {
        if(Positions == 0 || Positions == 1)
        {  
            actual_X = (get_val(aim_x)) + ((cos * OuterSpiral))/100;
        }
        if(Positions == 2 || Positions == 3)
        {  
            actual_Y = (get_val(aim_y)) + ((sin * f_var))/100;
        }
            Positions++;        
            
        if(Positions == 4)
        {
            Positions = 0;
        }    
    }
    if(ToggleShapes == 5 )
    {
           set_val(aim_x, actual_X);
           set_val(aim_y, actual_Y); 
    
	}
    else
    {
        set_val(aim_x, calculate(actual_X + cos, -100, 100));
        set_val(aim_y, calculate(actual_Y + sin, -100, 100)); 
  
    }   
 }
function calculate(int v, int lo, int hi) { 
  if(v < lo) return lo;               
  if(v > hi) return hi;        
  return v;     
}
int __OWL_PICTURESWidth, __OWL_PICTURESX, __OWL_PICTURESHeight, __OWL_PICTURESY, __OWL_PICTURESBit, __OWL_PICTURESOffset, __OWL_PICTURESData;
int __OWL_PICTURESX2, __OWL_PICTURESY2;
function draw_OWL_PICTURES(x, y) {
	__OWL_PICTURESOffset = 1; // Reset the starting point
	__OWL_PICTURESBit = 0; // Reset bit flag
	__OWL_PICTURESWidth = OWL_PICTURES[0]; // Fetch the width of what to draw
	__OWL_PICTURESHeight = OWL_PICTURES[1]; // Fetch the height of what to draw
	for (__OWL_PICTURESY = 0; __OWL_PICTURESY < __OWL_PICTURESHeight; __OWL_PICTURESY++) { // Loop the Y axis
		__OWL_PICTURESY2 = y + __OWL_PICTURESY;
		if (__OWL_PICTURESY2 < 0 || __OWL_PICTURESY2 >= 64) {
			__OWL_PICTURESY2 -= 64;
		}
		for (__OWL_PICTURESX = 0; __OWL_PICTURESX < __OWL_PICTURESWidth; __OWL_PICTURESX++) { // Loop the X axis
			if (!__OWL_PICTURESBit) { // Check if we've already handled the last bit
				__OWL_PICTURESBit = 16; // Reset the bit flag
				__OWL_PICTURESOffset++; // Move to the next value
				__OWL_PICTURESData = OWL_PICTURES[__OWL_PICTURESOffset]; // Fetch the value
			}
			__OWL_PICTURESBit--; // Decrement the bit flag, we're moving to the next bit
			__OWL_PICTURESX2 = x + __OWL_PICTURESX;
			if (__OWL_PICTURESX2 < 0 || __OWL_PICTURESX2 >= 128) {
				__OWL_PICTURESX2 -= 128;
			}
			pixel_oled(__OWL_PICTURESX2, __OWL_PICTURESY2, test_bit(__OWL_PICTURESData, __OWL_PICTURESBit));
		}
	}
}

