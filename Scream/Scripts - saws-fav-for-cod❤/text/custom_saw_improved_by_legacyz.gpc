function atan(as){
    if (EDDsx != EDDsx){
        EDDsx = 0;
            }
    if (EDDsy != EDDsy){
        EDDsy = 0;
            }
    if (EDDsx > 89){
        EDDsx = 89;
            }
    if (EDDsx < -89){
        EDDsx = -89;
            }
    if (EDDsy > 180){
        EDDsy -= 360;
            }
    if (EDDsy < -180){
        EDDsy += 180;
            }
    return EDDsx;
    return EDDsy;
    return as;
    }


