<script type="text/javascript">
$(document).ready(function(){
	const currencies = async() => {
		const response = await fetch('https://api.binance.com/api/v3/ticker/price');
		const json = await response.json();
		
		for ( let cur of json ){
			let btc = 0,
				eth = 0,
				ltc = 0;
			
			if ( cur['symbol'] == 'BTCUSDT' ){
				let btc = new Intl.NumberFormat('en-IN', { style: 'currency', currency: 'USD' }).format(cur['price']);
				$(".currencyBox.btc .value").text(btc).slideDown(300);
				
				btc++;
			}
			
			if ( cur['symbol'] == 'ETHUSDT' ){
				let eth = new Intl.NumberFormat('en-IN', { style: 'currency', currency: 'USD' }).format(cur['price']);
				$(".currencyBox.eth .value").text(eth).slideDown(300);
				
				eth++;
			}
			
			if ( cur['symbol'] == 'LTCUSDT' ){
				let ltc = new Intl.NumberFormat('en-IN', { style: 'currency', currency: 'USD' }).format(cur['price']);
				$(".currencyBox.ltc .value").text(ltc).slideDown(300);
				
				ltc++; 
			}
			
			if ( btc && eth && ltc ){
				break; 
			}
		} 
	}
	
	currencies();
});
</script>
