			{
				bg_id=images;
			}
			$("#logo_changerp").attr("rel", bg_id);
		});
		$("#logo_changern").click(function() {
			var bg_id = $("#logo_changerp").attr("rel");
			$(".header").css("backgroundImage", "url(" + "images/bgs/bg" + bg_id + ".png)");
