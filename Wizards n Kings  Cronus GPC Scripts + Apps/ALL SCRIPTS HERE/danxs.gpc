	if (SantiShape == Circular) {
		RYT = (get_val(RY)) + ((SinAngle * f_var))/100;
		RXT = (get_val(RX)) + ((CosAngle * f_var))/100;
			}
	if (SantiShape == TallOval) {
		RYT = (get_val(RY)) + ((SinAngle * f_var))/100;
		RXT = (get_val(RX)) + ((CosAngle * SantiShapeRangeMin))/100;
