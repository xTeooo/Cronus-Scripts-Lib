//IN GAME BUTTONS
define Ads             = PS4_L2;        // XB1_LT
define Fire            = PS4_R2;        // XB1_RT 
define Build           = PS4_UP;        // XB1_LS
define Pickaxe         = PS4_TRIANGLE;  // XB1_Y
define RX              = PS4_RX;        // XB1_RX
define RY              = PS4_RY;        // XB1_RY
define Off             = FALSE;
define On              = TRUE; 

//MENU MOD'S                                                                         //ON = TRUE / OFF = FALSE;// 
int AimAssistEXPO      = FALSE; // Aim Assist Exponential
define strength        = 18;    // Strength/value

int AimStickLinear     = TRUE; // Help with aiming moving the jyostick linear
define Ads_Assist      = TRUE; // Aim Assist Ads only 
define ist     = TRUE; // Aim Assist Fire only
define RADIUS          = 8;    // Radius/strength/value
define STEPS           = 30;   // Step/speed
define STEP_INTERVAL   = 1;    // time

int AntiRecoil         = TRUE;
define ARVert          = 20;   // More value, more force on the RY joystick, less value, less force on the RY jyostick
define ARHoriz         =  0;   // HIGHER VALUE +0 moves the crosshairs to the right, LOWER VALUE -0 moves the crosshairs to the left.

int BloomReducer   	   = FALSE;// While crouching the character will move back and forth, then the crosses get smaller by the minute, that's why we'll reduce the bloom.
int ast                = 8;  // The time while our character moves, against more value slower, less value more speed. (Hint: a value less than 100 is best. 

int BuildTracker       = TRUE; // When building, all the modifications will be deactivated so that they do not interfere, when returning to combat mode the modifications that we had activated will be activated again.
define BuildSyncTime   = 1000; // Auto De-Sync After 2 Seconds Of being Idol [Pickaxe == Manual Re-Sync]

int BlockRumble        = TRUE; // If we have vibration activated in the game, when activated, we would block the vibration in the controller.

int HairTriggers       = TRUE; // Instant Triggers [100%]

int PSonXBOX           = TRUE; // Touchpad Fix On PC or Xbox 

int AntirecoilVertical,AntirecoilHorizontal;
int BuildTrack,BuildCount,time;
int angle, cos_angle, sin_angle;
int actual_X, actual_Y, actual_Magnitude, max_Magnitude;

main {

  if(PSonXBOX && get_console() == PIO_XB360 && get_controller() == PIO_PS4) { swap (PS4_TOUCH,PS4_SHARE);}
                                                                                 
  if(BlockRumble) block_rumble();
  
  if(HairTriggers) { deadzone(PS4_L2,PS4_R2,100,100);}
  
  if(BuildTracker) {
  if(event_press(Build)) { BuildTrack = !BuildTrack;}
  if(BuildTrack) { Â¡BuildCount += get_rtime();
  if(event_press(Pickaxe) || BuildCount == BuildSyncTime) { BuildTrack = Off; BuildCount = 0;}
  if(get_ival(PS4_L1) || get_ival(PS4_L2) || get_ival(PS4_R2) || get_ival(PS4_R1) || get_ival(PS4_SQUARE)) BuildCount = 0;
   }
}
  if(!BuildTrack) {
  
  if(AntiRecoil) {
  if(get_ival(Ads) && get_ival(Fire)) { 	
  combo_run(Antirecoil);   
   }
}
  if(BloomReducer) {
  if(get_val(Ads) && (get_val(Fire))) {        
  combo_run(AntiBloom);
  }
  if(abs(get_val(PS4_LX)) > 40 || abs(get_val(PS4_LY)) > 40){
  combo_stop(AntiBloom);
      }
   }

  if(AimAssistEXPO) { 
  if(get_val(Ads)> 95) { 
  combo_run(AimEXPO);
   } 
}
  if(AimStickLinear) {
  actual_X = get_val(RX);
  actual_Y = get_val(RY);
  actual_Magnitude = isqrt(pow(actual_X, 2) + pow(actual_Y, 2));                                                                                                                                                                                              //CREATED BY EXPLOIT YT
  max_Magnitude = (actual_Magnitude < 100);
  
  if(!(time++ % STEP_INTERVAL)){
    angle += STEPS;
}
  angle = angle % 360;
  sin_angle = Polar_Array[angle % 360];
  cos_angle = Polar_Array[(angle + 270) % 360];                   
  cos_angle = (cos_angle * RADIUS) / 100;
  sin_angle = (sin_angle * RADIUS) / 100;
  
  if((Ads_Assist && get_val(Ads)) || (ist && get_val(Fire))){
  if(actual_Magnitude <= RADIUS){
  sin_angle -= actual_Y;
  cos_angle -= actual_X;
  }else {
  sin_angle = (sin_angle * (200 - ((abs(actual_Y) + actual_Magnitude) / 10) * 10) / 200) * max_Magnitude;
  cos_angle = (cos_angle * (200 - ((abs(actual_X) + actual_Magnitude) / 10) * 10) / 200) * max_Magnitude;
  }
  set_val(RX, clamp(actual_X + cos_angle, -100, 100));
  set_val(RY, clamp(actual_Y + sin_angle, -100, 100));
         }
      } 
   }
}

combo Antirecoil {
 
  AntirecoilVertical = get_val(PS4_RY) + ARVert;  
  if(AntirecoilVertical > 100) AntirecoilVertical = 100;
  if(abs(get_val(PS4_RY)) < abs(ARVert) + 5)
  set_val(PS4_RY, (AntirecoilVertical));
  AntirecoilHorizontal = get_val(PS4_RX) + ARHoriz;
  if(AntirecoilHorizontal > 100) AntirecoilHorizontal = 100;
  if(abs(get_val(PS4_RX)) < abs(ARHoriz) + 5)
  set_val(PS4_RX, AntirecoilHorizontal);
}

combo AimEXPO { 
 
  set_val(10, strength * -1); 
  wait(10) 
  set_val(9, strength); 
  wait(10) 
  set_val(10, strength); 
  wait(10) 
  set_val(9, strength * -1); 
  wait(10) 
}

combo AntiBloom { 
	
  set_val(PS4_LX,-40);
  wait(ast);
  set_val(PS4_LX, 40);
  wait(ast);
  if(ast < 160){ast = ast + 10;}
  else {ast = 100;
   }
}
