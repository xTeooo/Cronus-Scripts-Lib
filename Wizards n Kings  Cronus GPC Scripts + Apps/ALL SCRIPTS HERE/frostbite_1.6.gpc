//------------------------------------------------------------------------------
//------------------------------------------------------------------------------
define RT = 4; //R2/RB//--------------------------------------------------------
define LT = 7; //L2/LB//--------------------------------------------------------
//------------------------------------------------------------------------------
define RB= 3; //R1/RT//---------------------------------------------------------
define LB = 6; //L1/LT//--------------------------------------------------------
//------------------------------------------------------------------------------
define RS = 5; //R3/RS//--------------------------------------------------------
define LS = 8; //L3/LS//--------------------------------------------------------
//------------------------------------------------------------------------------
define A = 19; //PS4 CROSS/XB1 A//----------------------------------------------
define X = 20; //PS4 SQUARE/XB1 X//---------------------------------------------
define Y = 17;	//PS4 TRIANGLE/XB1 Y//------------------------------------------
define B = 18; //PS4 CIRCLE/XB1 B//---------------------------------------------
//------------------------------------------------------------------------------
define UP = 13; //Up//----------------------------------------------------------
define DOWN = 14; //Down//------------------------------------------------------
define LEFT = 15; //Left//------------------------------------------------------
define RIGHT = 16; //Right//----------------------------------------------------
//------------------------------------------------------------------------------
define START = 1; //VIEW/SHARE//------------------------------------------------
define BACK = 2; //Options/Menu//-----------------------------------------------
//------------------------------------------------------------------------------
define TOUCHPAD = 27; //Touchpad//----------------------------------------------
define RX = 9; //PS4 RX / Xbox RX//---------------------------------------------
define RY = 10; //PS4 RY / Xbox RY//--------------------------------------------
define LX = 11; //XB1_LX //PS4_LX//---------------------------------------------
define LY = 12; //XB1_LY //PS4_LY//---------------------------------------------
//------------------------------------------------------------------------------
int combat_alteration = FALSE;//------------------------------------------------
int CA1 = TRUE;//---------------------------------------------------------------
int CA2 = TRUE;//---------------------------------------------------------------
int EB = LS;//<------------change to your edit button---------------------------
int RSB = RS;//<------------change to your reset button-------------------------
//------------------------------------------------------------------------------
int BSVAL = 15;//<-------------Make this 2 or 3 higher than your deadZone number
//------------------------------------------------------------------------------
int rapidfire = FALSE;//--------------------------------------------------------
int DropShot = TRUE;//----------------------------------------------------------
int floot = TRUE;//-------------------------------------------------------------
int wallshot = TRUE;//----------------------------------------------------------
int lock = FALSE;//-------------------------------------------------------------
int togglebuild3 = TRUE;//------------------------------------------------------
int togglebuild2 = FALSE;//-----------------------------------------------------
int togglebuild = FALSE;//------------------------------------------------------
int toggle = TRUE;//------------------------------------------------------------
int toggle2 = TRUE;//-----------------------------------------------------------
int ARAB = TRUE;//--------------------------------------------------------------
int pump = FALSE;//-------------------------------------------------------------
int aimupAB = TRUE;//-----------------------------------------------------------
//------------------------------------------------------------------------------
main {   //start of end---------------------------------------------------------
//------------------------------------------------------------------------------
    if((get_val(LT)) && (event_press(RIGHT))) {
        toggle = !toggle;
        if(toggle) {
            combo_run(RumbleOnce);
    }  else {
            combo_stop(aimbot);
            combo_run(RumbleTwice);}
    }
    if(toggle) {
        if(get_val(LT)) {
            combo_run(aimbot);} }
//------------------------------------------------------------------------------
    if (togglebuild) {
    if(event_press(B)) {
        togglebuild3 = !togglebuild3;
        if(togglebuild3){
        pump = FALSE;
    }  else {
        pump = TRUE;} } }
//------------------------------------------------------------------------------
    if((get_val(LEFT)) && (event_press(RS))) {
        togglebuild2 = !togglebuild2;
        if(togglebuild2){
        togglebuild = TRUE;
        pump = TRUE;
        combo_run(RumbleOnce);
    }  else {
        pump = FALSE;
        togglebuild = FALSE;
        combo_run(RumbleTwice);} }
//------------------------------------------------------------------------------
     if(pump) {
        if(get_val(LT) && get_val(LB)) {
           set_val(LT, 0);
           set_val(LB, 0);
           combo_stop(aimbot);
           combo_run(build2);} }
//------------------------------------------------------------------------------
    if(pump) {
        if(get_val(LT) && get_val(RB)) {
           set_val(LT, 0);
           set_val(RB, 0);
           combo_stop(aimbot);
           combo_run(build1);} }
//------------------------------------------------------------------------------
    if((get_val(LT)) && (event_press(START))) {
        lock = !lock;
        if(lock) {
            combo_run(RumbleOnce); 
    }  else {
           combo_run(RumbleTwice);} }
//------------------------------------------------------------------------------
    if (lock){
       if (get_val(LT) && get_val(RT)){
       if (get_val(RX)<-15||get_val(RX)>15||get_val(RY)<-15||get_val(RY)>15||get_val(RX)<-15||get_val(RX)>15||get_val(RY)<-15||get_val(RY)>15){ 
           combo_run(Beta_Shake);} } }
//------------------------------------------------------------------------------
    if(lock){
    if (get_val(LT) && get_val(RT)) {
      aimupAB = !aimupAB;
      combo_run(aimup)   
          } else {
      combo_stop(aimup)}}
//------------------------------------------------------------------------------
if (get_val(RX)<-15||get_val(RX)>15||get_val(RY)<-15||get_val(RY)>15||get_val(RX)<-15||get_val(RX)>15||get_val(RY)<-15||get_val(RY)>15){ 
combo_stop(aimup);}
//------------------------------------------------------------------------------
    if (get_val(LT) && get_val(RT)) {
      ARAB = !ARAB;
      combo_run(AR)
          } else {
      combo_stop(AR)}
//------------------------------------------------------------------------------
if (get_val(RX)<-15||get_val(RX)>15||get_val(RY)<-15||get_val(RY)>15||get_val(RX)<-15||get_val(RX)>15||get_val(RY)<-15||get_val(RY)>15){ 
combo_stop(AR);}
//------------------------------------------------------------------------------
    if((get_val(LT)) && (event_press(LEFT))) {
        toggle2 = !toggle2;
        if(toggle2) {
            combo_run(RumbleOnce);
    }  else {
            combo_stop(xspam);
            combo_run(RumbleTwice);}
    }
    if(toggle2) {
        if(get_val(X)) {
            combo_run(xspam);} }
//------------------------------------------------------------------------------
    if((get_val(LT)) && (event_press(BACK))) {
        combat_alteration = !combat_alteration;
        if(combat_alteration) {
            combo_run(RumbleOnce); 
   }  else {
           combo_run(RumbleTwice);} }
//------------------------------------------------------------------------------
     if (combat_alteration){
    if (get_val(RIGHT)) {
      CA1 = !CA1;
      combo_run(QE);
    set_val(RIGHT, 0);
          } else {
      combo_stop(QE);}}
//------------------------------------------------------------------------------
     if (combat_alteration) {
    if (get_val(LEFT)) {
      CA2 = !CA2;
      combo_run(QR);}}
//------------------------------------------------------------------------------
    if (get_val(LT) && get_val(RT)) {
      rapidfire = !rapidfire;
      combo_run(rapid);
          } else {
      combo_stop(rapid);}
//------------------------------------------------------------------------------
    if (get_val(LT) && get_val(LB)) {
      DropShot = !DropShot;
      combo_run(GP);
    set_val(LB, 0);
          } else {
      combo_stop(GP);}
//------------------------------------------------------------------------------
    if (get_val(LT) && get_val(X)) {
      wallshot = !wallshot;
      combo_stop(xspam);
      combo_run(wallgp);
    set_val(X, 0);
          } else {
      combo_stop(wallgp);}
//------------------------------------------------------------------------------
//------------------------------------------------------------------------------
	}   //end of main-----------------------------------------------------------
//------------------------------------------------------------------------------
//---------------------------------combo code-----------------------------------
//------------------------------------------------------------------------------
combo aimbot {
    set_val(LT, 100);
    wait(300);
    set_val(LT, 0);
    wait(20);}
//------------------------------------------------------------------------------
combo Beta_Shake { 
    set_val(RY,BSVAL); 
    wait(10);
    set_val(RY,BSVAL); 
    wait(10);
    set_val(RX,BSVAL -1);
    wait(10);
    set_val(RX,BSVAL); 
    wait(10);} 
//------------------------------------------------------------------------------
combo QE {
    set_val(EB, 100);
    wait(150);
    set_val(EB, 0);
    wait(10);
    set_val(RT, 100);
    wait(40);
    set_val(RT, 0);
    wait(30);
    set_val(EB, 100);
    wait(150);
    set_val(EB, 0);
    wait(50);
    combo_stop(QE);}
//------------------------------------------------------------------------------
combo QR {
    set_val(EB, 100);
    wait(20);
    set_val(EB, 0);
    wait(10);
    set_val(RSB, 100);
    wait(20);
    set_val(RSB, 0);
    wait(10);
    set_val(EB, 100);
    wait(20);
    set_val(EB, 0);
    wait(10);
    combo_stop(QR);}
//------------------------------------------------------------------------------
combo xspam {
   set_val(X, 100);
    wait(80);
    set_val(X, 0);}
//------------------------------------------------------------------------------
combo build1 {
   call (buildF);
    set_val(RT, 100);
    wait(90);
    set_val(RT, 0);
    wait(60);
    set_val(B, 100);
    wait(80);
    set_val(B, 0);
    combo_stop (build1);}
//------------------------------------------------------------------------------
combo build2 {
   call (buildF);
    set_val(RB, 100);
    wait(90);
    set_val(RB, 0);
    wait(30);
    set_val(B, 100);
    wait(80);
    set_val(B, 0);
    combo_stop (build2);}
//------------------------------------------------------------------------------
combo AR {
    set_val(RY,26);
    wait(10);
    set_val(RY,0);
    wait(10);}
//------------------------------------------------------------------------------
combo aimup {
    call (AU1);
    call (AU1);
    call (AU1);
    call (AU2);
    call (AU2);}
//------------------------------------------------------------------------------
combo AU1 {
    set_val(RY,44);
    wait(10);
    set_val(RY,0);
    wait(10);}
//------------------------------------------------------------------------------
combo AU2{
    set_val(RY, -34);
    wait(10);
    set_val(RY,0);
    wait(10);}
//------------------------------------------------------------------------------
combo buildF {
    set_val(RT, 100);
    wait(140);
    set_val(RT, 0);
    wait(50);
    set_val(B, 100);
    wait(100);
    set_val(B, 0);}
//------------------------------------------------------------------------------
combo GP {
    set_val(RS, 100);
    wait(46);
    set_val(RS, 0);
    wait(46)}
//------------------------------------------------------------------------------
combo rapid {
    set_val(RT, 40);
    wait(40);
    set_val(RT, 0);
    wait(30);}
//------------------------------------------------------------------------------
combo wallgp {
    set_val(RS, 100);
    wait(15);
    set_val(RT, 0);
    wait(15);
    set_val(RT, 100);
    wait(15);
    set_val(RT, 0);
    wait(50);
    set_val(RS,100);
    wait(15);
    set_val(RS, 0);}
//------------------------------------------------------------------------------
combo RumbleOnce {
    set_rumble(RUMBLE_A, 100);
    wait(250);
    reset_rumble();}
//------------------------------------------------------------------------------
combo RumbleTwice {
    set_rumble(RUMBLE_A, 100);
    wait(250);
    set_rumble(RUMBLE_A, 0);
    wait(250);
    set_rumble(RUMBLE_A, 100);
    wait(250);
    reset_rumble();}
//------------------------------------------------------------------------------







































































































































