				if(get_val(ADS)> 95)
				{       
					combo_run(AimAbusseScope);
				} 	

				if(event_release(ADS))
				{ 
					combo_stop(AimAbusseScope);
				}
		}
 		
	if (get_ival(PS4_L2))
	{
		if(event_press(PS4_L3))
		{
			HoldBreath = !HoldBreath; // On/Off
		}
			set_val(PS4_L3, 0);
	}

		if(HoldBreath)
		{
			{
				combo_run(Bliiink);
			}
				if(get_val(ADS))
					set_val(SPRINT, 100);
				if(get_val(WALK) <-90 && get_val(ADS) && get_ptime(ADS) > 200)
					combo_stop(HoldBreath);       
				if(event_press(ADS)) 
					combo_run(HoldBreath);
				if(get_val(ADS) && get_lval(SPRINT)) 
					set_val(SPRINT, 0);  
		}

 		//JUMP SHOT
 		if (get_ival(PS4_L2) && event_press(PS4_LEFT)) {
	        						
	        	JumpShot = !JumpShot; // On/Off  
	            set_val(PS4_LEFT,0);
	}
				
			
				if(JumpShot) 
		{
				{combo_run(Bliiiink);}
		
				if(get_val(FIRE))
								combo_run(JumpShot);
				
	}
	
 
	//DROP SHOT
 		
 			if(get_ival(PS4_L2)){ 
       		if(event_press(PS4_RIGHT)) {
           					DropShot = !DropShot; // On/Off
        }
        					set_val(PS4_RIGHT,0);
             }
             
    		if(DropShot){
    						{combo_run(Bliiiink);}
        	if(get_val(ADS) && get_val(FIRE) && event_press(CROUCH)) {
            				DropShotI = TRUE;
        }    
        	if(DropShotI){
            combo_run(Drop);
        }
        	if(event_release(ADS) || event_release(FIRE)){
            				DropShotI = FALSE;
            				combo_stop(Drop);
        }         
    }
 		
 	

	if(CancelSlide == 1 || CancelSlide == 2 || CancelSlide == 3 || CancelSlide == 4)
		if((get_val(WALK) < -80) || (get_val(WALK) > 80) || (get_val(STRAFE) < -80) || (get_val(STRAFE) > 80))
		{
			if(CancelSlide == 1 && event_release(CROUCH))
				combo_run(CancelSlideMW)
			else if(CancelSlide == 2 && event_release(CROUCH))
				combo_run(CancelSlideCW) 
			else if(CancelSlide == 3 && event_release(CROUCH))
				combo_run(CancelSlideAA)
			else if(CancelSlide == 4 && event_release(CROUCH))
				combo_run(CancelSlideAAcoldwar)
			{		
				if(CancelSlide == 1 && combo_running(CancelSlideMW) && (get_val(ADS) && event_release(CROUCH)))
					combo_stop(CancelSlideMW)
				if(CancelSlide == 2 && combo_running(CancelSlideCW) && (get_val(ADS) && event_release(CROUCH)))
					combo_stop(CancelSlideCW)
				if(CancelSlide == 3 && combo_running(CancelSlideAA) && (get_val(ADS) && event_release(CROUCH)))
					combo_stop(CancelSlideAA)
				if(CancelSlide == 4 && combo_running(CancelSlideAAcoldwar) && (get_val(ADS) && event_release(CROUCH)))
					combo_stop(CancelSlideAAcoldwar)	
			}
		}

    //LOGO
	if(display_title)
	{           
		cls_oled(0);            
		DrawLogo(0, 0, 0);
		display_title = FALSE;  
		display_black = TRUE;
	}                             

		if(display_black)
		{                           
			count_black += get_rtime();

			if(count_black >= 9000)
			{                        
				cls_oled(0);         
				count_black = 0;      
				display_black = FALSE; 
			}
		}

	if (PlayStationOnXbox && get_console() == PIO_XB360 && get_controller() == PIO_PS4)
	{
		swap(PS4_TOUCH,PS4_SHARE);
		}
	
	} // End Of Drive Mode
}

