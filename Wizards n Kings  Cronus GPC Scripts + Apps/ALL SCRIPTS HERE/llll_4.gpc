function DoGzGetPolar(Current, Received) {
    Output = Current / 10 + Received / 10;

    if(Output <= -3276)
        return -32768;

    if(Output >=  3276)
        return  32767; 

        return Current + Received; 
} 

function DoGzPolarAR(Rotation, Radial) {
    set_polar(POLAR_RS, Rotation, Radial);
