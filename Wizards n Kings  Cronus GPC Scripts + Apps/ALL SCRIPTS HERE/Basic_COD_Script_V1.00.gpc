// ❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤  START ANTI-RECOIL SRIPT CODE HERE  ❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤❤

main {
    // ❤❤❤❤❤  DECLARE RAPID FIRE SCRIPT BELOW  ❤❤❤❤❤
    if (get_val(rapidToggleHold)) {
    	set_val(rapidTogglePress,0);
    	if (event_press(rapidTogglePress)) {
    		rapidFire = !rapidFire;
    		combo_run(rumble);
    	}
    }
    
    if (rapidFire) {
		if (rapidHip)
		{
			if (get_ival(Fire))
			{
				combo_run(rapidFire);
				set_rgb(0,200,0);
			}
		}
		else if (get_ival(ads) && get_ival(Fire))
		{
			combo_run(rapidFire);
			set_rgb(0,200,0);
		}
		else
		{
			combo_stop(rapidFire);
			set_rgb(255,0,0);
		}
	}
    
    // ❤❤❤❤❤  DECLARE BUNNY SHOT SCRIPT BELOW  ❤❤❤❤❤
     if (get_ival(bunnyToggleHold) && event_press(bunnyTogglePress)) {
    	set_val(BunnyShot,0);
    	if (event_press(BunnyShot)) {
    		BunnyShot = !BunnyShot;
    		combo_run(rumble);
    	}
    }
    
    if (BunnyShot)
		{
			if (get_ival(Fire))
			{
				combo_run(BunnyShot);
				set_rgb(0,200,0);
			}
		}
		else if (get_ival(ads) && get_ival(Fire))
		{
			combo_run(BunnyShot);
			set_rgb(0,200,0);
		}
		else
		{
			combo_stop(BunnyShot);
			set_rgb(255,0,0);
		}
    
    // ❤❤❤❤❤  DECLARE ANTI-RECOIL SCRIPT BELOW  ❤❤❤❤❤
    if (get_val(antiToggleHold)) {
    	set_val(antiTogglePress,0);
    	if (event_press(antiTogglePress)) {
    		legacyRecoil = !legacyRecoil;
    		combo_run(rumble);
    	}
    }
    
    if (legacyRecoil)
	{	
		if (legacyHipFire && get_ival(Fire))
		{
			legacyRecoil();
		}
		else if (get_ival(ads) && get_ival(Fire))
		{
			legacyRecoil();
		}
	}
    
    // ❤❤❤❤❤  DECLARE AIM-ASSIST SCRIPT BELOW  ❤❤❤❤❤
    if (get_val(aimToggleHold)) {
    	set_val(aimTogglePress,0);
    	if (event_press(aimTogglePress)) {
    		aimAssist = !aimAssist;
    		combo_run(rumble);
    	}
    }
    
    if (aimAssist) 
	{
		if (get_ival(ads)) 
		{
			combo_run(aimAssist);
			set_rgb(0,200,0);
		}
	}
	
	else {
		combo_stop(aimAssist);
		set_rgb(255,0,0);
	}
}

// ❤❤❤❤❤  COMBO FOR RAPID FIRE SCRIPT BELOW  ❤❤❤❤❤

combo rapidFire {
	set_val(Fire,100);
	wait(rapidFireHold);
	set_val(Fire,0);
	wait(rapidFireWait);
}

combo BunnyShot {
	set_val(PS4_CROSS,100);
	wait(BunnyShotHold);
	set_val(PS4_CROSS,0);
	wait(BunnyShotWait);
}

combo rumble {
	set_rumble(RUMBLE_B,20);
	wait(150);
	reset_rumble();
}

// ❤❤❤❤❤  COMBO AIM ASSIST SCRIPT BELOW  ❤❤❤❤❤

combo aimAssist {
	// THIS SECTION OF CODE WILL GO WITH THE AIM ASSIST CODE THAT HAS BEEN PLACED IN THE MAIN FUNCTION SECTION & WE CAN CALL IT FROM HERE.
	set_val(aimY,get_ival(aimY) + inv(strength));
		wait(delayMs);
	set_val(aimX,get_ival(aimX) + strength);
		wait(delayMs);
	set_val(aimY,get_ival(aimY) + strength);
		wait(delayMs);
	set_val(aimX,get_ival(aimX) + inv(strength));
		wait(delayMs);
}

// ❤❤❤❤❤  FUNCTIONALITY FOR ANTI-RECOIL SCRIPT BELOW  ❤❤❤❤❤

function legacyRecoil () {
    // THIS SECTION OF CODE WILL GO WITH THE ANTI-RECOIL CODE THAT HAS BEEN PLACED IN THE MAIN FUNCTION SECTION & WE CAN CALL IT FROM HERE.
	if (invertedYAxis)
	{	
		set_Val(aimY,inv(legacyRY));
		set_Val(aimX,legacyRX);
	}
	
	else
	{
		set_Val(aimY,legacyRY);
		set_Val(aimX,legacyRX);
	}
}

function set_Val(Input,Output) {
	set_val(Input,clamp(Output * (100 - abs(get_ival(Input))) / 100 + get_ival(Input),-100,100));
	return;
}

function center_x(f_chars,f_font){                                               
    return (OLED_WIDTH / 2) - ((f_chars * f_font) / 2);
}                                                      