/* ❤❤❤❤❤ */ define strength = 20; /* ❤❤❤❤❤ */
/* ❤❤❤❤❤ */ define delayMs = 20; /* ❤❤❤❤❤ */
/* ❤❤❤❤❤ */ int rapidFire = on; /* ❤❤❤❤❤ */
/* ❤❤❤❤❤ */ define rapidHip = on; /* ❤❤❤❤❤ */
/* ❤❤❤❤❤ */ define rapidFireHold = 40; /* ❤❤❤❤❤ */
/* ❤❤❤❤❤ */ define rapidFireWait = 40; /* ❤❤❤❤❤ */
/* ❤❤❤❤❤ */ define BunnyShotHold = 40; /* ❤❤❤❤❤ */
/* ❤❤❤❤❤ */ define BunnyShotWait = 40; /* ❤❤❤❤❤ */
/* ❤❤❤❤❤ */ int BunnyShot = FALSE; /* ❤❤❤❤❤ */

