/* ❤❤❤❤❤ */ define Down = PS4_CROSS; /* ❤❤❤❤❤ */
/* ❤❤❤❤❤ */ define on = TRUE; /* ❤❤❤❤❤ */
/* ❤❤❤❤❤ */ define off = FALSE; /* ❤❤❤❤❤ */
/* ❤❤❤❤❤ */ int legacyRecoil = on; /* ❤❤❤❤❤ */
/* ❤❤❤❤❤ */ define invertedYAxis = off; /* ❤❤❤❤❤ */
