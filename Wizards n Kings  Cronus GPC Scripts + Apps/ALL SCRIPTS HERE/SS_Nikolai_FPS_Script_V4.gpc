			} 
			else 
			{
				double__tap[button] = -1;
				return 1;
			}
		}
	return 0;
}

function NumberToString(Value, f_Digits, X) {
	i 			 =    0x1;
	digitindex_1 = 0x2710;

	if (Value < 0)
	{
		putc_oled(i, 45);
		i += 1;
		Value = abs(Value);
	}

		for (charindex_1 = 5; charindex_1 >= 1; charindex_1--)
		{
			if (f_Digits >= charindex_1)
			{
				putc_oled(i, (Value / digitindex_1) + 48);
				Value %= digitindex_1;
				i++;

				if (charindex_1 == 4)
				{
					putc_oled(i, 44);
					i++;
				}
			}
				digitindex_1 /= 0xA;
		}
			puts_oled(CenterX(i - 1, OLED_FONT_MEDIUM_WIDTH), X, OLED_FONT_MEDIUM, i - 1, OLED_WHITE);
}

