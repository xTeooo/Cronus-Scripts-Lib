combo RGB {
	wait(10);

	set_rgb(Colour, Colour[1], Colour[2]);

	Colour[Dec]--; 
	Colour[Inc]++;

	if(!Colour[Dec])
	{
		Inc = (Inc + 1) % 3; 
		Dec = (Dec + 1) % 3; 
	}
}

combo Vibrate {
	set_rumble(RUMBLE_A, 85);
	wait(225);
	reset_rumble();
}

