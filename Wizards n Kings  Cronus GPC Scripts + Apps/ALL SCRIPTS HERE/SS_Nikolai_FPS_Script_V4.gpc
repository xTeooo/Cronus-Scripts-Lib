init {
	Init_Script();
	InitializeShapeChange();

	if(get_pvar(SPVAR_64, 0, 10, 5) != 9) 
	{
		set_pvar(SPVAR_64, 9):
			Reset_Script_Vars();
			Save();
	}

	Load_PVARs();
}
/*-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+
 | | | | | | | | |M|A|I|N| | |S|E|C|T|I|O|N| | | | | S | S | _ | N | i | k | o | l | a | i | | | | | | |
 +-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+*/
main {
	Buttons(LayOut);
	ControllerRSLS(Sticks);
	PS4CrossOver();

	//AUTO RUN - START
	if(USE_AUTO_RUN && !AutoRun_KS)
	{
		if(USE_AUTO_RUN == 1)
		{
			SPRINTING = TRUE;
		}
		else if(USE_AUTO_RUN == 2)
		{
			if (!get_val(ADS) && get_val(WALK) < -THRESHOLD)
			{
				combo_run(cRun);
			}

			if (get_val(SPRINT) && get_val(WALK) < -THRESHOLD)
			{
				SPRINTING = TRUE;
			}

				if(SPRINTING)
				{
					if(get_ival(WALK) > -THRESHOLD)
					{
						SPRINTING = FALSE;
					}
				}
		}
		else if (USE_AUTO_RUN == 3)
		{
			if(!get_ival(ADS) && (!combo_running(cCancelSlideMW) || combo_running(cCancelSlideCW)) && get_ival(WALK) <= -67 && isqrt(pow(abs(get_ival(STRAFE)), 2) + pow(abs(get_ival(WALK)), 2)) >= 97)
				combo_run(cRun);
		}
	}

		if(event_press(RELOAD))
		{
			combo_run(cStopSprint);
		}

		if (event_press(ADS) || event_press(FIRE))
		{
			combo_stop(cStopSprint);
			AutoRun_KS = FALSE;
		}
	//AUTO RUN - END

	//HAIR TRIGGERS -  START
	if(Hair_Triggers)
	{
		if(get_ival(PS4_R2) > 10) // We don't assign these as ADS, FIRE variable because ONLY L2 and R2 can have a hair triggers to them
			Set(PS4_R2, 100);

		if(get_ival(PS4_L2) > 10)
			Set(PS4_L2, 100);
	}
	//HAIR TRIGGERS - END

	if(Kill_Switch_Toggle && get_ival(KILL_SWITCH_BUTTON) && Double_Click(KILL_SWITCH_BUTTON_1))
	{
		KillSwitch = !KillSwitch;
		frame      = -1;
		combo_restart(NikO);
		display    = TRUE;
	}

	if(!KillSwitch)
	{
		if(ZS == 0)
		{

	    //RAPIDFIRE - START
		if(QUICK_TOGGLES)
		{
			if(get_ival(ADS) && event_press(RAPIDFIRE_QT))
			{
				Rapid_Fire_Toggle = !Rapid_Fire_Toggle;
				LedBlink(Rapid_Fire_Toggle);
				combo_run (Vibrate);
			}
		}
			if(Rapid_Fire_Toggle)
			{
				if(get_ival(FIRE) && get_ptime(FIRE) >= 80)
				{
					combo_run(cRapidFire);	
				}

				if(event_release(FIRE))
				{
					combo_stop(cRapidFire);
				}
				else if(Rapid_Fire_Toggle == 2)
				{
					if(get_ival(FIRE) && get_ptime(FIRE) >= 80)
					{
						combo_run(cAkimboRapid);
					}

					if(event_release(FIRE))
					{
						combo_stop(cAkimboRapid);
					}
				}
				else if(Rapid_Fire_Toggle == 3)
				{
					if(get_ival(FIRE) && get_ptime(FIRE) >= 80)
					{
						combo_run(cBurstFire);
					}

					if(event_release(FIRE))
					{
						combo_stop(cBurstFire);
					}
				}
			}
		//RAPIDFIRE - END

		//EVIL POLAR - START
		C_RX = get_ival(AIM_X);
		C_RY = get_ival(AIM_Y);

		if(get_val(ADS) || get_val(FIRE))
		{
			if(update_timer >= REFRESH_RATE)
			{
				block(AIM_X, REFRESH_RATE);
				block(AIM_Y, REFRESH_RATE);

				_RET = Get_Mvt_Direction(C_RX, L_RX);

				if(_RET != 0)
					mvt_direction = _RET;

					update_timer = 0;

					L_RX = C_RX;
					L_RY = C_RY;
			}

		if((AR_Type) && ((get_val(ADS)) && (get_val(FIRE))) && Check_Magnitude(MAGNITUDE, C_RX, L_RX, C_RY, L_RY))
		{
			if(AR_Type == 1)
			{
				AR_RY = Rumble_EviL_AR();
			}
			else if(AR_Type == 2)
			{
				AR_RY = Dynamic_EviL_AR(FIRE, AR_Strength, C_RY, C_RX);
			}

                if(inverted == -1)
                    C_RY += inv(AR_RY);
                else
                    C_RY += AR_RY;
		}

		if(Sweet_EviL_Polar)
		{
			if(mvt_direction == 1)
			{
				C_RX += Get_Polar_Coordinate(random(MIN_RADIUS, MAX_RADIUS), SIN_(angle));
				C_RY += Get_Polar_Coordinate(random(MIN_RADIUS, MAX_RADIUS), COS_(angle));
			}
			else
			{
				C_RX += Get_Polar_Coordinate(random(MIN_RADIUS, MAX_RADIUS), COS_(angle));
				C_RY += Get_Polar_Coordinate(random(MIN_RADIUS, MAX_RADIUS), SIN_(angle));
			}
				angle = ((angle + random(MIN_ANGLE, MAX_ANGLE)) % 360);
		}
				Set(AIM_X, clamp(C_RX, -100, 100));
				Set(AIM_Y, clamp(C_RY, -100, 100));

				update_timer += get_rtime();
			}
			else
			{
				update_timer = 0;
				update_timer = REFRESH_RATE;
				angle		 = 90 * inverted;

				Set(AIM_X, C_RX);
				Set(AIM_Y, C_RY);
			}
		//EVIL POLAR - END

		//AIM ABUSE - START
		if(Aim__Abuse == TRUE)
		{
			Hair_Triggers = FALSE;

			if(get_ival(ADS) > 95) 
			{
				combo_run(cAimAbuse); 
			}

			if(event_release(ADS))
			{ 
				combo_stop(cAimAbuse); 
			}
		}
		else 
		{
			Hair_Triggers = TRUE;
		}
		//AIM ABUSE - END
		
		//DROPSHOT - START
		if(QUICK_TOGGLES)
		{
			if(get_ival(ADS) && event_press(DROPSHOT_QT))
			{
				DropShot = !DropShot;
				LedBlink(DropShot);
				combo_run (Vibrate);
			}
		}
			if(DropShot == 1)
			{
				if(get_ival(FIRE))
				{
					combo_run(cDropShot);
				}
			}
			else if(DropShot == 2)
			{
				if(event_release(FIRE))
				{
					DropShotVariable = FALSE;
				}

				if(!KillSwitch)
				{
					if(get_val(FIRE) && event_press(CROUCH))
					{
						DropShotVariable = TRUE;
					}
				}

				if(!KillSwitch)
				{
					if(DropShotVariable)
					{
						if(get_ival(FIRE))
						{
							combo_run(cDropShot);
						}
					}
				}
			}
		//DROPSHOT - END

		//DNAM Polar With Taylor Shapes - START
	if(TaylorShapes)
	{
		CycleRefresh += get_rtime();

		if(AngleDirection)
		{
			AngleDirection = (get_val(AIM_X) > 0);
		}

		if((abs(get_val(AIM_X)) < 10) * (abs(get_val(AIM_Y)) < 10))
			Angle = (Angle + ADSSpeed) % 360;
		else
			Angle = (Angle + TaylorAngle) % 360;

		if(get_ival(ADS) || get_ival(FIRE))
		{
			DoNotAtMePolar(POLAR_RS, Angle, TaylorRadius);
		}
	}
		//DNAM Polar With Taylor Shapes - END

		//Crescens Polar - START
		Axis = isqrt(abs(get_ival(AIM_X)) * abs(get_ival(AIM_X)) + abs(get_ival(AIM_Y)) * abs(get_ival(AIM_Y)));

		if(CrescensPolar)
		{
			if(get_ival(FIRE))
			{
				if(CrescensRadiusBoost)
				{
					RadIus_ = _PolarRadius + _PolarRBoost;
				}

				if(CrescensSpeedBoost)
				{
					AngLe_ = _PolarAngle + _PolarABoost;
				} 
			}
			else
			{
				RadIus_ = _PolarRadius;
				AngLe_  = _PolarAngle;
			}

			if (get_ival(ADS) || get_ival(FIRE))
			{
				if (Axis <= 15)
				{
					set_polar(POLAR_RS, SpEed_ = (SpEed_ + AngLe_) % 360, (RadIus_ * 328) + get_polar(POLAR_RS, 0));
				}
				else
				{
					combo_run(cPolar);
				}
			}
		}
		//Crescens Polar - END

		//STRAFE ASSIST - START
		if(ToggleSpeedStrafe)
		{
			if(get_ival(FIRE) && get_ptime(FIRE) >= 80)
			{
				Angle_     += get_rtime();
				StrafeAngle = (StrafeAngle  + (Angle_ / 100)) % 360;
				combo_run(cStrafeAA);
			}
		}
		//STRAFE ASSIST - END

		//FAST MELEE - START
	if(Fast_Melee)
	{
		if(get_val(MELEE))
			combo_run(cTurboMelee);

		if(get_val(ADS) && event_press(MELEE) < 500)
			combo_stop(cTurboMelee);
	}
		//FAST MELEE - END

		//SMART GRENADE - START
	if(Smart_Grenade)
	{
		if(combo_running(cCookAndRelease) && (cooktime == smartgrenade_throwdelay) && (Main_Button_Pressed(LETHAL) || Double_Click(LETHAL)))
		{
			combo_stop(cCookAndRelease);
		}
		if(get_ival(LETHAL) && get_ptime(LETHAL) < 200)
		{
			cooktime = smartgrenade_throwdelay;
			combo_run(cCookAndRelease);
		}
		if(get_ival(LETHAL) && get_ptime(LETHAL) > 200)
		{
			cooktime = smartgrenade_safedelay;
			combo_run(cCookAndRelease);
		}
	}
		//SMART GRENADE - END

		//STRAFE - START
	if(Strafe_Toggle)
	{
		if(get_ival(FIRE) && get_ptime(FIRE) >= 80)
		{
			combo_run(cStrafe);
		}
	}
		//STRAFE - END

		//BUNNY HOP - START
		if(QUICK_TOGGLES)
		{
			if(get_ival(ADS) && event_press(BUNNYHOP_QT))
			{
				B_Hop = !B_Hop;
				LedBlink(B_Hop);
				combo_run (Vibrate);
			}
		}
			if(B_Hop)
			{
				if (get_ival(JUMP) && get_ptime(JUMP) > 200)
					combo_run(cBunnyHop);
				else 
					combo_stop(cBunnyHop);
			}
		//BUNNY HOP - END
		
		//QuickScope - START
	if(QuickScope)
	{
		if(QuickScope == 1)
		{
			if(Trigger_Toggle == TRUE && get_ival(RELOAD) || Trigger_Toggle == TRUE && get_ival(LETHAL))
			{
				Unset(RELOAD);
				Unset(LETHAL);
			}
			else if(event_release(RELOAD) || event_release(LETHAL))
			{
				Trigger_Toggle = FALSE;
			}

			if(event_press(ADS))
			{ 
				Trigger_Toggle = TRUE; 
			}
			else if(get_ival(ADS) && get_ptime(ADS) > 140)
			{
				Set(SPRINT, 100); 
			}
			else if(Trigger_Toggle == TRUE && event_release(ADS))
			{
				combo_run(cQuickScope);
			} 
				if(event_press(SPRINT))
				{
					combo_run(cHoldBreath_);
				}
			}
			else if(QuickScope == 2)
			{
				if(event_release(ADS) && get_ptime(ADS) <= 250)
				{
					combo_run(cQuickScope);
				}
			}
	}
		//QuickScope - END

		//FAST RELOAD - START
		if(Fast_Reload && Double_Click(RELOAD))
		{
			combo_run(cFastReload);
		}
		//FAST RELOAD - END

		//PERFECT ACCURACY - START
	if(PerfectAccuracy)
	{
		if(get_ival(ADS) > 80 && get_ival(LETHAL) > 10)
		{
			Unset(LETHAL);
			newHold =       40;
			newRest =      150;
			combo_run(cPerfectionAccuracy);
		}

		if(get_ival(ADS) < 80 && get_ival(ADS) > 10 && get_ival(LETHAL) > 10)
		{
			Unset(LETHAL);
			newHold = burstHold;
			newRest = burstRest;
			combo_run(cPerfectionAccuracy);
		}
	}
		//PERFECT ACCURACY - END

		//SCOPESHOT - START
		if(ScopeShot)
		{
			if(get_ival(FIRE) && !get_ival(ADS))
				Set(ADS, 100); 
		}
		//SCOPESHOT - END

		//CROUCH RISE - START
		if(Prone_Rise)
		{
            if(get_ival(FIRE) && get_ptime(FIRE) >= 80)
                combo_run(cProneRise);
		}
	    //CROUCH RISE - END

		//STRAFE JUMP - START
		if(Strafe_Jump)
		{
			if(get_ival(FIRE) && get_ptime(FIRE) >= 80)
			{
				combo_run(cJumpyShot);
				combo_run(cStrafe);
			}
		}
		//STRAFE JUMP - END
	    
	    //CHICKEN MODE - START
		if (Chicken_Mode)
		{
			if(get_ival(FIRE) && get_ptime(FIRE) >= 80)
			{
				combo_run(cStrafe);
				combo_run(cDropping_Shot);
			}
		}
		//CHICKEN MODE - END	  
	  		
	  	//HOLDBREATH - START
		if(HoldBreath)
		{
			if(get_ival(ADS))
				Set(SPRINT, 100);

			if(get_ival(WALK) < -90 && get_ival(ADS) && get_ptime(ADS) > 200)
				combo_stop(cHoldBreath);       

			if(event_press(ADS)) 
				combo_run(cHoldBreath);

			if(get_ival(ADS) && get_lval(SPRINT)) 
				Unset(SPRINT);  
		}
		//HOLDBREATH - END

		//SLIDECANCLE - START
		if(Slide_Cancel_Mode)
		{
	        if((get_ival(WALK) < -THRESHOLD) || (get_ival(WALK) > THRESHOLD) || (get_ival(STRAFE) < -THRESHOLD) || (get_ival(STRAFE) > THRESHOLD))
			{
				SlideEnable = 0;

