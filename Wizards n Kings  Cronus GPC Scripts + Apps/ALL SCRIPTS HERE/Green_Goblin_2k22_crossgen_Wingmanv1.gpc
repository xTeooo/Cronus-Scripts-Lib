int SpotUp = 442; int dunkmetertime = 1000;	int AutoDunk	int BlockOutput																

int Offline;int FIND = 1;const string SHOW1 = "CATCH&SHOOT"; const string SHOW2 = "DunkMeter";

const string LINE1 = "NBA 2K22";const string LINE2 = "LockDown";const string LINE3 = "Created By";const string LINE4 = "Green Goblin";

init{  
	SpotUp = get_pvar(PVAR_1,10,3000,SpotUp);
	Offline = get_pvar(PVAR_2, 1,1,Offline);
	printf(center_x(sizeof(LINE1) - 1, OLED_FONT_MEDIUM_WIDTH),5,OLED_FONT_MEDIUM,OLED_WHITE,LINE1[0]);//-- Display Script Title          
    printf(center_x(sizeof(LINE2) - 1, OLED_FONT_MEDIUM_WIDTH),25,OLED_FONT_MEDIUM,OLED_WHITE,LINE2[0]);//-- Display Script Title          
    printf(center_x(sizeof(LINE3) - 1, OLED_FONT_SMALL_WIDTH),45,OLED_FONT_SMALL,OLED_WHITE,LINE3[0]);//-- Display Script Title          
    printf(center_x(sizeof(LINE4) - 1, OLED_FONT_SMALL_WIDTH),55,OLED_FONT_SMALL,OLED_WHITE,LINE4[0]);//-- Display Script Title 
}

int LockDownMode;

define MAX_RANGE = 330;

main {

// -- Advanced Defense
	 if(get_val(PS4_L2) && get_val(PS4_R2)) 
	{
		if(FindMax(abs(get_val(PS4_LX)),abs(get_val(PS4_LY))) > 15) 
		{
			LockDownMode =  MAX_RANGE - (FindMax(abs(get_val(XB1_LX)),abs(get_val(XB1_LY))) * 3);
			combo_run(LockDownMode);	
		}
			
        }


	vm_tctrl(10);// SCRIPT SPEED USE -9 OR 10 IF A DELAY DEFAULT IS SET TO 10
	cls_oled(0);
   	set_val(PS4_ACCX,0);
	if(Offline == FALSE){
	set_led(LED_1, 0);
	set_led(LED_2, 2);
	set_led(LED_3, 0);
	set_led(LED_4, 0);
	}
	else if(Offline == TRUE){
	set_led(LED_1, 0);
	set_led(LED_2, 0);
	set_led(LED_3, 2);
	set_led(LED_4, 0);
	}
	
	 if(BlockOutput){ //----block output
		    if(event_release(BlockOutput)){
			    BlockOutput = 0;
		    }else{
		        set_val(BlockOutput,0);
			}
	    }
	 //--Auto Dunk
	    if(event_press(XB1_RB))
				{  
					combo_run(AutoDunk); 
				}
		
		    	
	if(event_press(PS4_SQUARE)){
		set_val(PS4_SQUARE,100);
 		combo_run(RayAllen);
 	}	
    if(get_val(PS4_L2) && get_val(PS4_R2)){
    	if(event_press(PS4_DOWN) && Offline == TRUE){
    		set_val(PS4_DOWN,0);
    		Offline = FALSE;
            SpotUp = SpotUp - 150; 
    		combo_run(RUMBLE);	
        }
        if(event_press(PS4_UP) && Offline == FALSE){
    		set_val(PS4_UP,0);
    		Offline = TRUE;
        	SpotUp = SpotUp + 150; 
			combo_run(RUMBLE);
        }
 	}	
	if(get_val(PS4_SQUARE)){
		printf(5,5,1,1,SHOW1[0]);
		Get(SpotUp,Finds(SpotUp));
		if (event_press(PS4_UP)){
			set_val(PS4_UP,0);		
			SpotUp += 10;
		}
		if (event_press(PS4_DOWN)){
			set_val(PS4_DOWN,0);		
			SpotUp -= 10;
		}
		if (event_press(PS4_LEFT)){
			set_val(PS4_LEFT,0);		
			SpotUp -= 1;
		}
		if (event_press(PS4_RIGHT)){
			set_val(PS4_RIGHT,0);		
			SpotUp += 1;
		}
	} 	
	set_pvar(PVAR_1, SpotUp);
    set_pvar(PVAR_2, Offline);
   
    } 

combo RayAllen{
	set_val(PS4_SQUARE,100);
	wait(SpotUp);
	set_val(PS4_SQUARE,0);
	wait(10);
}
combo RUMBLE{
	set_rumble(RUMBLE_A,100);
	set_rumble(RUMBLE_B,100);
	wait(100);
	reset_rumble();
}
combo LockDownMode{
    set_val(XB1_LT,100);
    set_val(XB1_RT,0);
    wait(30);
    set_val(XB1_LT,0);
    set_val(XB1_RT,0);
    wait(LockDownMode);
    set_val(XB1_RT,100);
    set_val(XB1_LT,0);
    wait(30);
    set_val(XB1_LT,0);
    set_val(XB1_RT,0);
    wait(LockDownMode);
}

combo AutoDunk{
	set_val(XB1_RY, -100);
	set_val(XB1_RX, +0);
	wait(dunkmetertime);
	}

function FindMax(a,b) {
    if(a > b){
        return a;
    }
    return b;
    }
    function center_x(f_chars,f_font){  //--  center_x(number of chars,font size);                                                 
    return (OLED_WIDTH / 2) - ((f_chars * f_font) / 2); //-- return X for Centering String Horizontally
}                 
function Get(Number,I_Got){if(I_Got >= 5){putc_oled(FIND,(Number / 10000) + 48);Number %= 10000;FIND++;}if(I_Got >= 4){putc_oled(FIND,(Number / 1000) + 48);Number %= 1000;FIND++;}if(I_Got >= 3){putc_oled(FIND,(Number / 100) + 48);Number %= 100;FIND++;}if(I_Got >= 2){putc_oled(FIND,(Number / 10) + 48);Number %= 10;FIND++;}putc_oled(FIND,Number + 48);puts_oled(48,45,1,FIND,1);FIND = 1;}
function Finds(Numbers){Numbers = abs(Numbers);if(Numbers / 10000 > 0) return 5;if(Numbers / 1000 > 0) return 4;if(Numbers / 100 > 0) return 3;if(Numbers / 10 > 0) return 2;return 1;}

data 					  
(0,0,0,0,//-- Off	    
1,0,0,0, //-- Dim Blue	  
0,1,0,0, //-- Dim Red		  
