int double__tap;
function double_click(button) {        
	if (double__tap[button] >= 0) {                    
		double__tap[button] += get_rtime(); 
		if (double__tap[button] > 450)
		double__tap[button] = -1;                                                                                       
	}           
	if (event_release(button) && get_ptime(button) <= 200) { 
		if (double__tap[button] < 0) {                    
			double__tap[button] = 0;                        
		} else {             
			double__tap[button] = -1;                       
			return 1;                                   
		}                                                     
	}                                                  
	return 0;
}
