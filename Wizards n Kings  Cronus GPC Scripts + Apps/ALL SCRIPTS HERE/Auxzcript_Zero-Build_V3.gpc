//Aim Assist Pro by Auxzify (Enhanced for Zero Build S3,C3)
âœ§
int ProAim_HipZeroBuild = 11;
int ProAim_HipZeroBuild2 = -11;

int ProAim_AimZeroBuild = 12;
int ProAim_AimZeroBuild2 = -12;

int ProAim_DynamicZeroBuild = 14;
int ProAim_DynamicZeroBuild2 = -14;


//Aim Assist Configuration
âœ§
int Latency = 14;
int Restart = 12;
  
  
main{

combo_run (ProAim_ZeroBuildX);

if(get_val(7)) combo_run(ProAim_ZeroBuildS);

if(get_val(4)) combo_run(ProAim_ZeroBuildDynamic);

if(get_val(7) && get_val(4)) combo_run(ProAim_ZeroBuildDynamic);

if (abs(get_val(XB1_LY)) < 0 && abs(get_val(XB1_LX)) < 0 ){ 
set_val(XB1_LY,0); 
set_val(XB1_LX,0); 
} 
if (abs(get_val(XB1_RY)) < 0 && abs(get_val(XB1_RX)) < 0 ){ 
set_val(XB1_RY,0); 
set_val(XB1_RX,0); 
}}

function a_f(p,m) {
if(abs(get_val(p)) < Restart)
return m;
return get_val(p);}

combo ProAim_ZeroBuildX { 
    set_val(10,a_f(10,ProAim_HipZeroBuild));
    wait(Latency)
    set_val(9,a_f(9,ProAim_HipZeroBuild));
    wait(Latency)
    set_val(10,a_f(10,ProAim_HipZeroBuild2));
    wait(Latency)
    set_val(9,a_f(9,ProAim_HipZeroBuild2));
    wait(Latency)}
    
combo ProAim_ZeroBuildS { 
    set_val(10,a_f(10,ProAim_AimZeroBuild));
    wait(Latency)
    set_val(9,a_f(9,ProAim_AimZeroBuild));
    wait(Latency)
    set_val(10,a_f(10,ProAim_AimZeroBuild2));
    wait(Latency)
    set_val(9,a_f(9,ProAim_AimZeroBuild2));
    wait(Latency)}
    
combo ProAim_ZeroBuildDynamic { 
    set_val(10,a_f(10,ProAim_DynamicZeroBuild));
    wait(Latency)
    set_val(9,a_f(9,ProAim_DynamicZeroBuild));
    wait(Latency)
    set_val(10,a_f(10,ProAim_DynamicZeroBuild2));
    wait(Latency)
    set_val(9,a_f(9,ProAim_DynamicZeroBuild2));
    wait(Latency)}

