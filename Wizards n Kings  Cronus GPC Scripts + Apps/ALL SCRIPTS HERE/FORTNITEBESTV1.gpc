 // AIM ASSIST TOGGLES \\                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                              
int    AimAssist        = On;      // AIM ASSIST TOGGLE
define HipFire          = On; // AIM ASSIST WITH HIP FIRE
define AABoost          = On; 	// AIM ASSIST BOOST TOGGLE
define AAHipFireBoost   = On; // AIM ASSIST 2 WITH HIP FIRE
define ExtraAA          = On;  	// AIM ASSIST 2 BOOST
// AIM ASSIST VALUES \\
int    Power            = 2; // AIM ASSIST POWER 0-3
define Radius           = 15;    // SIZE OF SCREEN SHAKE 0 - 20 
define Angle            = 120;    // SPEED OF THE AIM ASSIST 0 - 120
define RadiusBoost      = 15;     // RADIUS BOOST  0 - 50
define AngleBoost       = 20;     // SPEED BOOST OF AIM ASSIST 0 - 50

// ANTI RECOIL \\
int AntiRecoil = On;      // ANTI RECOIL TOGGLE
define AntiRecoilHold  = PS4_L2; // ANTIRECOIL HOLD TOGGLE
define AntiRecoilPress = PS4_LEFT;   // ANTIRECOIL PRESS TOGGLE
define RecoilHoldOnFly = FALSE; // ANTIRECOIL ADJUSTMENT HOLD
define RecoilUpPress   = PS4_UP;    // ANTIRECOIL ADJUSTMENT STRONGER
define RecoilDownPress = PS4_LEFT;  // ANTIRECOIL ADJUSTMENT WEAKER
int Vertical = 10;		  // ANTI RECOIL STRENGTH 1 - 50 

// SLIDE CANCEL \\
int SlideCancel = Off; // SC 
define SlideCancelHold = PS4_L2; // SLIDE CANCEL TOGGLE HOLD
define SlideCancelPress = PS4_UP; // SLIDE CANCEL TOGGLE PRESS
// BUNNY HOP \\
int BunnyHop    = Off; // BUN BUN
define BunnyHopHold = PS4_L2; // BUNNY HOP TOGGLE HOLD
define BunnyHopPress = PS4_RIGHT; // BUNNY HOP TOGGLE PRESS

// RAPID FIRE \\
int RapidFire = Off; // Rapid Fire
define RapidFireHold = PS4_L2; // RAPID FIRE HOLD
define RapidFirePress = PS4_DOWN; // RAPID FIRE TOGGLE

/*                                                                                                                                                                                                                                     
‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
                                                
