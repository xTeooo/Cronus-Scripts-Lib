const string ModValue [] = {""   , "DoGz Polar Radius"  , "DoGz Polar Angle"  , "DoGz Radius Boost"  , "DoGz Angle Boost"  , "DoGz Polar Release"  , "Legacy AR 1 RY"  , "Legacy AR 1 RX"  , " Prone Shot Wait"  , " Jump Shot Wait"  , "SlideCancel Delay" , " Bunny Hop Wait" };
