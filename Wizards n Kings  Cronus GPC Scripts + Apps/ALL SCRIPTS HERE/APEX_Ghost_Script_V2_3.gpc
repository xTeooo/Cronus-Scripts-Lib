    //Layout
    define PS = 0;
    define SHARE = 1;
    define OPTIONS = 2;   
    define R1 = 3;
    define R2 = 4;        
    define R3 = 5;        
    define L1 = 6;       
    define L2 = 7;      
    define L3 = 8;    
    define RX = 9;       
    define RY = 10;      
    define LX = 11;       
    define LY = 12;      
    define UP = 13;       
    define DOWN = 14;    
    define LEFT = 15; 
    define RIGHT = 16;
    define TRIANGLE = 17;
    define CIRCLE = 18;  
    define CROSS = 19;   
    define SQUARE = 20;
 
    data(0,0,3,0,0,0,0,3);
 
    const byte AR_T[][] =
    {{10,50,140},
    {10,90,150}};
    const byte AR_V[][] =
    {{38,31,11},//Low Recoil - Basso Rinculo
    {45,17,11}};//High Recoil - Alto Rinculo
    int aRX;
    const byte AR_H[][] =
    {{0, 20, 8}}; //Don't Touch that - Non toccare
 
 
 
 
    //Rapid Fire Values - Valori Rapid Fire
    define RAPIDFIRE_SPEED = 6;
 
    //Custom Sensitivity - Sensibilità personalizzata
    int GEN_SENS = 150; //General - Generale
    int ADS_SENS = 140; //On ADS - Quando sei in mirata
    int ADS_FIRE_SENS = 140; //On Fire ADS - Quando sei in mirata e stai sparando
    int FIRE_SENS = 140; //On fire without ADS - Quando spari senza mirare
 
    //Strafe
    int RIGHT_Speed = 200; //How long to go RIGHT - Quanto a lungo ti sposti verso destra
    int LEFT_Speed = 200; //How long to go LEFT - Quanto a lungo ti sposti verso sinistra
 
 
    //Aim Assist Values - Valori aim assist
    int AP = 20; 
    int AM = -20; 
    int Delay = 20;
    int Release = 21;
 
 
 
 
    //Aim Abuse Values - Valori aim Abuse
    int HOLD = 0;
    int RELEASE = 0;
 
 
 
    //Toggles
    int rumbles;
    int d_tap;
    int rs_x;
    int rs_y;
    int combo_count;
    int v,i,aRY;
    int ar_idx;
    int rumble = -1;
    int rumble_t;
    int USE_SENS;
    int onoff;
    int Strafe_Toggle;
 
    init{
    led_color(ar_idx);
 
    } 
 
    main {
 
 
 
 
 
    set_val(TRACE_1, RADIUS);
 
 
    if(!get_val(L2) && !get_val(R2)) { 
    USE_SENS=GEN_SENS;}
    else if(get_val(L2) && !get_val(R2)) { 
    USE_SENS=ADS_SENS;}    
    else if(!get_val(L2) && get_val(R2)) { 
    USE_SENS=FIRE_SENS;}
    else if(get_val(L2) && get_val(R2)) { 
    USE_SENS=ADS_FIRE_SENS;}
    sensitivity(RY,NOT_USE,USE_SENS);
    sensitivity(RX,NOT_USE,USE_SENS);
 
 
 
    if(d_tap)
    d_tap -= get_rtime();
 
 
 
 
 
 
 
    if(get_val(L2) && event_press(R3)) {
    Strafe_Toggle=!Strafe_Toggle;
    if(Strafe_Toggle) combo_run(Flash_ON); 
    else combo_run(Flash_OFF);}
    if(Strafe_Toggle && get_val(L2) && get_val(R2))
    combo_run(Strafe);else combo_stop(Strafe);
 
 
 
    if(get_val(L2) && event_press(UP)) {
    rf_onoff = !rf_onoff;
    rumble(rf_onoff);}
    if(rf_onoff && get_val(R2))
    combo_run(RAPID_FIRE);
    if(rumbles)
    combo_run(RUMBLE); 
 
    if(AST)
    if(get_val(L2) && !get_val(R2)) 
    combo_run(AimAssist);
    else 
    combo_stop(AimAssist); 
 
    //POLAR
    if(polar_aim_assist)
         {
 
         // vm_tctrl(VM_INTERVAL); 
 
          actual_X = get_val(X);   
          actual_Y = get_val(Y);           
          actual_Magnitude = isqrt(pow(actual_X, 2) + pow(actual_Y, 2));  
          max_Magnitude = (actual_Magnitude < 100);                      
 
          if(!(time++ % STEP_INTERVAL)){                                  
                 angle += STEPS;     
 
          }                                                               
          angle = angle % 360;                                            
          sin_angle = Polar_Array[angle % 360];                           
          cos_angle = Polar_Array[(angle + 270) % 360];                  
          cos_angle = (cos_angle * RADIUS) / 100;                         
          sin_angle = (sin_angle * RADIUS) / 100;                          
 
          if((ACTIVE_WHEN_ADS && get_val(PS4_L2)) || (ACTIVE_WHEN_FIRING && get_val(PS4_R2))){ 
               if(actual_Magnitude <= RADIUS){                                                      
                    sin_angle -= actual_Y;                                                              
                    cos_angle -= actual_X;   
 
               }else {                                                                                       
                    sin_angle = (sin_angle * (200 - ((abs(actual_Y) + actual_Magnitude) / 10) * 10) / 200) * max_Magnitude; 
                    cos_angle = (cos_angle * (200 - ((abs(actual_X) + actual_Magnitude) / 10) * 10) / 200) * max_Magnitude; 
 
               }                                                  
               set_val(X, calculate(actual_X + cos_angle, -100, 100));
               set_val(Y, calculate(actual_Y + sin_angle, -100, 100)); 
          }
 
    if(AT)if(get_val(L2)> 95){combo_run(AimAbuse);}
    if(event_release(L2)){ combo_stop(AimAbuse);}
 
    if(get_val(L2) && event_press(DOWN)) {
    ar_idx = !ar_idx;
    controllo_orizzontale = !controllo_orizzontale;
    rumble = ar_idx;
    if(Rinculo_orizzontale && !ar_idx)
    led_color(3);
    else
    led_color(ar_idx);
 
    }
 
    if(get_val(L2) && event_press(RIGHT)){
        Rinculo_orizzontale = !Rinculo_orizzontale;
        rumble = ar_idx;
 
        if(!ar_idx && Rinculo_orizzontale)
        led_color(3);
        else
        led_color(ar_idx);
    }
 
    if(get_val(L2) && get_val(R2)) {
    if(get_ptime(R2) >= AR_T[ar_idx][0])    
    aRY =  AR_V[ar_idx][0];
    if(controllo_orizzontale && Rinculo_orizzontale){
    aRX = AR_H[ar_idx][0];}
    if(get_ptime(R2) >= AR_T[ar_idx][1] * 10)
    aRY = AR_V[ar_idx][1];
    if(controllo_orizzontale && Rinculo_orizzontale){
    aRX = AR_H[ar_idx][1]*-1;}
    if(get_ptime(R2) >= AR_T[ar_idx][2] * 10)
    aRY = AR_V[ar_idx][2];
    if(controllo_orizzontale && Rinculo_orizzontale){
    aRX = AR_H[ar_idx][2]*-1;}
 
 
    if(abs(get_val(RX)) < aRY + 5)
    set_val(RY,xy_val(RY,aRY));
    if(controllo_orizzontale && Rinculo_orizzontale){
   set_val(9, xy_val(RX, aRX));}}
 
    if(rumble >= 0) {
    if(!rumble_t)
    set_rumble(RUMBLE_A,100);
    rumble_t += get_rtime();
    if(rumble_t ==  250)  
    reset_rumble();
    if(rumble_t == 400) {
    rumble--;
    rumble_t = 0;}}
    }
}
 
 
     combo AimAssist {
        set_val(10, xy_val(10, AP));
        wait(Delay);
        set_val(9, xy_val(9,AP));
        wait(Delay);
        set_val(10,xy_val(10,AM));
        wait(Delay);
        set_val(9, xy_val(9,AM));
        wait(Delay);
        }
 
    combo AimAbuse {
    set_val(L2, 100);
    wait(HOLD);
    set_val(L2, 0);
    wait(RELEASE);}
 
    combo RAPID_FIRE {
    set_val(R2, 100);
    wait(RAPIDFIRE_SPEED);
    set_val(R2, 0);
    wait(RAPIDFIRE_SPEED);}
 
 
 combo Strafe {
    set_val(11,100);
    wait(RIGHT_Speed);
    set_val(11,-100);
    wait(LEFT_Speed);}
 
    combo RUMBLE {
    set_rumble(RUMBLE_A,100);
    wait(250);
    reset_rumble();
    wait(200);
    rumble--;}
 
 
    combo Flash_ON {
    LED(0,0,2,0);
    set_rumble(RUMBLE_A,100);
    wait(400);
    reset_rumble();
    reset_leds();}
 
    combo Flash_OFF {
    LED(0,2,0,0);
    set_rumble(RUMBLE_B,100);
    wait(200);
    reset_rumble();
    LED(0,0,0,0);
    wait(100);
    set_rumble(RUMBLE_B,100);
    LED(0,2,0,0);
    wait(200);
    reset_rumble();
    reset_leds();}
 
 
    function LED(a,b,c,d) {
    set_led(LED_1,a);
    set_led(LED_2,b);
    set_led(LED_3,c);
    set_led(LED_4,d);}
    function rumble(f_bln) {
    if(f_bln)
    rumble = 1;
    else
    rumble = 2;}
 
 
 
    function xy_val(f_axis,f_val) {
        if(abs(get_val(f_axis)) < Release) 
            return f_val;
        return get_val(f_axis); 
    } 
 
    //per il polar
    function calculate(int v, int lo, int hi) { 
      if(v < lo) return lo;               
      if(v > hi) return hi;        
      return v;     
    }  
 
    function led_color(color) {
    for(i = 0; i <= 3; i++) {
    set_led(i,dbyte((color * 4) + i));}}␀