		if (get_val(BUILD)) {                                                       
		}                                                                                
		if (event_release(FIRE) || get_val(EDIT_RESET_BUTTON) || get_val(PICKAXE)) {  
		set_click(BUILD, 1, 0);                                                 
		edit = FALSE;}}
		
		//Strafe LEFT & R1 = ON/OFF
		if(get_val(LEFT) && event_press(RBUMP)) { 
