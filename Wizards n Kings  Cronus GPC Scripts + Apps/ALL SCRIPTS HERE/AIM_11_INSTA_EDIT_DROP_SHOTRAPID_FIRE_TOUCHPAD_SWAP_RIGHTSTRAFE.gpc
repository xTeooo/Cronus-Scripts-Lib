	  	combo_run(master);                                                                
		}                                                                            
		} else if (i > num) {                                                           
		a = FALSE; }}                                                                                                                                                                                                      																																																																																																																																													                                                                                                                                                                                                                                                                                                                                                                                  	                                                                                                                     
		if(EDIT_BUTTON_HOLD)                                                                     
		{                                                                            
      	if(get_val(EDIT_BUTTON) && get_ptime(EDIT_BUTTON) > HOLD_TIME)                     
        {                                                                               
      	edit = TRUE;                                                                      
        }                                                                                
      	}                                                                                  
      	else if(!EDIT_BUTTON_HOLD)                                                        
        {                                                                                
      	if(get_val(EDIT_BUTTON))                                                         
        {                                                                               
      	edit = TRUE;                                                                      
        }                                                                               
      	}                                                                                  
