function xy_val(f_axis,f_val) {
    if(abs(get_val(f_axis)) < stickyaim_size + 1) 
        return f_val;
    return get_val(f_axis); 
}
 
 
function BAT_ECHO_ASSIST() {
	
	TimerEcho += get_rtime();
	
	if (TimerEcho < StepTime) {
		
		EchoVal = EchoValue;
	
	} else if (TimerEcho < (StepTime * 2)) {
		
		EchoVal = EchoValue + Increase;
	
	} else if  (TimerEcho < (StepTime * 3)) {
		
		EchoVal = EchoValue + (Increase * 2);
	
	} else if  (TimerEcho < (StepTime * 4)) {
		
		EchoVal = EchoValue + (Increase * 3);
	
	} else if  (TimerEcho < (StepTime * 5)) {
		
		EchoVal = EchoValue + (Increase * 4);}
	
	if  (TimerEcho > StepTime * 6) {
		
		TimerEcho = 0;	}
	
	combo_run(BAT_ECHO_LOCATION);
}

function Aim_Perfection(Last_Value, Current_Value, Boost, Correction, X_AXIS, Y_AXIS )         { 
     
     
           if(abs(Last_Value - Current_Value) < Aim_Perfection_Limit) 
                {
                    
                    if(Last_Value < Current_Value)
                        {           
                            if (Boost)
                                { 
                                    if (X_AXIS)
                                        set_val(PS4_RX, (Current_Value + Aim_Boost)); 
     
                                    if (Y_AXIS)
                                            set_val(PS4_RY, (Current_Value + Aim_Boost));
                                }
     
     
                            else if(Correction)
                                {
                                    if (X_AXIS)
                                        set_val(PS4_RX, (Current_Value - Aim_Correction)); 
     
                                    if (Y_AXIS)
                                    set_val(PS4_RY, (Current_Value - Aim_Correction));            
                                }
                        }
                    else 
                        {
     
                            if (Boost)
                                { 
                                    if (X_AXIS)
