main {

   if(PSonXBOX && get_console() == PIO_XB360 && get_controller() == PIO_PS4) { swap (PS4_TOUCH,PS4_SHARE);}
                                                                                 
     
      if(BlockRumble) block_rumble();
  
 
        if(HairTriggers) { deadzone(PS4_L2,PS4_R2,100,100);}	
  
  
//DEADZONE
	if (abs(get_val(walk)) < 0 && abs(get_val(strafe)) < 0 ){ 
				set_val(XB1_LY,(deadzone_left)); 
				set_val(XB1_LX,(deadzone_left));}
	if (abs(get_val(PS4_RY)) < 0 && abs(get_val(PS4_RX)) < 0 ){ 
				set_val(XB1_RY,(deadzone_right)); 
				set_val(XB1_RX,(deadzone_right));}


  	if(get_val(Ads)){ 
   		if(abs(X_Current_Value) <= POS_Micro_MVT_Limit && abs(Y_Current_Value) <= POS_Micro_MVT_Limit){    
   		if(abs(X_Last_Value - X_Current_Value) < 15)	{
       			combo_stop(BATCOMPUTER_ASSIST_Perfection);
       			Sampling_Done = FALSE;
   		if(get_val(Fire) > 95)	{ 
        		combo_stop(BATCOMPUTER_TUNING);
        		fine_pulse = 0;
        		combo_run(BATCOMPUTER_AIM_ASSIST);}
   		else	{
        		combo_stop(BATCOMPUTER_AIM_ASSIST);
        		spiroide_pulse = 0;
        		combo_run(BATCOMPUTER_TUNING);}}}
   			else if(abs(X_Current_Value) <= 90 && abs(Y_Current_Value) <= 90){ 
       			combo_stop(BATCOMPUTER_TUNING);
        		combo_stop(BATCOMPUTER_AIM_ASSIST);
        		spiroide_pulse = 0;
        		fine_pulse = 0;
        		combo_run(BATCOMPUTER_ASSIST_Perfection);}}
    		else {
        		combo_stop(BATCOMPUTER_TUNING);
        		combo_stop(BATCOMPUTER_AIM_ASSIST);
        		combo_stop(BATCOMPUTER_ASSIST_Perfection);
       		 	spiroide_pulse = 0;
        		fine_pulse = 0;
        		Sampling_Done = FALSE;}
   
   if (BATCAVE_ECHO) {
	    if (get_ival(Ads) || (EchoHip && get_ival(Fire))) {
		    	BAT_ECHO_ASSIST();    } 
		   	else {
		    	TimerEcho = 0;   	}
		    	}
 
    if (BATT_STICKY_TARGETING) {
   		if(get_val(Ads)) combo_run(BATT_STICKY_TARGETING);
        	else combo_stop(BATT_STICKY_TARGETING);}
	    
	if (COWLKILLA_Headassist) {
	    if (get_ival(Ads) && get_ival(Fire) || HeadHipAssist && get_ival(Fire)) {
	   	if (abs(get_ival(LY)) < HeadMax) 	{ 
	        	Head += HeAdstep;
	        	combo_run(COWLKILLA_Headassist);     	}	
	    if (!get_ival(Fire)) Head = 0;
	    if (Head >= HeadMax) combo_stop(COWLKILLA_Headassist);}
	 			}
	 			
 	if (InstaFastReset) {
    	if (event_press(FastReset)) { 
       			combo_run(FastReset);}
				}
				
	if(BATSHOT){                             
		if(get_val(Ads) && get_val(Fire)){  
				BATSHOT_hold = 140;
				BATSHOT_rest = 0;            
				combo_run(BATSHOT);} 
			else if (combo_running(BATSHOT)){     
				combo_stop(BATSHOT);}                                             
					}  
						
	if(BATGRIP_ANTIRECOIL){                               
		if( get_val(PS4_L2) && get_val(PS4_R2)){      
					combo_run(BATGRIP_ANTIRECOIL); }               
		if( abs(get_val(RY)) > 36 || abs(get_val(RX)) > 36) { 
					combo_stop (BATGRIP_ANTIRECOIL); }               
}
	if(BloomReducer) {
		if(get_val(Ads) && (get_val(Fire))) {        
  
        combo_run(BATGRIP_ANTIBLOOM);
}
     if(abs(get_val(PS4_LX)) > 40 || abs(get_val(PS4_LY)) > 40){
 
        combo_stop(BATGRIP_ANTIBLOOM);
       }
}

   if(AimStickLinear) {
  
       actual_X = get_val(RX);
       actual_Y = get_val(RY);
       actual_Magnitude = isqrt(pow(actual_X, 2) + pow(actual_Y, 2));                                                                                                                                                                                              //CREATED BY EXPLOIT YT
       max_Magnitude = (actual_Magnitude < 100);
  
     if(!(time++ % STEP_INTERVAL)){
       
       angle += STEPS;
}
       angle = angle % 360;
       sin_angle = Polar_Array[angle % 360];
       cos_angle = Polar_Array[(angle + 270) % 360];                   
       cos_angle = (cos_angle * Radius) / 100;
       sin_angle = (sin_angle * Radius) / 100;
  
  if((BATAIM && get_val(Ads)) || (BATHIPFIRE && get_val(Fire))){
 
    	if(actual_Magnitude <= Radius){
  
       sin_angle -= actual_Y;
       cos_angle -= actual_X;
       }
       else {
       sin_angle = (sin_angle * (200 - ((abs(actual_Y) + actual_Magnitude) / 10) * 10) / 200) * max_Magnitude;
       cos_angle = (cos_angle * (200 - ((abs(actual_X) + actual_Magnitude) / 10) * 10) / 200) * max_Magnitude;
}
  
       set_val(RX, clamp(actual_X + cos_angle, -100, 100));
       set_val(RY, clamp(actual_Y + sin_angle, -100, 100));
      }
   } 
}     


