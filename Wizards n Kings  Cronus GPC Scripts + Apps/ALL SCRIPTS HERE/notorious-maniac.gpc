    	}
    	if(event_press(PS4_DOWN)){
        	indexVar += 1;
            updateDisplay = TRUE; 
    	}
    }
	if(!get_ival(Ads)){ 
		if(event_press(PS4_RIGHT)){ 
			indexVar  += 1;
			updateDisplay = TRUE;
		}
		if(event_press(PS4_LEFT)){ 
			indexVar  -= 1;
			updateDisplay = TRUE;
		}
	}
	if(event_release(PS4_CROSS)){
    	if(mainMenu){
