function customPolarSens(fId,fMid,fIndex) {
	fValue = get_val(fId);
    if(fMid != NOT_USE) {
    	fValueSens = -1;
    	if(fValue >= 0) fValueSens = 1;
        	fValue *= fValueSens;
        if(fValue <= fMid) 
        	fValue = (fValue * 16384) / fMid;
        else 
        	fValue = ((16384 * (fValue - fMid)) / (32767 - fMid)) + 16384;
        	fValue *= fValueSens;
    }
    if(fIndex != NOT_USE) {
        fValue = (fValue * fIndex) / 32767;
    }
    set_val(fId,clamp(fValue,-32768,32767));
    return;
}
