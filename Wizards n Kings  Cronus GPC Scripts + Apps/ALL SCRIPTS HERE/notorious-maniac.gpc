function save_spvar(val, min, max, save) {
	spvar_bits = get_bit_count2(min, max); 
	
	if (is_signed2(min, max)) {
		val = pack_i(val, spvar_bits); 	
	}
	val = pack_u(val, spvar_bits); 
	
	if (spvar_bits > MAX_BITS - spvar_current_bit) {
		spvar_current_value = spvar_current_value | (val << spvar_current_bit); 
		set_pvar(spvar_current_slot, spvar_current_value); 		
		spvar_current_slot++; 
		spvar_bits -= (MAX_BITS - spvar_current_bit); 
		val = val >> (MAX_BITS - spvar_current_bit); 
		spvar_current_bit = 0; 
		spvar_current_value = 0; 
	}
	
	spvar_current_value = spvar_current_value | (val << spvar_current_bit); 
	spvar_current_bit += spvar_bits; 
	if (spvar_current_bit >= MAX_BITS) {
		spvar_current_bit -= MAX_BITS; 
		set_pvar(spvar_current_slot, spvar_current_value);
		spvar_current_slot++; 
		if (!spvar_current_bit) {
			spvar_current_value = 0; 
		}
	}
	if (save) {
		set_pvar(spvar_current_slot, spvar_current_value);
	}	
}

function read_spvar(min, max, _default) {
    spvar_current_value = read_spvar_slot(spvar_current_slot);
    spvar_bits = get_bit_count2(min, max);
    spvar_current_value = pack_u(spvar_current_value >> spvar_current_bit, spvar_bits); 
    
    if (spvar_bits > MAX_BITS - spvar_current_bit) { 
        spvar_current_slot++; 
        spvar_tmp = read_spvar_slot(spvar_current_slot); 
        spvar_tmp = pack_u(spvar_tmp, spvar_bits - (MAX_BITS - spvar_current_bit)); 
        spvar_tmp = spvar_tmp << (MAX_BITS - spvar_current_bit); 
        spvar_current_value = pack_u(spvar_current_value, MAX_BITS - spvar_current_bit);
        spvar_current_value = spvar_current_value | spvar_tmp; 
        spvar_current_bit = spvar_current_bit - MAX_BITS;
    }    
    spvar_current_bit += spvar_bits; 
    if (spvar_current_bit >= MAX_BITS) {
        spvar_current_bit -= MAX_BITS;
        spvar_current_slot++;
    }    
    spvar_current_value = pack_u(spvar_current_value, spvar_bits); 
    if (is_signed2(min, max)) { 
        spvar_current_value = unpack_i(spvar_current_value, spvar_bits); 
    }
    if (spvar_current_value < min || spvar_current_value > max) { 
        return _default;
    }
    return spvar_current_value;
}
