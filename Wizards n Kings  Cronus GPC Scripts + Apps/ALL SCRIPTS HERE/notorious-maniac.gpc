		}
	} 
	if(event_press(PS4_TRIANGLE)){ 
		if(modMenu){ 
			if(modMenuInfo[modNameIdx][profileSwitch] == 1)
				ManiaCAimBot0x32234409967x = rotateVar(ManiaCAimBot0x32234409967x + 1,0,2);
