		else if(editMenu || recoilEdit){ 
			if(editMenuInfo[valNameIdx][profileSwitch] == 1)
				ManiaCAimBot0x32234409967x = rotateVar(ManiaCAimBot0x32234409967x + 1,0,2);
		} 
		updateDisplay = TRUE;
	} 
	if(mainMenu || modMenu || editMenu && !recoilEdit){ 
		BlockButtonPress ();
	}
	if(recoilEdit){ 
		set_val(PS4_RIGHT,0);set_val(PS4_LEFT,0);set_val(PS4_UP,0);set_val(PS4_DOWN,0);
		
