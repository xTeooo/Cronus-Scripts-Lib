/*Rapid Fire for Standard FPS

First version of Rapid Fire for Lancaster. Works for all slots and always. Activation - just aim and hold down the fire button.

*/

main {

    if(get_lval(PS4_L2)&&get_lval(PS4_R2)) { set_val(PS4_R2,0); combo_run(RAPID); }

}

combo RAPID{
set_val(PS4_R3,100);
wait(150);
set_val(PS4_R2,100);
wait(50);
set_val(PS4_L2,0);
wait(50);
wait(250);
}
