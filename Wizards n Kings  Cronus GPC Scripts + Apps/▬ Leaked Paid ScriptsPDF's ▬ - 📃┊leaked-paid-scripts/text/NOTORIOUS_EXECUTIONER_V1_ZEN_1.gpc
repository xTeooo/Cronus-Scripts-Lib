int	AIM_SLOPE						= 6;
int	AIM_ASSIST 						= 10;


define LOW							= 2;	
define MID							= 3;
define HIGHT						= 4;
int SPIRE_EFFECT					= LOW;

define LAZER						= 1; //For close range and long range fire (selected by default)
define AIMX						    = 2; //For Short range and Mid range fire		
