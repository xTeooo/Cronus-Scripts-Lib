//------------------------------------------------------------------------------
define FIRE = XB1_RT; //R2/RB//--------------------------------------------------------
define ADS = XB1_LT //L2/LB//--------------------------------------------------------
//------------------------------------------------------------------------------
define RB= XB1_RB; //R1/RT//---------------------------------------------------------
define LB = XB1_LB; //L1/LT//--------------------------------------------------------
//------------------------------------------------------------------------------
define RS = 5; //R3/RS//--------------------------------------------------------
define LS = 8; //L3/LS//--------------------------------------------------------
//------------------------------------------------------------------------------
define A = 19; //PS4 CROSS/XB1 A//----------------------------------------------
define X = 20; //PS4 SQUARE/XB1 X//---------------------------------------------
define Y = 17;	//PS4 TRIANGLE/XB1 Y//------------------------------------------
define B = 18; //PS4 CIRCLE/XB1 B//---------------------------------------------
//------------------------------------------------------------------------------
define UP = 13; //Up//----------------------------------------------------------
define DOWN = 14; //Down//------------------------------------------------------
define LEFT = 15; //Left//------------------------------------------------------
define RIGHT = 16; //Right//----------------------------------------------------
//------------------------------------------------------------------------------
define START = 1; //VIEW/SHARE//------------------------------------------------
define BACK = 2; //Options/Menu//-----------------------------------------------
//------------------------------------------------------------------------------
define TOUCHPAD = 27; //Touchpad//----------------------------------------------
define RX = 9; //PS4 RX / Xbox RX//---------------------------------------------
define RY = 10; //PS4 RY / Xbox RY//--------------------------------------------
define LX = 11; //XB1_LX //PS4_LX//---------------------------------------------
define LY = 12; //XB1_LY //PS4_LY//---------------------------------------------
define FastReset = XB1_LT;//----------------------------------------------------
//------------------------------------------------------------------------------
int HairTrigger = TRUE;//-------------------------------------------------------
int rapidfire = TRUE;//--------------------------------------------------------
int Pulse_shot = FALSE;//-------------------------------------------------------
int lock = FALSE;//-------------------------------------------------------------
int AAT = TRUE;//---------------------------------------------------------------
int aimbot = TRUE;//------------------------------------------------------------
int InstaFastReset = TRUE;//----------------------------------------------------
//------------------------------------------------------------------------------
int Shunt;//--------------------------------------------------------------------
define On = TRUE;//------------------------------------------------------------
define Off = FALSE;//-----------------------------------------------------------
main {   //start of end---------------------------------------------------------
//------------------------------------------------------------------------------
vm_tctrl(-5); // Run the VM every 2ms-------------------------------------------
//------------------------------------------------------------------------------
if (get_val(ADS) && event_release(XB1_VIEW)) {
        aimbot = !aimbot;
 if(aimbot) {
            combo_run(RumbleOnce);
    }  else {
            combo_stop(aimbotC);
            combo_run(RumbleTwice);}
    }
if(aimbot) {
 if(get_val(ADS)>=95) {
            combo_run(aimbotC);} }
  if(event_release(ADS)){ combo_stop(aimbotC);}
//------------------------------------------------------------------------------
if((get_val(ADS)) && (event_press(START))) {
        lock = !lock;
 if(lock) {
            combo_run(RumbleOnce); 
        }  else {
           combo_run(RumbleTwice);} }
//------------------------------------------------------------------------------
if(lock){
 if (get_val(ADS) && get_val(FIRE)) {
      AAT = !AAT;
      combo_run(shake);
      combo_run(RumbleOnce);
        } else {
      combo_run(RumbleTwice);
      combo_stop(shake);} }
//------------------------------------------------------------------------------
if (get_val(RX)<-10||get_val(RX)>10||get_val(10)<-10||get_val(10)>10||get_val(RX)<-10||get_val(RX)>10||get_val(10)<-10||get_val(10)>10){ 
combo_stop(shake);}
//------------------------------------------------------------------------------
if(get_val(ADS) && get_val(FIRE)) {
            combo_run(AR);} 
 if(get_val(ADS) && get_val(FIRE)){ combo_stop(AR);}
//------------------------------------------------------------------------------
if (get_val(RX)<-10||get_val(RX)>10||get_val(RY)<-10||get_val(RY)>10||get_val(RX)<-10||get_val(RX)>10||get_val(RY)<-10||get_val(RY)>10){ 
combo_stop(AR);}
//------------------------------------------------------------------------------
if(HairTrigger){ 
 if(get_ival(PS4_L2) > 1)
    set_val(PS4_L2, 100);
if(get_ival(PS4_R2) > 1)
    set_val(PS4_R2, 100);}
//------------------------------------------------------------------------------
if (Pulse_shot){
 if (get_val(ADS) && get_val(RB)) {
      combo_run(Pulse_shotC);
    combo_stop(xspam);
    set_val(RB, 0);
          } else {
      combo_stop(Pulse_shotC);} }
//------------------------------------------------------------------------------
if(get_val(ADS) && event_press(LEFT)) {
        Pulse_shot=!Pulse_shot; 
 if(Pulse_shot){
        combo_run(RumbleOnce); 
        }  else {
        combo_run(RumbleTwice);} }
//------------------------------------------------------------------------------
if (InstaFastReset && !get_ival(ADS) && event_press(ADS)) { 
Shunt = FastReset;
combo_run(FastReset);
}
//------------------------------------------------------------------------------
if (Shunt) {
if (event_release(Shunt)) {
Shunt = Off;
}
else {
set_val (Shunt,Off);
}
}
}//end of main------------------------------------------------------------------
//------------------------------------------------------------------------------
//---------------------------------combo code-----------------------------------
//------------------------------------------------------------------------------
//------------------------------------------------------------------------------
combo AR {
    set_val(RY,36);
wait(10);
    set_val(RY,0);
wait(10);}
//------------------------------------------------------------------------------
combo shake {
    set_val(RX,-10);
wait(18);
    set_val(RY,-10);
wait(18);
    set_val(RY,10);
wait(18);
    set_val(RX,10);
wait(18);}
//------------------------------------------------------------------------------
combo Pulse_shotC {
    set_val(FIRE, 100);
wait(10);
    set_val(FIRE, 0);
wait(290);}
//------------------------------------------------------------------------------
combo xspam {
    set_val(X, 100);
wait(80);
    set_val(X, 0);}
//------------------------------------------------------------------------------
combo aimbotC {
    set_val(ADS, 100);
wait(300);
    set_val(ADS, 0);
wait(20);}
//------------------------------------------------------------------------------
combo RumbleOnce {
    set_rumble(RUMBLE_A, 100);
wait(250);
    reset_rumble();}
//------------------------------------------------------------------------------
combo RumbleTwice {
    set_rumble(RUMBLE_A, 100);
wait(250);
    set_rumble(RUMBLE_A, 0);
wait(250);
    set_rumble(RUMBLE_A, 100);
wait(250);
    reset_rumble();}
//------------------------------------------------------------------------------
combo FastReset {
wait(50)
    set_val(ADS,100);        
    wait(50);
    combo_run(Confirm);
}
//------------------------------------------------------------------------------
combo InstantReset {
wait(20)
    combo_run(Confirm);
}
//------------------------------------------------------------------------------
combo Confirm {
set_val(RB,100);
    wait(50);
    set_val(RB,0);
    wait(100);
}
