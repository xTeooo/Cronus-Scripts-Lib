define FULL_BITMASK = -1;
define MAX_BITS = 32;
define MAX_FW_BITS = 32;

function reset_spvar() {
	spvar_current_slot = SPVAR_4; 
	spvar_current_bit = 0;
	spvar_current_value = 0;
}
function get_bit_count(val) {

	if (val < 0) {
		return get_bit_count(abs(val + 1));
	}
	spvar_tmp = 0;
	do {
		spvar_tmp++;
		val = val >> 1; 
	} while (val);
	return spvar_tmp;
}
function get_bit_count2(min, max) {	
	spvar_tmp = max(get_bit_count(min), get_bit_count(max));
	if (is_signed2(min, max)) {
		spvar_tmp++;
	}
	return spvar_tmp;
}
function is_signed2(min, max) { return min < 0 || max < 0; }

function make_full_mask(bits) {	
	if (bits == MAX_FW_BITS) {
		return FULL_BITMASK;
	}
	return (FULL_BITMASK & (~(1 << (MAX_FW_BITS - 1)))) >> (MAX_FW_BITS - bits - 1);
}
function make_sign(bits) { return 1 << clamp(bits - 1, 0, MAX_FW_BITS - 1); }
function make_sign_mask(bits) { return ~make_sign(bits); }

function pack_u(val, bits) { return val & make_full_mask(bits); }
function pack_i(val, bits) {
	if (val < 0) {
		return (abs(val) & make_sign_mask(bits)) | make_sign(bits);	
	}
	return val & make_sign_mask(bits);
}
function unpack_i(val, bits) {
	if (val & make_sign(bits)) {
		return 0 - (val & make_sign_mask(bits));
	}
	return val & make_sign_mask(bits);
}

int spvar_current_bit;
int spvar_current_slot;
int spvar_current_value;
int spvar_tmp;
int spvar_bits;

function read_spvar_slot(slot) { return get_pvar(slot, 0-(FULL_BITMASK & (1 << (MAX_BITS - 1))), 0-((0-(FULL_BITMASK & (1 << (MAX_BITS - 1)))) + 1), 0); }

function save_spvar(val, min, max, save) {
	spvar_bits = get_bit_count2(min, max); 
	
	if (is_signed2(min, max)) {
		val = pack_i(val, spvar_bits); 	
	}
	val = pack_u(val, spvar_bits); 
	
	if (spvar_bits > MAX_BITS - spvar_current_bit) { 
		spvar_current_value = spvar_current_value | (val << spvar_current_bit); 
		set_pvar(spvar_current_slot, spvar_current_value); 		
		spvar_current_slot++; 
		spvar_bits -= (MAX_BITS - spvar_current_bit); 
		val = val >> (MAX_BITS - spvar_current_bit); 
		spvar_current_bit = 0; 
		spvar_current_value = 0; 
	}
	
	spvar_current_value = spvar_current_value | (val << spvar_current_bit); 
	spvar_current_bit += spvar_bits; 
	if (spvar_current_bit >= MAX_BITS) {
		spvar_current_bit -= MAX_BITS; 
		set_pvar(spvar_current_slot, spvar_current_value);
		spvar_current_slot++; 
		if (!spvar_current_bit) {
			spvar_current_value = 0; 
		}
	}
	if (save) {
		set_pvar(spvar_current_slot, spvar_current_value);
	}	
}

function read_spvar(min, max, _default) {
    spvar_current_value = read_spvar_slot(spvar_current_slot); 
    spvar_bits = get_bit_count2(min, max); 
    spvar_current_value = pack_u(spvar_current_value >> spvar_current_bit, spvar_bits); 
    
    if (spvar_bits > MAX_BITS - spvar_current_bit) { 
        spvar_current_slot++; 
        spvar_tmp = read_spvar_slot(spvar_current_slot); 
        spvar_tmp = pack_u(spvar_tmp, spvar_bits - (MAX_BITS - spvar_current_bit)); 
        spvar_tmp = spvar_tmp << (MAX_BITS - spvar_current_bit); 
        spvar_current_value = pack_u(spvar_current_value, MAX_BITS - spvar_current_bit);
        spvar_current_value = spvar_current_value | spvar_tmp; 
        spvar_current_bit = spvar_current_bit - MAX_BITS; 
    }    
    spvar_current_bit += spvar_bits; 
    if (spvar_current_bit >= MAX_BITS) {
        spvar_current_bit -= MAX_BITS; 
        spvar_current_slot++;
    }    
    spvar_current_value = pack_u(spvar_current_value, spvar_bits); 
    if (is_signed2(min, max)) { 
        spvar_current_value = unpack_i(spvar_current_value, spvar_bits); 
    }
    if (spvar_current_value < min || spvar_current_value > max) { 
        return _default;
    }
    return spvar_current_value;
}
