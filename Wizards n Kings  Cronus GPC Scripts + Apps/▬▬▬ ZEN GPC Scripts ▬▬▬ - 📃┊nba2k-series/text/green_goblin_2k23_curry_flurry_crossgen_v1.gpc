- 𝑇𝑜 ℎ𝑖𝑡 𝑚𝑜𝑣𝑖𝑛𝑔 𝑠ℎ𝑜𝑡𝑠 ℎ𝑜𝑙𝑑 𝑟𝑡 + 𝑋

- To hit Standing Low Stamina shots hold RB (R1) ( Will Add low stamina fades just something quick as i been busy working )

- 𝑅𝟥 = 𝑅𝐻 𝐻𝑎𝑙𝑓𝑆𝑝𝑖𝑛

- 𝐿𝟥 = 𝐿𝐻 𝐻𝑎𝑙𝑓𝑆𝑝𝑖𝑛

- 𝑅𝑇+𝑅𝟥 = 𝑅𝐻 𝑆𝑝𝑖𝑛𝐵𝑎𝑐𝑘

- 𝑅𝑇+𝐿𝟥 = 𝐿𝐻 𝑆𝑝𝑖𝑛𝐵𝑎𝑐𝑘

- 𝑋+𝐿𝑒𝑓𝑡𝐷𝑝𝑎𝑑 = 𝐽𝑢𝑚𝑝𝑒𝑟 -𝟣

- 𝑋+𝑅𝑖𝑔ℎ𝑡𝐷𝑃𝑎𝑑 = 𝐽𝑢𝑚𝑝𝑒𝑟 +𝟣

- 𝑋+𝑈𝑃𝐷𝑝𝑎𝑑 = 𝐽𝑢𝑚𝑝𝑒𝑟 +𝟧

- 𝑋+𝐷𝑜𝑤𝑛𝐷𝑝𝑎𝑑 = 𝐽𝑢𝑚𝑝𝑒𝑟 -𝟧

- 𝑅𝑇+𝐿𝑒𝑓𝑡𝐷𝑝𝑎𝑑 = Fade -𝟣

- 𝑅𝑇+𝑅𝑖𝑔ℎ𝑡𝐷𝑃𝑎𝑑 = Fade +𝟣

- 𝑅𝑇+𝑈𝑃𝐷𝑝𝑎𝑑 = Fade +𝟧

- 𝑅𝑇+𝐷𝑜𝑤𝑛𝐷𝑝𝑎𝑑 = Fade -𝟧

- 𝑅B+𝐿𝑒𝑓𝑡𝐷𝑝𝑎𝑑 = LowStamina -𝟣

- 𝑅B+𝑅𝑖𝑔ℎ𝑡𝐷𝑃𝑎𝑑 = LowStamina +𝟣

- 𝑅B+𝑈𝑃𝐷𝑝𝑎𝑑 = LowStamina +𝟧

- 𝑅B+𝐷𝑜𝑤𝑛𝐷𝑝𝑎𝑑 = LowStamina -𝟧

- 𝐿𝑇+𝑅𝑇+𝑀𝑒𝑛𝑢 = 𝑆𝑎𝑣𝑒 𝑉𝑎𝑙𝑢𝑒𝑠

- 𝑀𝑒𝑛𝑢+𝑈𝑝𝐷𝑝𝑎𝑑 = 𝑆ℎ𝑜𝑡 𝑉𝑎𝑙𝑢𝑒𝑠 +𝟣𝟧𝟢 ( 𝑂𝑛𝑙𝑖𝑛𝑒 𝑄𝑢𝑖𝑐𝑘 𝑇𝑜𝑔𝑔𝑙𝑒 )

- 𝑀𝑒𝑛𝑢+𝖣𝗈𝗐𝗇𝐷𝑝𝑎𝑑 = 𝑆ℎ𝑜𝑡 𝑉𝑎𝑙𝑢𝑒𝑠 -𝟣𝟧𝟢 ( 𝑂𝑓𝑓𝑙𝑖𝑛𝑒 𝑄𝑢𝑖𝑐𝑘 𝑇𝑜𝑔𝑔𝑙𝑒 )

- 𝑆𝑐𝑟𝑖𝑝𝑡 𝑖𝑠 𝑠𝑒𝑡 𝑓𝑜𝑟 𝑜𝑓𝑓𝑙𝑖𝑛𝑒 𝑏𝑦 𝑑𝑒𝑓𝑎𝑢𝑙𝑡																							
	

																											
																																                                                          */

int JumperTime = 525;		
int MovingTime = 750;
int LowStaminaTime = 650

int Jumper;
int Moving;

init{

	JumperTime  = get_pvar(SPVAR_1, 300, 1000, JumperTime);
	MovingTime = get_pvar(SPVAR_2, 300, 1000, MovingTime);
	MovingTime = get_pvar(SPVAR_3, 300, 1000, LowStaminaTime);
	
}
main {
	
	set_val(TRACE_1,JumperTime);
	set_val(TRACE_2,MovingTime);
	set_val(TRACE_3,LowStaminaTime);
	
	if(event_press(XB1_RB))
		{   
			combo_run(LowStamina);
		}
	
	if(get_val(XB1_X)){
		if(event_press(XB1_RIGHT)){
			JumperTime += 1; 
			combo_run(rumble);
		}
		if(event_press(XB1_LEFT)){
			JumperTime -= 1; 
			combo_run(rumble);
		}
		if(event_press(XB1_UP)){
			JumperTime += 5; 
			combo_run(rumble);
		}
		if(event_press(XB1_DOWN)){
			JumperTime -= 5; 
			combo_run(rumble);
		}
	}
	
	if(get_val(XB1_RT)){
		if(event_press(XB1_RIGHT)){
			MovingTime += 1; 
			combo_run(rumble);
		}
		if(event_press(XB1_LEFT)){
			MovingTime -= 1; 
			combo_run(rumble);
		}
		if(event_press(XB1_UP)){
			MovingTime += 5; 
			combo_run(rumble);
		}
		if(event_press(XB1_DOWN)){
			MovingTime -= 5; 
			combo_run(rumble);
		}
	}
	
	if(get_val(XB1_RB)){
		if(event_press(XB1_RIGHT)){
			MovingTime += 1; 
			combo_run(rumble);
		}
		if(event_press(XB1_LEFT)){
			MovingTime -= 1; 
			combo_run(rumble);
		}
		if(event_press(XB1_UP)){
			MovingTime += 5; 
			combo_run(rumble);
		}
		if(event_press(XB1_DOWN)){
			MovingTime -= 5; 
			combo_run(rumble);
		}
	}
	
	if(get_val(XB1_MENU)){
		if(event_press(XB1_UP)){
			MovingTime += 150;
			JumperTime += 150;
			LowStaminaTime += 150;
			combo_run(rumble);
		}
		if(event_press(XB1_DOWN)){
			MovingTime -= 150;
			JumperTime -= 150;
			LowStaminaTime -= 150;
			combo_run(rumble);
		}
		
	}
    
   if(event_press(PS4_L3))
		{   
		    set_val(PS4_L3, 0);
			combo_run(LHHalfSpin);
		}
	
	
	{
		if(event_press(PS4_R3))
		{
		    
			combo_run(RHHalfSpin);
			set_val(PS4_R3, 0);
		}
	
	}
	
	if(get_val(PS4_R2))
	{		
		if (event_press(PS4_L3)) 
		{
		    
			combo_run(L_SPINBACK);
			set_val(PS4_L3, 0);
		}
		
		if (event_press(PS4_R3)) 
		{
		    
			combo_run(Takeover); 
			combo_run(R_SPINBACK);
			set_val(PS4_R3, 0);
		}
	}
		
		
	if(combo_running(R_SPINBACK) || combo_running(R_SPINBACK))
	 	{
	 	 combo_stop(LHHalfSpin);
	 	 combo_stop(RHHalfSpin);
	 	 set_val(PS4_R2, 0);
	 	}
	 	
	 	 
	
	if(get_ival(XB1_LT) && get_ival(XB1_RT) && event_press(XB1_MENU)){
		set_pvar(SPVAR_1,JumperTime);
		set_pvar(SPVAR_2,MovingTime);
		set_pvar(SPVAR_3,LowStaminaTime);
		combo_run(rumble);
	}
		
			
		if (event_press(XB1_X)) 
		{ Jumper = TRUE;
		Moving = FALSE;  
		}

	if(get_val(XB1_RT))
			
		if (event_press(XB1_X)) 
		{ Jumper = FALSE;
		Moving = TRUE;  
		}
    
    if(get_ival(XB1_X)){
    	set_val(XB1_X,0);
	    if(Jumper) {
	    	combo_run(Green1);
    	}
	    if(Moving) {
	    	combo_run(Green2);
    	}
    }

} 

combo rumble{
    wait(100);
    set_rumble(RUMBLE_A,80);
    wait(100);
    reset_rumble();
}

combo Green1{
	set_val(XB1_X,100);
	wait(JumperTime);
	set_val(XB1_X,0);
	wait(20);
}
combo Green2{
	set_val(XB1_X,100);
	wait(MovingTime);
	set_val(XB1_X,0);
	wait(20);
}

combo L_SPINBACK
{ 
	set_val(XB1_RY, -100); 
	wait(30); 
	set_val(XB1_RX, -100); 
	wait(30);	
	set_val(XB1_RY, 100);
	wait(60);
	set_val(XB1_LY, 100); 
	
	wait(800);
}
combo R_SPINBACK
{ 
	set_val(XB1_RY, -100); 
	wait(30);
	set_val(XB1_RX, 100); 
	wait(30);
	set_val(XB1_RY, 100);
	wait(60);
	set_val(XB1_LY, 100); 
	wait(800);
}

combo LHHalfSpin
{ 
	set_val(PS4_RX, -100);
	wait(10);
	set_val(PS4_RX, -100);
	set_val(PS4_RY, -30);
	wait(10);
	set_val(PS4_RX, -20);
	set_val(PS4_RY, -100);
	wait(10);	
	set_val(PS4_RY, -100);
	wait(10);
}


combo RHHalfSpin
{ 
	set_val(PS4_RX, 100);
	wait(10);
	set_val(PS4_RX, 100);
	set_val(PS4_RY, -30);
	wait(10);	
	set_val(PS4_RX, 20);
	set_val(PS4_RY, -100);
	wait(10);	
	set_val(PS4_RY, -100);
	wait(10);
}

combo LowStamina{
	set_val(XB1_X,100);
	wait(LowStaminaTime);
	set_val(XB1_X,0);
	wait(20);
}

combo Takeover
{
	set_val(PS4_R3, 100);
	wait(100);
}	
 