function p_complete() { 
    i_cnt++;
    if (i_cnt > VerticalTime[ProfileIdx] * 10) {
    	//--avoid stack overflow
        i_cnt = VerticalTime[ProfileIdx] * 10;
        //--progression complete
        return 1;
    }
    //--progression not complete
    return 0;
}
function p_val() {
    i_num++;
    if(i_num == i_pnt) {
        //--reset counter
    	i_num = 0;
    	//--adjust ar_y 
    	ar_y += i_val;
    }
    return ar_y;
}	
