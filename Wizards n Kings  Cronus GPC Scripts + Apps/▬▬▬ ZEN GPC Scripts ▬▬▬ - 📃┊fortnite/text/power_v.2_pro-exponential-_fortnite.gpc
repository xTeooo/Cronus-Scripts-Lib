)
 /*
 .~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~.
(                    Menu On/Off                     )
 `-~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-*
*/
int AimAssistExploit       = On;           
int AimAssistExploit2      = On;
int TargetTrackerExploit   = On;
int AimAbuse               = On;
int Antirecoil             = On;
int RapidFire              = Off; //Rapid Fire (ADS + LEFT) ON/OFF
int DropShot               = Off; //DROPSHOT (ADS + CROUCH) ON/OFF
int BuildTracker           = On;  //BuildTracker (Leave Alone) When building the mod, it is suspended, when leaving the construction mode the mods will be activated again, this serves so that when building no mod interferes.

/*
 .~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~. 
(                Mod Values/Times                    ) 
 `-~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-* 
*/ 

//AimAssist[PRO] Values/Times
int AimAssist_Strength  = 28; //28
int tt_aim_speed        = 95;

//AimAbuse Values/Times
int AimAbuse_HOLD       = 260;
int AimAbuse_RELEASE    = 5;

//AntiRecoil Values/Times
int AntiRecoil          = 14;
 
//Dropshot Values/Times
int dsdown = 150;     // Time you are down (Change it to be more realistic dropshot. I like it with this value)
int dsup   = 20;   // Time you are up (Change it to be more realistic dropshot. I like it with this value)

/*
 .~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~.
(                 Script Variables                   )
 `-~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-*
*/
 int BuildTrack;
 int SnapShot_hold; 
 int SnapShot_rest;
 int Anti_Recoil;

main{
 vm_tctrl(-9);
//∙∙·▫▫ᵒᴼᵒ▫ₒₒ▫ᵒᴼᵒ▫ₒₒ▫ᵒᴼᵒ BuildTracker ᵒᴼᵒ▫ₒₒ▫ᵒᴼᵒ▫ₒₒ▫ᵒᴼᵒ▫▫·∙∙
if(BuildTracker){
if(event_press(Build)){
 BuildTrack = !BuildTrack;}
if(!BuildTrack){

//∙∙·▫▫ᵒᴼᵒ▫ₒₒ▫ᵒᴼᵒ▫ₒₒ▫ᵒᴼᵒ AimAssist V2[PRO] ᵒᴼᵒ▫ₒₒ▫ᵒᴼᵒ▫ₒₒ▫ᵒᴼᵒ▫▫·∙∙
if(TargetTrackerExploit){
if(get_val(Ads) && get_val(Fire)){
sensitivity(RX,0,tt_aim_speed); 
sensitivity(RY,0,tt_aim_speed);
}}
if(AimAssistExploit){   
if(get_val(Ads)> 95){ combo_run(Auto_Aim);}
if(abs(get_val(9)) > 19 || abs(get_val(10)) > 19) 
combo_stop(Auto_Aim);
}
if(AimAssistExploit2){                             
if(get_val(Ads) && get_val(Fire)){  
SnapShot_hold = 140;
SnapShot_rest = 0;            
combo_run(SnapShot);} 
else if 
(combo_running(SnapShot)){     
combo_stop(SnapShot);}                                             
}                         

//∙∙·▫▫ᵒᴼᵒ▫ₒₒ▫ᵒᴼᵒ▫ₒₒ▫ᵒᴼᵒ Antirecoil V2 ᵒᴼᵒ▫ₒₒ▫ᵒᴼᵒ▫ₒₒ▫ᵒᴼᵒ▫▫·∙∙
if(Antirecoil){                               
if( get_val(Ads) && get_val(Fire)){      
combo_run(AntiRecoil); }               
if( abs(get_val(RY)) > 36 || abs(get_val(RX)) > 36) { 
combo_stop (AntiRecoil); }               
}

//∙∙·▫▫ᵒᴼᵒ▫ₒₒ▫ᵒᴼᵒ▫ₒₒ▫ᵒᴼᵒ AimAbuse V2 ᵒᴼᵒ▫ₒₒ▫ᵒᴼᵒ▫ₒₒ▫ᵒᴼᵒ▫▫·∙∙
if(AimAbuse){
if(get_val(Ads)> 95){
combo_run(AimAbuse);}
if(event_release(Ads)){ 
combo_stop(AimAbuse);}
}

//∙∙·▫▫ᵒᴼᵒ▫ₒₒ▫ᵒᴼᵒ▫ₒₒ▫ᵒᴼᵒ RapidFire V2 ᵒᴼᵒ▫ₒₒ▫ᵒᴼᵒ▫ₒₒ▫ᵒᴼᵒ▫▫·∙∙
if(get_val(Ads) && event_press(Left)){
RapidFire =! RapidFire;
if(RapidFire == TRUE){combo_run(Notify);g=0;h=2;}
if(RapidFire == FALSE){combo_run(Notify);g=2;h=0;} 
}
if(RapidFire){
if(get_val(Ads)&&get_val(Fire)){combo_run(RapiidFire);}
}
}

//∙∙·▫▫ᵒᴼᵒ▫ₒₒ▫ᵒᴼᵒ▫ₒₒ▫ᵒᴼᵒ Dropshot V2 ᵒᴼᵒ▫ₒₒ▫ᵒᴼᵒ▫ₒₒ▫ᵒᴼᵒ▫▫·∙∙
if(get_val(Ads) && event_press(Crouch)){
DropShot =! DropShot;
if(DropShot == TRUE){combo_run(Notify);g=0;h=2;}
if(DropShot == FALSE){combo_run(Notify);g=2;h=0;} 
}
if(DropShot){
if(get_val(Ads)&&get_val(Fire)){combo_run(DropShot);}
}
}
}
/* 
 .~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~. 
(                   Combos                           ) 
 `-~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-* 
*/ 
combo Auto_Aim {          
set_val(10, (AimAssist_Strength)); wait(10)
set_val(9, (AimAssist_Strength)); wait(10)
set_val(10, AimAssist_Strength * -1); wait(10)
set_val(9, AimAssist_Strength * -1); wait(10)}

combo SnapShot { 
set_val(Ads, 100);    
wait(SnapShot_hold);                
set_val(Ads, 0);      
wait(SnapShot_rest);}

combo AimAbuse {
set_val(Ads, 100);
wait(AimAbuse_HOLD);
set_val(Ads, 0);
wait(AimAbuse_RELEASE);}

combo DropShot{
set_val(5,100);
wait(dsdown);
wait(dsup);
set_val(5,100);
}

combo RapiidFire {
set_val(Fire, 100); 
wait(35);
set_val(Fire, 0); 
wait(25);
}

combo AntiRecoil { 
if(get_val(Fire)) {
Anti_Recoil = get_val(10) + AntiRecoil;
if(Anti_Recoil > 100) Anti_Recoil = 100;
set_val(10, Anti_Recoil);
}}

combo Notify {
if(Notify){set_rumble(RUMBLE_B,65);}
set_led(LED_1, 0);
set_led(LED_2, g);
set_led(LED_3, h);
set_led(LED_4, 0);
wait(550);
set_led(LED_1, 0);
set_led(LED_2, 0);
set_led(LED_3, 0);
set_led(LED_4, 0);
wait(250);
set_led(LED_1, 0);
set_led(LED_2, g);
set_led(LED_3, h);
set_led(LED_4, 0);
wait(250);
set_led(LED_1, 0);
set_led(LED_2, 0);
set_led(LED_3, 0);
set_led(LED_4, 0);
wait(250);
set_led(LED_1, 0);
set_led(LED_2, g);
set_led(LED_3, h);
set_led(LED_4, 0);
wait(250);
reset_leds();
}