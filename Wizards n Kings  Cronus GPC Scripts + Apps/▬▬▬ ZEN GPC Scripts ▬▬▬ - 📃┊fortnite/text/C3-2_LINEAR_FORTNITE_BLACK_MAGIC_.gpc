}

function AimAssistEcho() {
	
	TimerEcho += get_rtime();
	
	if (TimerEcho < StepTime) {
		
		EchoVal = EchoValue;
	
	} else if (TimerEcho < (StepTime * 2)) {
		
		EchoVal = EchoValue + Increase;
	
	} else if  (TimerEcho < (StepTime * 3)) {
		
		EchoVal = EchoValue + (Increase * 2);
	
	} else if  (TimerEcho < (StepTime * 4)) {
		
		EchoVal = EchoValue + (Increase * 3);
	
	} else if  (TimerEcho < (StepTime * 5)) {
		
		EchoVal = EchoValue + (Increase * 4);
	}
	
	if  (TimerEcho > StepTime * 6) {
		
		TimerEcho = 0;
	}
	
	combo_run(EchoAssist);
}

function DoGzDecider(Strength,Step) {

	if (Power) {
		
		Ypower = Strength;
		Xpower = Strength * -1;
		Increments = Step;
		Power = Off;
	}
	
	if (!power) {
	
		Xpower += Increments;
		Ypower -= Increments;
	
	} else {
		
		Xpower -= Increments;
		Ypower += Increments;
	}
	
	CountPower ++;
	
	if (CountPower >= (Strength / Increments)) {
		
		power = !power;
		CountPower = 0;
	}	
	
	if (Axis > Deadzone) {
	
		if (Reducer) {
		
			set_Ival(AimY,(Ypower - Strength / 2));
			set_Ival(AimX,(Xpower - Strength / 2 * -1));
		
		} else {
		
			set_val(AimY,(Ypower - Strength / 2) + get_ival(AimY));
			set_val(AimX,(Xpower - Strength / 2 * -1) + get_ival(AimX));	
		}
		
	} else {
		
		if (Reducer) {
				
			set_Ival(AimY,(Ypower - Strength / 2));
