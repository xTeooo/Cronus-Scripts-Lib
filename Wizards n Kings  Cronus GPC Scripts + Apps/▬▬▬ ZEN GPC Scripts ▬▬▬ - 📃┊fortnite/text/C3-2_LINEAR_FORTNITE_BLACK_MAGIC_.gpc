		set_Val(stick + 8, (rad * cos(angle))/ 100);
	}
	
	if (TaylorShapes == 8) { 
		
		if (Position == 0) { 
			
			set_Val(stick + 7, (rad * cos(135 + angle)) / 100 + Tracker1);
		    set_Val(stick + 8, (rad * sin(45 + angle)) / 100 + Tracker2);
		} 
		
		if (Position == 1) { 
			set_Val(stick + 7, (rad * cos(45 + angle)) / 100  + Tracker1);
