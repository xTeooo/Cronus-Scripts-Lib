function SixthSenseRecoil() {
   
	if (Start) {
		
    	vTime = Off;
    	MoveY = Off;
    	MovementY = Off;
    	vP = 19;
    	vT = 25;
	    
		Start = Off;
	}
	
	if (AntiRecoilMode == RecoilSet) {
	
	    if (vTime <= modValue1[vT]) {
	    	
	    	set_Val(AimY,modValue1[vP]);
	    
	    } else {
	    	
	    	set_Val(AimY,modValue1[vP]);
	    }
	    
	    if (vTime >= modValue1[vT]) {
	    
	        if (get_ival(Ads) && get_ival(Fire)) {
	        	
	        	if (vT == 31 && vTime >= modValue1[vT]) {
	        		vP = 18;
	        		vT = 24;
	        	}
	        }
	        
	        vTime = Off;
	        vP = clamp(vP++,19,24);
	        vT = clamp(vT++,25,31);
	    }		
	}
	
	if (AntiRecoilMode == RecoilProgression) {
	
		if (!MovementY) {
	
			if (!MoveY)	
				
				YAxis = modValue1[vP];
	
			if (vTime >= modValue1[vT]) {
				
				MoveY = On;
		
			if (modValue1[vP] > modValue1[vP + 1]) 
				
				YAxis --;
			
			if (modValue1[vP] < modValue1[vP + 1]) 
				
				YAxis ++;
				vTime = Off;
		}
		
		if (YAxis == modValue1[vP + 1]) {
			
			MoveY = Off;
			vP = clamp(vP++,19,24);
	        vT = clamp(vT++,25,31);
		}
			else if (vP == 24) {
				
				MovementY = On;
			}
		}
		
		set_Val(AimY,YAxis);
	}
    	vTime += get_rtime();
}

