		}
	}
}

function PolarFlow(Pfb,Psv,Pfv,Pv,Pf,Pid) {
	
	if(Pf) {
		
		Pfb += Pv;
	
		if(Pfb <= Pfv)  
    	
    		return Pfb;
		
		if(Pfb > Pfv) {
			
			Pvid[Pid] = Off;
			return Pfb - Pv;
    	}
	
	} else {
		
		Pfb -= Pv;    
		
		if(Pfb >= Psv)  
			
			return Pfb;

		if(Pfb < Psv) {
			
			Pvid[Pid] = On; 
			return Pfb + Pv;
		}
	} 
   	
   	return Off;    
}

