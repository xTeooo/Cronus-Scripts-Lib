function adjust(val,inc,min,max) {

    if (event_press(Down)) {
    	
    	val += inc;
    	
    	if (val > max) val = min;
    	if (val < min) val = max;
    	
    	render = On;
    }
    
    if (event_press(Up)) {
    	
    	val -= inc;
    	
    	if (val > max) val = min;
    	if (val < min) val = max;
    	
    	render = On;
    }
    
    return val;
}

function adjustv(val,inc,min,max) {

    if (press(Right)) {
    	
    	val += inc;
    	
    	if (val > max) val = min;
    	if (val < min) val = max;
	   	    
    	render = On;
    	
    	if (val > 0)
    		
    		update();
    }
    
    if (press(Left)) {
    
    	val -= inc;
    	
    	if (val > max) val = min;
    	if (val < min) val = max;
	    
    	render = On;
    	
    	if (val > 0)
    		
    		update();
    }

	return val;
}

