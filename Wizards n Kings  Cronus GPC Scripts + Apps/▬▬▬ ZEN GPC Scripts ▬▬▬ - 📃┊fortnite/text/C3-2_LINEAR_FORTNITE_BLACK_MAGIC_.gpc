    		block_all_inputs();
    		Input_Block = 0;
    			
    	do {
      				
      		if (Input_Block == 21) Input_Block = 27;
      					
      		if (Input_Block >= 9 && Input_Block <= 12) {
      		
      			set_val(Input_Block, get_ival(Input_Block));
      						
      		} else if (event_press(Input_Block)) Input_Press = Input_Block;
      			
      			Input_Block++;
    		} 
    		
    		while (Input_Block != 28);
  	
  	} else if (Input_Press == Block) {
  	
		set_val(Block,((Input_Held + 50) > get_ptime(Block)) * 100);
	}
}

