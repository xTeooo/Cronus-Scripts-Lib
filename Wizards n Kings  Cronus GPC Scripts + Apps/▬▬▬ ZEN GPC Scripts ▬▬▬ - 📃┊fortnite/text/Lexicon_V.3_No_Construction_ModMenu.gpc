    //            HEAD SHOT ASSIST 
    //===========================================   
    if (Head_Shot) {

        if (get_ival(AdsBtn) && get_ival(FireBtn) || HipAssistHead && get_ival(FireBtn)) {

            if (abs(get_ival(RY)) < HeadMax) {
        
                Head += HeadStep;
                combo_run(HeadShotAssist);
		}
	}
            if (!get_ival(FireBtn)) Head = 0;
            if (Head >= HeadMax) combo_stop(HeadShotAssist)
	}
}

    //===========================================
