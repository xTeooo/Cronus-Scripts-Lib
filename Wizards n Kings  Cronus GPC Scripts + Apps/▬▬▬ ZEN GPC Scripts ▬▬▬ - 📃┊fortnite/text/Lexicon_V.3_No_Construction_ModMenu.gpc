	            Anti_Recoil                    = ToggleSwitch(ModNameIdx == 4, Anti_Recoil);
	            Head_Shot                      = ToggleSwitch(ModNameIdx == 5, Head_Shot);
	            Drop_Shot                      = ToggleSwitch(ModNameIdx == 6, Drop_Shot);
	            Slide_Shot                      = ToggleSwitch(ModNameIdx == 7, Slide_Shot);
