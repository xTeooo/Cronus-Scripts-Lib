    }
}
    //===========================================
    //          FIRE ASSIST EXPLOIT V3
	//===========================================  
    if (Assist_Fire_Exploit) {

        if (get_ival(FireBtn) && !get_ival(AdsBtn)) {

            if (abs(get_ival(LX)) <= 40 && abs(get_ival(LY)) <= 35) {

                combo_run(FireAssist);
            
            } else {

                combo_stop(FireAssist);
        }
    }
}
    //===========================================
    //          100% ACCUARCY EXPLOIT V3
	//===========================================   
    if (Accuracy_Exploit) {

