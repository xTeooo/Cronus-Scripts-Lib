	//===========================================         
	if (get_val(CrouchShotHold) && event_press(CrouchShotPress)) {
        
          Drop_Shot = !Drop_Shot;
      }
    
		if (Drop_Shot && get_ival(FireBtn)) {
            
            combo_run(DropShot);
        }    
            else {
               
            combo_stop(DropShot);
}

    
    //===========================================
    //               SLIDE SHOT
	//===========================================        
	
	if (get_val(SlideHold) && event_press(SlidePress)) {
        
          Slide_Shot = !Slide_Shot;
      }
    
