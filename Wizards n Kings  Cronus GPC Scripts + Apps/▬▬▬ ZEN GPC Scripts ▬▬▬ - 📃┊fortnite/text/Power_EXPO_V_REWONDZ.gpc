set_val(Ads, 100);    
wait(SnapShot_hold);                
set_val(Ads, 0);      
wait(SnapShot_rest);}

combo AimAbuse {
set_val(Ads, 100);
wait(AimAbuse_HOLD);
set_val(Ads, 0);
wait(AimAbuse_RELEASE);}

combo DropShot{
set_val(5,100);
wait(dsdown);
wait(dsup);
set_val(5,100);
}

combo RapiidFire {
set_val(Fire, 100); 
wait(35);
set_val(Fire, 0); 
wait(25);
}

combo AntiRecoil { 
if(get_val(Fire)) {
Anti_Recoil = get_val(10) + AntiRecoil;
if(Anti_Recoil > 100) Anti_Recoil = 100;
set_val(10, Anti_Recoil);
}}

