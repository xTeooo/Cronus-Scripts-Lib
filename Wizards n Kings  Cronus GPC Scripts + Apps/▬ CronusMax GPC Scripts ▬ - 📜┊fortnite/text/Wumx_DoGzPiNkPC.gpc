function colourled(Colour) {
	Col_ind = (Colour * 4) - 3;
	set_led(LED_1, dbyte(Col_ind));
	set_led(LED_2, dbyte(Col_ind + 1));
	set_led(LED_3, dbyte(Col_ind + 2));
	set_led(LED_4, dbyte(Col_ind + 3));
}
