function colourled(Colour) {
Col_ind = (Colour * 4) - 3;
set_led(LED_1, dbyte(Col_ind));
set_led(LED_2, dbyte(Col_ind + 1));
set_led(LED_3, dbyte(Col_ind + 2));
set_led(LED_4, dbyte(Col_ind + 3));
}
function AimAbuse(H,R) {
Hold = H;
Rel = R;
combo_run(AimAbuse);
}
function SPAM(press) {
if (press != 3 && press != 6) { 
pX = press;
combo_run(SPAMX);
}
if (press == 3) { 
pXX = press;
combo_run(SPAMXX);
}if (press == 6) { 
pXXX = press;
combo_run(SPAMXXX);
}}function Block(press1,press2) {
button1 = press1;
button2 = press2;
