define Reset    = XB1_LS;// Edit Reset In Game

// DO NOT ALTER BELOW
int DamAngle,DamCos,DamSin,DamX,DamY,ActualMagnitude,MaxMagnitude,RadiusDam,AngleDam,AAT,Axis,Head,BuildTrack,BuildCount,Edit1,Edit2,EditT,Axe,BuildPro;
int BoostAR,AxisCY,AxisLY,TimeValue,OutPut,RumValue,RecoilTimer;
define On  = TRUE;
define Off = FALSE;
define VersionOne   = 1;
define VersionTwo   = 2;
define VersionThree = 3;
int PolarAxis;
