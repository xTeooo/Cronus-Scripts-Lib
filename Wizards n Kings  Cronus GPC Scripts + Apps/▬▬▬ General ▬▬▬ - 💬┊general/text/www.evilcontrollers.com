<script>
  require(['jquery','underscore'],function($,_){
  var form = $('#newsletter-validate-detail');
  form.submit(function(e) {
    if(form.validation('isValid')){
            var email = $("#newsletter").val();
            var url = form.attr('action');
            var loadingMessage = $('#loading-message');
            if(loadingMessage.length == 0) {
                form.find('.field.form-group.newsletter').append('<div id="loading-message" style="display:none;padding-top:10px;color: #54d216;font-size: 14px;">&nbsp;</div>');
                var loadingMessage = $('#loading-message');
            }
            e.preventDefault();
            try{
                form.find('.field.form-group.newsletter .control').hide();
                form.find('.actions').hide();
                loadingMessage.html('Submitting...').show().removeClass('error');
                $.post(url, {"email": email}, function(data, status){
                    var cookieMessage = _.unique($.cookieStorage.get('mage-messages'), 'text');
                    $.mage.cookies.set('mage-messages', '', {
                        samesite: 'strict',
                        domain: ''
                    });
                  if(status == 'success') {
                      var message = 'Thank you for Subscribing.';
                      form.find('.field.form-group.newsletter .control').hide();
                      form.find('.actions').hide();
                      if(cookieMessage.length) {
                          message = cookieMessage[0].text;
                          if(cookieMessage[0].type !='success') {
                              form.find('.field.form-group.newsletter .control').show();
                              form.find('.actions').show();
                              loadingMessage.addClass('error');
                              form.find('.field.form-group.newsletter .control input[type="email"]').val(form.find('.field.form-group.newsletter .control input[type="email"]').val());
                          }
                      }
                    loadingMessage.html(message);

                  } else {
                      var message = 'Sorry there was an error.';
                      if(cookieMessage.length) {
                          message = cookieMessage[0].text;
                      }
                    loadingMessage.html(message);
                      loadingMessage.addClass('error');
                    form.find('.field.form-group.newsletter .control').show();
                    form.find('.actions').show();
                  }
                });
            } catch (e){
                form.find('.field.form-group.newsletter .control').show();
                form.find('.actions').show();
                loadingMessage.html(e.message);
            }
            return false;
        }
    });
  })
</script>
