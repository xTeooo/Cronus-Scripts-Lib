//------------------------------------------------------------------------------
define RT = 4; //R2/RB//--------------------------------------------------------
define LT = 7; //L2/LB//--------------------------------------------------------
//------------------------------------------------------------------------------
define RB= 3; //R1/RT//---------------------------------------------------------
define LB = 6; //L1/LT//--------------------------------------------------------
//------------------------------------------------------------------------------
define RS = 5; //R3/RS//--------------------------------------------------------
define LS = 8; //L3/LS//--------------------------------------------------------
//------------------------------------------------------------------------------
define A = 19; //PS4 CROSS/XB1 A//----------------------------------------------
define X = 20; //PS4 SQUARE/XB1 X//---------------------------------------------
define Y = 17;	//PS4 TRIANGLE/XB1 Y//------------------------------------------
define B = 18; //PS4 CIRCLE/XB1 B//---------------------------------------------
//------------------------------------------------------------------------------
define UP = 13; //Up//----------------------------------------------------------
define DOWN = 14; //Down//------------------------------------------------------
define LEFT = 15; //Left//------------------------------------------------------
define RIGHT = 16; //Right//----------------------------------------------------
//------------------------------------------------------------------------------
define START = 1; //VIEW/SHARE//------------------------------------------------
define BACK = 2; //Options/Menu//-----------------------------------------------
//------------------------------------------------------------------------------
define TOUCHPAD = 27; //Touchpad//----------------------------------------------
define RX = 9; //PS4 RX / Xbox RX//---------------------------------------------
define RY = 10; //PS4 RY / Xbox RY//--------------------------------------------
define LX = 11; //XB1_LX //PS4_LX//---------------------------------------------
define LY = 12; //XB1_LY //PS4_LY//---------------------------------------------
//------------------------------------------------------------------------------
int HairTrigger = TRUE;//-------------------------------------------------------
int rapidfire = FALSE;//--------------------------------------------------------
int Pulse_shot = FALSE;//-------------------------------------------------------
int lock = FALSE;//-------------------------------------------------------------
int AAT = TRUE;//---------------------------------------------------------------
int aimbot = TRUE;//------------------------------------------------------------
//------------------------------------------------------------------------------
//------------------------------------------------------------------------------
main {   //start of end---------------------------------------------------------
//------------------------------------------------------------------------------
vm_tctrl(-2); // Run the VM every 2ms-------------------------------------------
//------------------------------------------------------------------------------
if (get_val(LT) && event_release(RIGHT)) {
        aimbot = !aimbot;
 if(aimbot) {
            combo_run(RumbleOnce);
    }  else {
            combo_stop(aimbotC);
            combo_run(RumbleTwice);}
    }
if(aimbot) {
 if(get_val(LT)>=95) {
            combo_run(aimbotC);} }
  if(event_release(LT)){ combo_stop(aimbotC);}
//------------------------------------------------------------------------------
if((get_val(LT)) && (event_press(START))) {
        lock = !lock;
 if(lock) {
            combo_run(RumbleOnce); 
        }  else {
           combo_run(RumbleTwice);} }
//------------------------------------------------------------------------------
if(lock){
 if (get_val(LT) && get_val(RT)) {
      AAT = !AAT;
      combo_run(shake);
      combo_run(RumbleOnce);
        } else {
      combo_run(RumbleTwice);
      combo_stop(shake);} }
//------------------------------------------------------------------------------
if (get_val(RX)<-15||get_val(RX)>15||get_val(10)<-15||get_val(10)>15||get_val(RX)<-15||get_val(RX)>15||get_val(10)<-15||get_val(10)>15){ 
combo_stop(shake);}
//------------------------------------------------------------------------------
if(get_val(LT) && get_val(RT)) {
            combo_run(AR);} 
 if(get_val(LT) && get_val(RT)){ combo_stop(AR);}
//------------------------------------------------------------------------------
if (get_val(RX)<-15||get_val(RX)>15||get_val(RY)<-15||get_val(RY)>15||get_val(RX)<-15||get_val(RX)>15||get_val(RY)<-15||get_val(RY)>15){ 
combo_stop(AR);}
//------------------------------------------------------------------------------
if(get_val(X)) {
      combo_run(xspam);}
 if(event_release(X)){ combo_stop(xspam);} 
  if(get_val(LT)){combo_stop(xspam);}
//------------------------------------------------------------------------------
if(HairTrigger){ 
 if(get_val(RT))
       set_val(RT,100);}
//------------------------------------------------------------------------------
if (Pulse_shot){
 if (get_val(LT) && get_val(RB)) {
      combo_run(Pulse_shotC);
    combo_stop(xspam);
    set_val(RB, 0);
          } else {
      combo_stop(Pulse_shotC);} }
//------------------------------------------------------------------------------
if(get_val(LT) && event_press(LEFT)) {
        Pulse_shot=!Pulse_shot; 
 if(Pulse_shot){
        combo_run(RumbleOnce); 
        }  else {
        combo_run(RumbleTwice);} }
//------------------------------------------------------------------------------
//------------------------------------------------------------------------------
}//end of main------------------------------------------------------------------
//------------------------------------------------------------------------------
//---------------------------------combo code-----------------------------------
//------------------------------------------------------------------------------
//------------------------------------------------------------------------------
combo AR {
    set_val(RY,36);
wait(10);
    set_val(RY,0);
wait(10);}
//------------------------------------------------------------------------------
combo shake {
    set_val(RX,-30);
wait(30);
    set_val(RY,-25);
wait(30);
    set_val(RY,20);
wait(30);
    set_val(RX,25);
wait(30);}
//------------------------------------------------------------------------------
combo Pulse_shotC {
    set_val(RT, 100);
wait(10);
    set_val(RT, 0);
wait(290);}
//------------------------------------------------------------------------------
combo xspam {
    set_val(X, 100);
wait(80);
    set_val(X, 0);}
//------------------------------------------------------------------------------
combo aimbotC {
    set_val(LT, 100);
wait(300);
    set_val(LT, 0);
wait(20);}
//------------------------------------------------------------------------------
combo RumbleOnce {
    set_rumble(RUMBLE_A, 100);
wait(250);
    reset_rumble();}
//------------------------------------------------------------------------------
combo RumbleTwice {
    set_rumble(RUMBLE_A, 100);
wait(250);
    set_rumble(RUMBLE_A, 0);
wait(250);
    set_rumble(RUMBLE_A, 100);
wait(250);
    reset_rumble();}
//------------------------------------------------------------------------------
























































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































































