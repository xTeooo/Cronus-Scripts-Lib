define dead_zone_left = 12;		//Adjust for your controller
define dead_zone_right = 14;	//Adjust for your controller

main{
vm_tctrl(-9);
block_rumble ( );
swap(PS4_TOUCH,PS4_SHARE);
	if(abs(get_val(11)) < dead_zone_left && abs(get_val(12)) < dead_zone_left){
		set_val(11 , 0);
		set_val(12 , 0);
	}
	
	if(abs(get_val(9)) < dead_zone_right && abs(get_val(10)) < dead_zone_right){
		set_val(9 , 0);
		set_val(10 , 0);
	}
}