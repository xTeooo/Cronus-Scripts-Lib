	        	HGunAASpd        = HandGunAASpeed;
	        	HGunAAStr        = HandGunAASize;
	  
	        	InitializeShapeChange()
    		}
    		if(!ToggleSniperMode[ProfileIdx] == 1)
    		{
    			HandGunVStrength = TaylorARVert[ProfileIdx];
