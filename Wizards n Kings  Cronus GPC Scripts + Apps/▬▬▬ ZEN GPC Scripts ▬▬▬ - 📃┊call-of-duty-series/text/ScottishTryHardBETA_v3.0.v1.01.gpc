			ToggleJumpShot  [ProfileIdx]  = QuickToggleMods(ActivateHoldButton[4] , ActivatePressButton[4]  ,ToggleJumpShot  [ProfileIdx],18); 
	
			if(ToggleSniperMode[ProfileIdx] == 1) 
	        {
