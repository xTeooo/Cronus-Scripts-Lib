function EditableSwitch(f_val) { 
	  
	if(ModORVal[2] == ValRangeMinMax[ValIdx++][0]){
        if(ValRangeMinMax[ValIdx][6] == 1){
        	if(event_press(PS4_UP)){
	      		f_val += 1;
	      		if((f_val + AAValues[ToggleAdminVals[ProfileIdx]][ModORVal[2]])  > ValRangeMinMax[ModORVal[2]][2])
	       			f_val = ValRangeMinMax[ModORVal[2]][2] - AAValues[ToggleAdminVals[ProfileIdx]][ModORVal[2]];
                	DisplayMenu = TRUE;    
            }
            if(event_press(PS4_DOWN)){	
            	f_val -= 1;
			    if((f_val + AAValues[ToggleAdminVals[ProfileIdx]][ModORVal[2]]) < inv(ValRangeMinMax[ModORVal[2]][1]))
			    	f_val = inv(ValRangeMinMax[ModORVal[2]][1] + AAValues[ToggleAdminVals[ProfileIdx]][ModORVal[2]]);	
		        	DisplayMenu = TRUE;
            }
            if(ValRangeMinMax[ValIdx][0] == 17){
	    		String(CurveType[AAValues[ToggleAdminVals[ProfileIdx]][ModORVal[2]] + f_val],Small,6,0);
	        }
	        else if(ValRangeMinMax[ValIdx][0] == 18){
		    	if(aimAssistType == 0)
		    		String(Misc[25],Small,6,0);
		    	else if(aimAssistType == 1)
		    		String(CircularCurve[AAValues[ToggleAdminVals[ProfileIdx]][ModORVal[2]] + f_val],Small,6,0);
		    	else if(aimAssistType == 2)
		    		String(SquareCurve[AAValues[ToggleAdminVals[ProfileIdx]][ModORVal[2]] + f_val],Small,6,0);
		    	else if(aimAssistType == 3)
		    		String(MixedCurve[AAValues[ToggleAdminVals[ProfileIdx]][ModORVal[2]] + f_val],Small,6,0);
	        }
	        else { 
	        
	        	String(ToggleOnly[AAValues[ToggleAdminVals[ProfileIdx]][ModORVal[2]] + f_val],Small,6,0); 
		    }       
        } 
        if(ValRangeMinMax[ValIdx][6] == 0 && ValRangeMinMax[ValIdx][0] != 16){
        	
			f_val = MultiFunction(PS4_UP,PS4_DOWN,f_val,ValRangeMinMax[ModORVal[2]][3],ValRangeMinMax[ModORVal[2]][1],ValRangeMinMax[ModORVal[2]][2]);
			
		    if(ValRangeMinMax[ValIdx][0] == 8){
		    	String(CurveType[ToggleAAType[ProfileIdx]],Small,6,0);
		    }
		    else if(ValRangeMinMax[ValIdx][0] == 32){
		    	String(CurveType[ToggleAAType[ProfileIdx]],Small,6,0);
		    } 
		    else if(ValRangeMinMax[ValIdx][0] == 9){
		    	if(ToggleAAType[ProfileIdx] == 0)
		    		String(Misc[25],Small,6,0);
		    	else if(ToggleAAType[ProfileIdx] == 1)
		    		String(CircularCurve[TaylorShapesv1[ProfileIdx]],Small,6,0);
		    	else if(ToggleAAType[ProfileIdx] == 2)
		    		String(SquareCurve[TaylorShapesv1[ProfileIdx]],Small,6,0);
		    	else if(ToggleAAType[ProfileIdx] == 3)
		    		String(MixedCurve[TaylorShapesv1[ProfileIdx]],Small,6,0);		
		    }
		   
		    else if(ValRangeMinMax[ValIdx][0] == 33){
		    	if(ToggleAAType[ProfileIdx] == 0)
		    		String(Misc[25],Small,6,0);
		    	else if(ToggleAAType[ProfileIdx] == 1)
		    		String(CircularCurve[TaylorShapesv1[ProfileIdx]],Small,6,0);
		    	else if(ToggleAAType[ProfileIdx] == 2)
		    		String(SquareCurve[TaylorShapesv1[ProfileIdx]],Small,6,0);
		    	else if(ToggleAAType[ProfileIdx] == 3)
		    		String(MixedCurve[TaylorShapesv1[ProfileIdx]],Small,6,0);
		    }
		    else
		    {
				if(f_val == 1)
					String(Misc[0],Small,6,0);   
				else 
				    String(Misc[1],Small,6,0);  
		    }
		}
		if(ValRangeMinMax[ValIdx][0] == 16){
			if(event_press(PS4_UP)){ 
				f_val = Refresh(f_val + 1,0,20) 
				ResetAdminValues();
				DisplayMenu = TRUE;
			} 
			if(event_press(PS4_DOWN)){ 
				f_val = Refresh(f_val - 1,0,20) 
				ResetAdminValues();
				DisplayMenu = TRUE;
			} 
		    String(AdminAA[ToggleAdminVals[ProfileIdx]],Small,6,0);
		}
	}      	
    return f_val;
}
