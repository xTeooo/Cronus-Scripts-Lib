    if(ModORVal[2] == ValRangeMinMax[ValIdx++][0]){ 
	    if(get_ival(Ads)){ 
	    	if(ValRangeMinMax[ValIdx][6] != 1){ 
		        ValueIndex = MultiFunction(PS4_RIGHT,PS4_LEFT,ValueIndex,ValRangeMinMax[ModORVal[2]][3],ValRangeMinMax[ModORVal[2]][1],ValRangeMinMax[ModORVal[2]][2]);  
		        ValueIndex = MultiFunction(PS4_UP,PS4_DOWN,ValueIndex,ValRangeMinMax[ModORVal[2]][4],ValRangeMinMax[ModORVal[2]][1],ValRangeMinMax[ModORVal[2]][2]);  
		    } 
		    if(ValRangeMinMax[ValIdx][6] == 1){
		    
		    	if(get_ival(Ads)){ 
			    	if(event_press(PS4_RIGHT)){
			      		ValueIndex += 1;
			      		if((ValueIndex + AAValues[ToggleAdminVals[ProfileIdx]][ModORVal[2]])  > ValRangeMinMax[ModORVal[2]][2])
			       		ValueIndex = ValRangeMinMax[ModORVal[2]][2] - AAValues[ToggleAdminVals[ProfileIdx]][ModORVal[2]];
		                DisplayMenu = TRUE;  
		            }
		            if(event_press(PS4_LEFT)){	
		            	ValueIndex -= 1;
			      		if((ValueIndex + AAValues[ToggleAdminVals[ProfileIdx]][ModORVal[2]]) < inv(ValRangeMinMax[ModORVal[2]][1]))
			       		ValueIndex = inv(ValRangeMinMax[ModORVal[2]][1] + AAValues[ToggleAdminVals[ProfileIdx]][ModORVal[2]]);	
		               	DisplayMenu = TRUE;
		            }
		        
		            if(event_press(PS4_UP)){
			      		ValueIndex += 10;
			      		if((ValueIndex + AAValues[ToggleAdminVals[ProfileIdx]][ModORVal[2]])  > ValRangeMinMax[ModORVal[2]][2])
			       		ValueIndex = ValRangeMinMax[ModORVal[2]][2] - AAValues[ToggleAdminVals[ProfileIdx]][ModORVal[2]];
		                DisplayMenu = TRUE;    
		            }
		            if(event_press(PS4_DOWN)){	
		            	ValueIndex -= 10;
			      		if((ValueIndex + AAValues[ToggleAdminVals[ProfileIdx]][ModORVal[2]]) < inv(ValRangeMinMax[ModORVal[2]][1]))
			       		ValueIndex = inv(ValRangeMinMax[ModORVal[2]][1] + AAValues[ToggleAdminVals[ProfileIdx]][ModORVal[2]]);	
		               	DisplayMenu = TRUE;
		            }
	        	} 
	        }	 
	    }
       	if(ValRangeMinMax[ValIdx][6] == 1){ 
        	NumberToString(AAValues[ToggleAdminVals[ProfileIdx]][ModORVal[2]] + ValueIndex,FindDigits(AAValues[ToggleAdminVals[ProfileIdx]][ModORVal[2]] + ValueIndex));  
        }
        else if(ValRangeMinMax[ValIdx][0] == 4 || ValRangeMinMax[ValIdx][0] == 7) 
        	NumberToString(ValueIndex * 100, FindDigits(ValueIndex * 100));
        else
        	NumberToString(ValueIndex, FindDigits(ValueIndex));     
    }
    return ValueIndex;                
 }
