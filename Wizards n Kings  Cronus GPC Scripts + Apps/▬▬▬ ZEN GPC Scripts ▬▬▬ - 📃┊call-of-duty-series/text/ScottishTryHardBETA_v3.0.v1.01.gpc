function MultiFunction(Inc,Dec,Value,Adjust,Min,Max){  
    
    if(event_press(Inc) || get_ival(Inc) && get_ptime(Inc) > 250){
    	Value += Adjust
    	DisplayMenu = TRUE;
    	if(Value > Max) 
    		return Min;
  
    	if(!Value)
	        combo_run(RumbleOff);
	    else
	        combo_run(RumbleOn);  
    }
    if(event_press(Dec) || get_ival(Dec) && get_ptime(Dec) > 250){
    	Value -= Adjust
    	DisplayMenu = TRUE;
    
    	if(Value < Min)
    		return Max;
    		
    	if(!Value)
	        combo_run(RumbleOff);
	    else
	        combo_run(RumbleOn);  
    }
    set_val(Inc,0); set_val(Dec,0);
    return Value; 
}
