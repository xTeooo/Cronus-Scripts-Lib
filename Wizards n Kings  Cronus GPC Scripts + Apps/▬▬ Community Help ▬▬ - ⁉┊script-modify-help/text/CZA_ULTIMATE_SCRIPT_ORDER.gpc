function adjust_recoil(val, p, check, s_mod) {
	if(p > 0) 
	{
		s = (val * Sight_Vals[primary_sight]) / 100;

		if(check < 2)
			b = ((val * Barrel_Vals[primary_barrel][0]) / 100) * -1;
			sens = (val * s_mod) / 100;
			val = val + (s + b + sens);
	}
	else
	{	
		s = (val * Sight_Vals[secondary_sight]) / 100;

		if(check < 2)
			b = ((val * Barrel_Vals[secondary_barrel][0]) / 100) * -1;
			sens = (val * s_mod) / 100;
			val = val + (s + b + sens);
	}
		return val;	
}
