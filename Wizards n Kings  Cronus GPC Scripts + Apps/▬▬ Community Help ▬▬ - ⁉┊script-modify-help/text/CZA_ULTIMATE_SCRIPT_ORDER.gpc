	{
		if(ref == f_idx || f_idx == 71)
		{
			f_val += 1;

			if(f_val > limit)
				f_val = start;
			if(f_val < start)
				f_val = limit;
				combo_run(RUMBLE_ON);
		}
	display_new = TRUE;
	}
	if(event_press(PS4_DOWN))
	{
		if(ref == f_idx || f_idx == 71)
		{
			f_val -= 1;

		if(f_val > limit)
			f_val = start;
		if(f_val < start)
			f_val = limit;
			combo_run(RUMBLE_OFF);	
		}
	display_new = TRUE;
	}
		return f_val;
