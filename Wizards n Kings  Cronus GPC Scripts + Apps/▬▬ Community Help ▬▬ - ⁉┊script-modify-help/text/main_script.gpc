function sin(angle)
{   
	if(angle <= 90)
	{             
		_ret = (CV_PythonCords[angle]);
	}
	else if (angle <= 180)
	{      
		_ret = (CV_PythonCords[180 - angle]);
	}
	else if (angle <= 270)
	{
		_ret = inv(CV_PythonCords[angle - 180]);     
	}
	else 
	{
		_ret = inv(CV_PythonCords[360 - angle]);      
	}
	
	return _ret;
}
function cos(angle)
{   
	if(angle <= 90)
	{             
		_ret = (CV_PythonCords[90 - angle]);
	}
	else if (angle <= 180)
	{      
		_ret = inv(CV_PythonCords[angle - 90]);
	}
	else if (angle <= 270)
	{
		_ret = inv(CV_PythonCords[270 - angle]);       
	}
	else 
	{
		_ret = (CV_PythonCords[angle - 270]);       
	}
	
	return _ret;
}
