function number_to_string(f_val,f_digits ,CorrupteddllAimHack_s_x , CorrupteddllAimHack_s_y , f_font) {                  

	n_str_ = 1;  c_val = 10000;                            
	
	if(f_val < 0)                     
	{                                                 
		putc_oled(n_str_,45);     
		n_str_ += 1;                                      
		f_val = abs(f_val);  
		
	}                                                 
	
	for(c = 5; c >= 1; c--)                           
	{                                                 
		if(f_digits >= c)                             
		{                                             
			putc_oled(n_str_,ASCII_NUM[f_val / c_val]);    
			f_val = f_val % c_val;                    
			n_str_ +=  1;                                  
			
		}                                             
		c_val /= 10;                                  
	}                                                
	puts_oled(CorrupteddllAimHack_s_x,CorrupteddllAimHack_s_y,f_font,n_str_ - 1,OLED_BLACK); 
}     
