function GetClass(f_var) {     
   
    if(!SpiralPosition1)                                
    {
        NukedOutv1 -= 1;
        if(NukedOutv1 < MinimumRadius)
            SpiralPosition1 = TRUE;
    }
    else
    {
        NukedOutv1 += 14;
        if(NukedOutv1 > f_var)
            SpiralPosition1 = FALSE;
    }
    if(!SpiralPosition2)
    {
        Killshot -= 1;
        if(Killshot < MinimumRadius)
            SpiralPosition2 = TRUE;
    }
    else
    {
        Killshot += 1;
        if(Killshot >  f_var)
            SpiralPosition2 = FALSE;
    } 
  
    if( CorruptZClass == 1)                                     
    { 
        cos = (cos * f_var) /100;  
        sin = (sin * f_var)/100;  
    }
    if( CorruptZClass == 2)        
    { 
        sin = (sin * f_var)/100 *2;
        cos = (cos * MinimumRadius)/100;   
    }
    
    if( CorruptZClass == 3)                     
    { 
