 +-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+*/
main {

if (Default) swap (18,5)
/*
+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+
| | | |M|E|N|U| |A|D|S| | | | | | | | | | | | | | | | | | | | | | | | | | | | | | | | |
+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+*/
if(get_val(ADS))
	{//hold ADS button   
	if(event_press (2))
					{ 
						AIM__CORRECT = !AIM__CORRECT;				
						rumble (AIM__CORRECT);
				}
						set_val (2,0);
}
/*
 +-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+
 | | | | | | | | |A|I|M| | |C|O|R|R|E|C|T|I|O|N|S| | | | | | | | | | | | | | | | | | | |
 +-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+*/  
if(AIM__CORRECT)
	{ 
	  if(get_val(FIRE) && get_val(ADS)) 
	  { 
	    Use_sens = ADS_FIRE_SENS;
	  }
	  if(get_val(FIRE) && !get_val(ADS)) 
	  { 
	    Use_sens = HIP_SENS;
	  }
	  if(!get_val(FIRE) && !get_val(ADS)) 
	  {
	    Use_sens = GEN_SENS;
	  }
	  if(!get_val(FIRE) && get_val(ADS)) 
	  {
	    Use_sens = ADS_SENS;
	  } 
	    sensitivity(9, NOT_USE, Use_sens);
	    sensitivity(10, NOT_USE, Use_sens);
	}

